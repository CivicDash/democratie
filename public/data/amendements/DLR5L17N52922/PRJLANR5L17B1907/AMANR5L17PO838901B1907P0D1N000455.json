}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_12"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;L.&nbsp;131&#x2011;7 du code de la s&#x00E9;curit&#x00E9; sociale est compl&#x00E9;t&#x00E9; par un III ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;III.&nbsp;&#x2013;&nbsp;Toute mesure de r&#x00E9;duction ou d&#x2019;exon&#x00E9;ration de cotisations de s&#x00E9;curit&#x00E9; sociale, institu&#x00E9;e &#x00E0; compter du&nbsp;1<sup>er</sup>&nbsp;janvier&nbsp;2026, est compens&#x00E9;e par la suppression, dans la m&#x00EA;me proportion, d&#x2019;une mesure de r&#x00E9;duction ou d&#x2019;exon&#x00E9;ration de cotisations de s&#x00E9;curit&#x00E9; sociale existante.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; limiter la mise en place de nouveaux dispositifs d&#x2019;exon&#x00E9;rations de cotisations sociales en pr&#x00E9;voyant que chaque nouveau dispositif fait l&#x2019;objet de la suppression d&#x2019;un dispositif existant pour un montant &#x00E9;quivalent.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-29", "datePublication": "2025-11-08", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-11-08T12:24:39+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B1907P0D1N000455?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2026IDS29880", "article99": "false", "loiReference": {"codeLoi": "c s\u00e9curit\u00e9 sociale", "divisionCodeLoi": "L131-7"}, "discussionCommune": {"idDiscussion": "33971", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "84165", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
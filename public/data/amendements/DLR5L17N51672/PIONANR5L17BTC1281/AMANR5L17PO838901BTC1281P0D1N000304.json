{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1281P0D1N000304", "chronotag": "1757948208450", "legislature": "17", "identification": {"numeroLong": "304", "numeroOrdreDepot": "304", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1281P0D1", "texteLegislatifRef": "PIONANR5L17BTC1281", "triAmendement": "kkaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaalprzzu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722390", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA330909", "PA721442", "PA794994", "PA794750", "PA793094"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Corneloup,  M.&#160;Descoeur, Mme&#160;Petex, M.&#160;Portier, M.&#160;Liger et M.&#160;Ray"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1281.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;6 par la phrase suivante&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Des moyens sp&#x00E9;cifiques sont d&#x00E9;di&#x00E9;s &#x00E0; la mise en place de ce dispositif&nbsp;: organisation, temps, disponibilit&#x00E9; des professionnels, formation et acculturation, annonces complexes, contexte d&#x2019;incertitude, ma&#x00EE;trise s&#x00E9;mantique.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La volont&#x00E9; d&#x2019;organiser des discussions, programmes, recueils de valeurs et pr&#x00E9;f&#x00E9;rences restera un v&#x0153;u pieux si n&#x2019;est pas mise en place une r&#x00E9;forme en profondeur des organisations actuellement aux prises avec la hausse de la demande et la baisse de l&#x2019;offre de soin (besoin de temps et de disponibilit&#x00E9; des professionnels) et des mentalit&#x00E9;s soignantes (ce qui passe par de la formation et de l&#x2019;acculturation &#x00E0; la mani&#x00E8;re d&#x2019;aborder ces annonces complexes et le contexte d&#x2019;incertitude qui leur est propre, avec la personne malade). A titre de comparaison, le dispositif d&#x2019;annonce en canc&#x00E9;rologie date du Plan national Cancer de 2003 et n&#x2019;est soit toujours pas appliqu&#x00E9;, soit limit&#x00E9; faute de temps et d&#x2019;int&#x00E9;r&#x00EA;t ou de conscience m&#x00E9;dicale.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-06", "datePublication": "2025-05-16", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-05-16T15:13:22+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1281P0D1N000304?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29386", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
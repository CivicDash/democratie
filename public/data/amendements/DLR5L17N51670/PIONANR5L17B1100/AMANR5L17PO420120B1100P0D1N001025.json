: {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;7, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;seuls m&#x00E9;decins&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;m&#x00E9;decins, aux magistrats et aux avocats&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;objectif ici est de pr&#x00E9;voir les cas o&#x00F9; une proc&#x00E9;dure en justice serait d&#x00E9;clench&#x00E9;e. Pour que celle-ci soit men&#x00E9;e avec s&#x00E9;rieux, il semble l&#x00E9;gitime&nbsp;que l&#x2019;enregistrement des d&#x00E9;clarations des professionnels de sant&#x00E9; soit accessible aux magistrats et avocats en charge du dossier.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-05", "datePublication": "2025-05-02", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-02T11:28:09+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N001025?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instanc
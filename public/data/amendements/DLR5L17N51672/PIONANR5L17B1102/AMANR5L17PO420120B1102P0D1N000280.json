{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1102P0D1N000280", "chronotag": "1744277727392", "legislature": "17", "identification": {"numeroLong": "AS280", "numeroOrdreDepot": "280", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1102P0D1", "texteLegislatifRef": "PIONANR5L17B1102", "triAmendement": "eiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabm", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794762", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794434", "PA841645", "PA840075", "PA842073", "PA840143", "PA794678", "PA795900", "PA793672", "PA721932", "PA793696", "PA793354", "PA794322", "PA795552", "PA793656", "PA793230"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Dogor-Such,  M.&#160;Bentz, M.&#160;Bernhardt, M.&#160;Casterman, M.&#160;Dussausaye, M.&#160;Florquin, M.&#160;Frapp&#233;, Mme&#160;Hamelet, Mme&#160;Loir, Mme&#160;Lorho, M.&#160;Muller, Mme&#160;M&#233;lin, M.&#160;M&#233;nag&#233;, M.&#160;Odoul, Mme&#160;Pollet et Mme&#160;Ranc"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 9", "articleDesignationCourte": "ART. 9", "articleDesignation": "ARTICLE 9", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1102.asp#D_Article_9"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;1, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x2019;accompagnement&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;palliatifs&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la deuxi&#x00E8;me phrase du m&#x00EA;me alin&#x00E9;a, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&nbsp;&#x00AB;&nbsp;d&#x2019;accompagnement, notamment en soins&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">III.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la derni&#x00E8;re phrase du m&#x00EA;me alin&#x00E9;a, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x2019;accompagnement&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;palliatifs&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Il s&#x2019;agit ici d&#x2019;une illustration suppl&#x00E9;mentaire du d&#x00E9;classement des soins palliatifs op&#x00E9;r&#x00E9; par la PPL. &nbsp;Cette disposition de l&#x2019;article&nbsp;9 fait r&#x00E9;f&#x00E9;rence &#x00E0; un rapport annuel&nbsp; sur les soins d&#x2019;accompagnement et non sur les soins palliatifs, ceux-ci ne formant qu&#x2019;une sous cat&#x00E9;gorie des soins d&#x2019;accompagnement alors que les soins palliatifs sont reconnus pas l&#x2019;OMS, ce que ne sont pas les soins&nbsp; d&#x2019;accompagnement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-03", "datePublication": "2025-04-08", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-04-10T11:35:00+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1102P0D1N000280?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "29870", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
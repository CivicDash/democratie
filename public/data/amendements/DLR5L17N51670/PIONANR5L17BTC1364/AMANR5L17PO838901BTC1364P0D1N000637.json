contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">La propagande ou la publicit&#x00E9; en faveur de l&#x2019;aide &#x00E0; mourir, par assistance au suicide ou par euthanasie, est r&#x00E9;prim&#x00E9;e par l&#x2019;article&nbsp;223&#x2011;14 du code p&#x00E9;nal.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Au regard de la promotion du suicide assist&#x00E9; et de l&#x2019;euthanasie, dans certains pays, en particulier par des associations, il importe d&#x2019;emp&#x00EA;cher de telles d&#x00E9;rives.&nbsp;</p><p>Tel est le sens de cet amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-07", "datePublication": "2025-05-24", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-24T11:47:39+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N000637?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29513", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "31106", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "78680", "typePosition": "Dern
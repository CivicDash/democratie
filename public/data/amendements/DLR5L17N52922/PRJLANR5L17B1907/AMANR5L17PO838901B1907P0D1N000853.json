{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B1907P0D1N000853", "chronotag": "1762557567214", "legislature": "17", "identification": {"numeroLong": "853", "numeroOrdreDepot": "853", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "awaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaad", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793354", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe Rassemblement National", "libelle": "Mme&#160;M&#233;lin et les membres du groupe Rassemblement National"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_6"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; supprimer l&#x2019;article 6 qui pr&#x00E9;voit le gel des seuils de revenus conditionnant l&#x2019;application des taux r&#x00E9;duits ou nuls de CSG.</p><p style=\"text-align: justify;\">Sous couvert de rigueur budg&#x00E9;taire, cet article constitue en r&#x00E9;alit&#x00E9; une hausse d&#x00E9;guis&#x00E9;e d&#x2019;imp&#x00F4;t pour les m&#x00E9;nages modestes. En bloquant la revalorisation des bar&#x00E8;mes, de nombreux retrait&#x00E9;s, invalides et demandeurs d&#x2019;emploi verront leurs pr&#x00E9;l&#x00E8;vements sociaux augmenter. Cet article vient s&#x2019;ajouter au gel des prestations sociales et des pensions et accentuera la perte de pouvoir d&#x2019;achat des plus fragiles.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-30", "datePublication": "2025-11-05", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-11-05T18:44:06+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B1907P0D1N000853?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2026IDS29864", "article99": "false", "loiReference": {"@xmlns:xsi
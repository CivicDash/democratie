{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1100P0D1N000799", "chronotag": "1757947888309", "legislature": "17", "identification": {"numeroLong": "AS799", "numeroOrdreDepot": "799", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1100P0D1", "texteLegislatifRef": "PIONANR5L17B1100", "triAmendement": "beaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadzoc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA721932", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794434", "PA840075", "PA840915", "PA842255", "PA794762", "PA793656"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Lorho,  M.&#160;Bentz, M.&#160;Casterman, M.&#160;Monnier, Mme&#160;Sicard, Mme&#160;Dogor-Such et Mme&#160;Pollet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 4", "articleDesignationCourte": "ART. 4", "articleDesignation": "ARTICLE 4", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1100.asp#D_Article_4"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "8", "alineaDesignation": "Alin\u00e9a 8"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;personne&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">r&#x00E9;diger ainsi la fin de l&#x2019;alin&#x00E9;a&nbsp;8&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;qui a re&#x00E7;u tous les soins dont elle pouvait b&#x00E9;n&#x00E9;ficier&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">&#x00AB;&nbsp;Mon premier souci&nbsp;&#x00BB;, note le Serment d&#x2019;Hippocrate, &#x00AB;&nbsp;sera de r&#x00E9;tablir, de pr&#x00E9;server ou de promouvoir la sant&#x00E9; dans tous ses &#x00E9;l&#x00E9;ments, physiques et mentaux, individuels et sociaux.&nbsp;&#x00BB; Cet appel fait aux m&#x00E9;decins traduit l&#x2019;injonction qui leur est faite de pr&#x00E9;server par tous moyens (bien que sans &#x00AB;&nbsp;prolonger abusivement les agonies&nbsp;&#x00BB;) la sant&#x00E9; de leur patient.&nbsp;</p><p style=\"text-align: justify;\">Cette n&#x00E9;cessit&#x00E9; de mettre en &#x0153;uvre tout ce qui est en le pouvoir des m&#x00E9;decins pour dispenser des soins &#x00E0; leurs patients est absente des conditions d&#x2019;acc&#x00E8;s au suicide assist&#x00E9; et &#x00E0; l&#x2019;euthanasie. Il est n&#x00E9;cessaire de conditionner l&#x2019;acc&#x00E8;s &#x00E0; de telles proc&#x00E9;dures &#x00E0; l&#x2019;assurance de r&#x00E9;ception de tous les soins n&#x00E9;cessaires.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-05", "datePublication": "2025-04-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-28T18:29:56+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N000799?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "30050", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B1906P2D1N000232", "chronotag": "1761780403242", "legislature": "17", "identification": {"numeroLong": "II-AC232", "numeroOrdreDepot": "232", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B1906P2D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "ahsaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaac", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793102", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA796118", "PA794562", "PA793158", "PA720822", "PA842255", "PA841199", "PA795912", "PA794670", "PA795552", "PA840869", "PA841749", "PA794198", "PA795644", "PA840159"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Christian&#160;Girard,  Mme&#160;Auzanot, M.&#160;Ballard, M.&#160;Beaurain, M.&#160;Bilde, Mme&#160;Sicard, M.&#160;Perez, Mme&#160;Lavalette, Mme&#160;Parmentier, M.&#160;Odoul, Mme&#160;Joubert, Mme&#160;Joncour, M.&#160;Chudeau, Mme&#160;Da&#160;Conceicao&#160;Carvalho et M.&#160;Clavet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ETAT G - LISTE DES OBJECTIFS ET DES INDICATEURS DE PERFORMANCE", "articleDesignationCourte": "ART. 52", "articleDesignation": "ARTICLE 52", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE 52", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906C.asp#D_Etat_G__Liste_des_objectifs_et_des_ind"}, "missionVisee": {"codeEtat": "D", "codeMissionMinefi": "ZD", "libelleMission": "Avances \u00e0 l'audiovisuel public", "missionRef": "3393"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "157", "alineaDesignation": "Alin\u00e9a 157"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;157 par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;,&nbsp;impartiale, neutre et r&#x00E9;ellement au service du contribuable fran&#x00E7;ais&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le service public audiovisuel doit &#x00EA;tre le bien commun de tous les Fran&#x00E7;ais, non l&#x2019;expression d&#x2019;un courant politique ou culturel particulier. Il est donc imp&#x00E9;ratif de garantir la neutralit&#x00E9; et l&#x2019;impartialit&#x00E9; de France T&#x00E9;l&#x00E9;visions, tant dans ses r&#x00E9;dactions que dans ses choix &#x00E9;ditoriaux, souvent l&#x2019;objet de partis pris id&#x00E9;ologiques.</p><p style=\"text-align: justify;\">L&#x2019;exigence de pluralisme, principe constitutionnel, impose que toutes les sensibilit&#x00E9;s politiques, sociales et territoriales puissent y &#x00EA;tre repr&#x00E9;sent&#x00E9;es de mani&#x00E8;re &#x00E9;quilibr&#x00E9;e.</p><p style=\"text-align: justify;\">Dans un contexte de d&#x00E9;fiance croissante envers les m&#x00E9;dias, la cr&#x00E9;dibilit&#x00E9; du service public d&#x00E9;pend de sa capacit&#x00E9; &#x00E0; s&#x2019;affranchir de tout biais id&#x00E9;ologique pour redevenir un vecteur d&#x2019;unit&#x00E9; nationale et de confiance d&#x00E9;mocratique.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-25", "datePublication": "2025-10-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-30T00:26:36+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B1906P2D1N000232?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
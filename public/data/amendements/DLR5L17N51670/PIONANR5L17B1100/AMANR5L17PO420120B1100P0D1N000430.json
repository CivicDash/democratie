cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;10 par la phrase suivante&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le refus de la personne de b&#x00E9;n&#x00E9;ficier des soins palliatifs et d&#x2019;accompagnement ne peut constituer pour le m&#x00E9;decin un motif de rejet de la demande&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement des d&#x00E9;put&#x00E9;s socialistes et apparent&#x00E9;s vise &#x00E0; pr&#x00E9;ciser que le refus d'acc&#x00E9;der &#x00E0; des soins palliatifs et d'accompagnement ne peut avoir pour effet un refus par le m&#x00E9;decin de l'acc&#x00E8;s &#x00E0; l'aide &#x00E0; mourir.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-03", "datePublication": "2025-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-04-29T17:24:33+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N000430?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi":
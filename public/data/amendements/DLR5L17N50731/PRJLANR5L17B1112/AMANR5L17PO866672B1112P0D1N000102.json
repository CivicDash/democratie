{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO866672B1112P0D1N000102", "chronotag": "1757947750819", "legislature": "17", "identification": {"numeroLong": "CS102", "numeroOrdreDepot": "102", "prefixeOrganeExamen": "CSCYBER", "numeroRect": "0"}, "examenRef": "EXANR5L17PO866672B1112P0D1", "texteLegislatifRef": "PRJLANR5L17B1112", "triAmendement": "caiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaji", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794946", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793904", "PA796118", "PA841131", "PA793134", "PA840075", "PA642988", "PA840119", "PA841139", "PA795966", "PA793254", "PA841955", "PA794570", "PA794954", "PA796050"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Villedieu,  M.&#160;Lopez-Liguori, Mme&#160;Auzanot, Mme&#160;Bouquin, M.&#160;Buisson, M.&#160;Casterman, M.&#160;de&#160;L&#233;pinau, Mme&#160;Delannoy, M.&#160;Gabarron, M.&#160;Rambaud, M.&#160;Rancoule, M.&#160;Renault, M.&#160;Sabatou, M.&#160;Salmon et M.&#160;Schreck"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 19", "articleDesignationCourte": "ART. 19", "articleDesignation": "ARTICLE 19", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1112.asp#D_Article_19"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;la troisi&#x00E8;me phrase de l&#x2019;alin&#x00E9;a&nbsp;2, supprimer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;lors de leur collecte&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;Association fran&#x00E7;aise pour le nommage Internet en coop&#x00E9;ration (AFNIC) alerte sur les risques de surtransposition du droit europ&#x00E9;en en mati&#x00E8;re de r&#x00E9;gulation des noms de domaine. Elle critique notamment l&#x2019;obligation de v&#x00E9;rification syst&#x00E9;matique a priori de l&#x2019;identit&#x00E9; des titulaires, qu&#x2019;elle juge disproportionn&#x00E9;e par rapport aux objectifs de lutte contre la fraude. Une telle mesure devrait &#x00EA;tre r&#x00E9;alis&#x00E9;e a posteriori aux cas signal&#x00E9;s ou suspects, d&#x2019;autant que seuls 0,02&nbsp;% des quelque 4 millions de noms de domaine sont concern&#x00E9;s par un usage frauduleux. Imposer un contr&#x00F4;le syst&#x00E9;matique dans un syst&#x00E8;me comptant 800&nbsp;000 cr&#x00E9;ations annuelles et 700&nbsp;000 titulaires est irr&#x00E9;aliste et contre-productif.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-09-05", "datePublication": "2025-09-09", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-09-09T23:18:13+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO866672B1112P0D1N000102?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "81237", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
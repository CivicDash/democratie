eInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;3, supprimer le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, proc&#x00E9;der &#x00E0; la m&#x00EA;me suppression &#x00E0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;5, &#x00E0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;10, &#x00E0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;13, &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;15, &#x00E0; la seconde&nbsp;occurrence &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;17 et &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;19.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Amendement de repli.&nbsp;</p><p style=\"text-align: justify;\">Amendement r&#x00E9;dactionnel visant &#x00E0; conserver la centralit&#x00E9; de la notion de &#x00AB;&nbsp;soins palliatifs&nbsp;&#x00BB;, seul&nbsp;r&#x00E9;f&#x00E9;rentiel reconnu par l&#x2019;OMS, en rempla&#x00E7;ant les occurrences de l&#x2019;expression &#x00AB;&nbsp;soins&nbsp;d&#x2019;accompagnement&nbsp;&#x00BB; par l&#x2019;expression &#x00AB;&nbsp;soins palliatifs d&#x2019;accompagnement&nbsp;&#x00BB;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-01", "datePublication": "2025-04-09", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-04-09T15:23:26+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentati
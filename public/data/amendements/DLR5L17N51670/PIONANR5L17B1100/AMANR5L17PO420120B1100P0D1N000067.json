{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1100P0D1N000067", "chronotag": "1757947997894", "legislature": "17", "identification": {"numeroLong": "AS67", "numeroOrdreDepot": "67", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1100P0D1", "texteLegislatifRef": "PIONANR5L17B1100", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaag", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA608416", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA346782", "PA722390", "PA720386", "PA330008", "PA840979", "PA793508", "PA606098", "PA774954", "PA266797", "PA794058"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Hetzel,  M.&#160;Juvin, Mme&#160;Corneloup, M.&#160;Di&#160;Filippo, M.&#160;Breton, M.&#160;Le&#160;Fur, M.&#160;Brigand, M.&#160;Marleix, Mme&#160;Blin, M.&#160;Gosselin et Mme&#160;Gruet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1100.asp#D_Article_6"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;6 d&#x00E9;finit la proc&#x00E9;dure d&#x2019;examen de la demande d&#x2019;euthanasie ou de suicide assist&#x00E9; jusqu&#x2019;&#x00E0; la prescription de la substance l&#x00E9;tale.</p><p style=\"text-align: justify;\">Alors que la mise en &#x0153;uvre de la s&#x00E9;dation profonde et continue jusqu&#x2019;au d&#x00E9;c&#x00E8;s et les arr&#x00EA;ts de traitement font l&#x2019;objet de proc&#x00E9;dures coll&#x00E9;giales, elle n&#x2019;est pas pr&#x00E9;vue pour l&#x2019;application de cet article.</p><p style=\"text-align: justify;\">Cela concentre le pouvoir de d&#x00E9;cision finale sur un m&#x00E9;decin m&#x00EA;me s&#x2019;il peut recueillir l&#x2019;avis d&#x2019;autres professionnels. C&#x2019;est un retour en arri&#x00E8;re.</p><p style=\"text-align: justify;\">Aussi, il convient de supprimer cet article.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-01", "datePublication": "2025-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-04-29T21:04:25+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N000067?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "77050", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
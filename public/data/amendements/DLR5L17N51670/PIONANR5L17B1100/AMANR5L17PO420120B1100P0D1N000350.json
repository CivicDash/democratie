{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1100P0D1N000350", "chronotag": "1757947945228", "legislature": "17", "identification": {"numeroLong": "AS350", "numeroOrdreDepot": "350", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1100P0D1", "texteLegislatifRef": "PIONANR5L17B1100", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadaoi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795900", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794762", "PA794678", "PA795552", "PA793656", "PA721932", "PA841645", "PA840075", "PA793672"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Hamelet,  Mme&#160;Dogor-Such, M.&#160;Frapp&#233;, M.&#160;Odoul, Mme&#160;Pollet, Mme&#160;Lorho, M.&#160;Bernhardt, M.&#160;Casterman et Mme&#160;Loir"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1100.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "12", "alineaDesignation": "Alin\u00e9a 12"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;12, substituer au mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;deux&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;cinq&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Comme il a tr&#x00E8;s justement &#x00E9;t&#x00E9; remarqu&#x00E9; en audition, le d&#x00E9;lai minimal de deux jours n&#x2019;est pas de nature &#x00E0; prendre en compte une donn&#x00E9;e essentielle&nbsp;: la fluctuation des envies du malade. Certes, le malade peut revenir sur sa d&#x00E9;cision &#x00E0; tout moment de la proc&#x00E9;dure, mais la confirmation de son choix &#x00E0; l&#x2019;issue de la proc&#x00E9;dure de consultation du m&#x00E9;decin enclenche la deuxi&#x00E8;me phase de la proc&#x00E9;dure. Une personne aura plus de mal &#x00E0; admettre vouloir faire un retour en arri&#x00E8;re apr&#x00E8;s une prise de d&#x00E9;cision h&#x00E2;tive si tout est d&#x00E9;j&#x00E0; engag&#x00E9;.</p><p style=\"text-align: justify;\">Il est propos&#x00E9; de rallonger ce d&#x00E9;lai &#x00E0; cinq jours pour permettre un meilleur temps de r&#x00E9;flexion tout en garantissant un d&#x00E9;lai compatible avec le cas d&#x2019;une personne malade dont le pronostic vital est engag&#x00E9; &#x00E0; court terme.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-03", "datePublication": "2025-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-30T11:26:20+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N000350?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": "dc"}, "discussionCommune": {"idDiscussion": "30172", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
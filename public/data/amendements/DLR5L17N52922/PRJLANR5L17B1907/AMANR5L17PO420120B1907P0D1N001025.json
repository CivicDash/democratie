{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1907P0D1N001025", "chronotag": "1761906073545", "legislature": "17", "identification": {"numeroLong": "AS1025", "numeroOrdreDepot": "1025", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "ieaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaak", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA774962", "groupePolitiqueRef": "PO845514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA793174"}, "suffixe": "et les membres du groupe Gauche D\u00e9mocrate et R\u00e9publicaine", "libelle": "Mme&#160;Lebon,  M.&#160;Monnet et les membres du groupe Gauche D&#233;mocrate et R&#233;publicaine"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 31", "articleDesignationCourte": "ART. 31", "articleDesignation": "ARTICLE 31", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_31"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "10", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 10"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;10, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Si le manquement constat&#x00E9; est d&#x00FB; &#x00E0; une indisponibilit&#x00E9; et &#x00E0; une non-conformit&#x00E9; des outils n&#x00E9;cessaires au respect des dispositions de l&#x2019;article&nbsp;L.&nbsp;1111&#x2011;15, la p&#x00E9;nalit&#x00E9; n&#x2019;est pas appliqu&#x00E9;e &#x00E0; l&#x2019;&#x00E9;tablissement, au service, &#x00E0; l&#x2019;organisme mais &#x00E0; l&#x2019;&#x00E9;diteur informatique responsable du manquement selon les dispositions pr&#x00E9;vues &#x00E0; l&#x2019;article&nbsp;L.&nbsp;1470&#x2011;6.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet&nbsp;amendement, issu de propositions formul&#x00E9;es par la FHF, vise &#x00E0; pr&#x00E9;voir qu&#x2019;en cas de manquement constat&#x00E9; li&#x00E9; non &#x00E0; l&#x2019;&#x00E9;tablissement ou au professionnel de sant&#x00E9;, mais &#x00E0; l&#x2019;indisponibilit&#x00E9; ou &#x00E0; la non-conformit&#x00E9; des outils n&#x00E9;cessaires, la p&#x00E9;nalit&#x00E9; soit appliqu&#x00E9;e &#x00E0; l&#x2019;&#x00E9;diteur responsable, et non &#x00E0; la structure.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-20", "datePublication": "2025-10-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-10-31T11:21:01+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1907P0D1N001025?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "83625", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
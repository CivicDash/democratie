Informatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;5 pr&#x00E9;cise les conditions de pr&#x00E9;sentation d&#x2019;une demande d&#x2019;euthanasie ou de suicide assist&#x00E9;.</p><p style=\"text-align: justify;\">En proposant dans cet article de b&#x00E9;n&#x00E9;ficier des soins palliatifs, le Gouvernement met sur le m&#x00EA;me plan les soins palliatifs et les demandes d&#x2019;euthanasie ou de suicide assist&#x00E9;.</p><p style=\"text-align: justify;\">Alors qu&#x2019;il s&#x2019;agit d&#x2019;une rupture anthropologique entre les soins palliatifs et l&#x2019;acc&#x00E8;s &#x00E0; l&#x2019;euthanasie ou au suicide assist&#x00E9;, il convient de supprimer cet article.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-05", "datePublication": "2025-05-20", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-20T18:43:20+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N000032?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29391", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "78278", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B1906P2D1N002226", "chronotag": "1762557553098", "legislature": "17", "identification": {"numeroLong": "II-CF2226", "numeroOrdreDepot": "2226", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B1906P2D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "iabkaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841381", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA795422"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Pantel, rapporteure et M.&#160;Maudet, rapporteur"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ARTICLE 77", "articleDesignationCourte": "APR\u00c8S ART. 77", "articleDesignation": "APR\u00c8S L'ARTICLE 77, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906C.asp#D_ARTICLE_77"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "SB", "libelleMission": "S\u00e9curit\u00e9s", "missionRef": "3365"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s le 23&#x00B0; du&nbsp;I de l&#x2019;article&nbsp;128 de&nbsp;loi n&#x00B0; 2005&#x2011;1720 du&nbsp;30&nbsp;d&#x00E9;cembre&nbsp;2005 de finances rectificative pour 2005, il est ins&#x00E9;r&#x00E9; un 24&#x00B0; ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;24&#x00B0; S&#x00E9;curit&#x00E9; civile.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Depuis 2022, le Parlement ne dispose plus d&#x2019;une vision exhaustive de l&#x2019;ensemble des cr&#x00E9;dits du budget g&#x00E9;n&#x00E9;ral de l&#x2019;&#x00C9;tat consacr&#x00E9;s &#x00E0; la politique de s&#x00E9;curit&#x00E9; civile, du fait de la non reconduction du document de politique transversale (DPT) &#x00AB;&nbsp;s&#x00E9;curit&#x00E9; civile&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">&#x00C0;&nbsp;titre indicatif, plus de la moiti&#x00E9; de cr&#x00E9;dits du budget g&#x00E9;n&#x00E9;ral consacr&#x00E9;s &#x00E0; la s&#x00E9;curit&#x00E9; civile figuraient dans d&#x2019;autres programmes que le programme 161 dans le cadre du projet de loi de finances pour 2021 (&#x00E9;cologie, agriculture, administration g&#x00E9;n&#x00E9;rale et territoriale de l&#x2019;&#x00C9;tat, etc.)</p><p style=\"text-align: justify;\">Le rapporteurs sp&#x00E9;ciaux proposent donc de r&#x00E9;tablir ce DPT, afin de garantir la bonne information du Parlement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-11-03", "datePublication": "2025-11-05", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-11-05T19:12:41+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P2D1N002226?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "xx loi 2005-1720", "divisionCodeLoi": "128"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
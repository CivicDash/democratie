cation", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Pr\u00e9vention des risques", "programmeRef": "557497", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "\u00c9nergie, climat et apr\u00e8s-mines", "programmeRef": "557499", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Service public de l'\u00e9nergie", "programmeRef": "557501", "autorisationEngagement": "-800000000", "creditPaiement": "-800000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage des politiques de l'\u00e9cologie, du d\u00e9veloppement et de la mobilit\u00e9 durables", "programmeRef": "557503", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Fonds d'acc\u00e9l\u00e9ration de la transition \u00e9cologique dans les territoires", "programmeRef": "557505", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00fbret\u00e9 nucl\u00e9aire et radioprotection", "programmeRef": "557507", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Ecologie \u2013 mise en extinction du plan de relance", "programmeRef": "557509", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Face au retard accumul&#x00E9; en mati&#x00E8;re d&#x2019;investissements pour la r&#x00E9;g&#x00E9;n&#x00E9;ration du r&#x00E9;seau ferroviaire, cet amendement propose un investissement de 800 millions d&#x2019;euros suppl&#x00E9;mentaire pour l&#x2019;ann&#x00E9;e 2026.&nbsp;</p><p style=\"text-align: justify;\">A ce titre, l&#x2019;amendement proc&#x00E8;de d&#x2019;une part &#x00E0; l&#x2019;augmentation de 800 millions d&#x2019;euros en AE et CP de&nbsp;l&#x2019;action <i>41 - Ferroviaire</i>&nbsp;du programme 203 &#x00AB; Infrastructures et services de transports &#x00BB;. Pour garantir la recevabilit&#x00E9; de cette proposition, il&nbsp;proc&#x00E8;de d&#x2019;autre part &#x00E0; la r&#x00E9;duction &#x00E0; due concurrence en AE et CP des cr&#x00E9;dits de&nbsp;l&#x2019;action <i>09 &#x2013; Soutien aux &#x00E9;nergies renouvelables &#x00E9;lectriques en m&#x00E9;tropole continentale</i> du programme 345 &#x00AB; Service public de l'&#x00E9;nergie &#x00BB;.</p><p style=\"text-align: justify;\">Cette baisse vise uniquement &#x00E0; respecter les r&#x00E8;gles de recevabilit&#x00E9;. Les auteurs de cet amendement n&#x2019;ont aucune intention de baisser les cr&#x00E9;dits de cette action et&nbsp;invitent&nbsp;le Gouvernement &#x00E0; lever le gage.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-29", "datePublication": "2025-11-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-11-12T18:35:51+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P2D1N000533?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": "41"}, "discussionCommune": {"idDiscussion": "34709", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
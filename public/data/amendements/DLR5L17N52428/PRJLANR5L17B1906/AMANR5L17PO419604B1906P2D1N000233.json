atif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Patrimoines", "programmeRef": "581577", "autorisationEngagement": "-12694336", "creditPaiement": "-12694336", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Cr\u00e9ation", "programmeRef": "581579", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Transmission des savoirs et d\u00e9mocratisation de la culture", "programmeRef": "581581", "autorisationEngagement": "12694336", "creditPaiement": "12694336", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Soutien aux politiques du minist\u00e8re de la culture", "programmeRef": "581583", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Socialistes et apparent&#x00E9;s vise &#x00E0; r&#x00E9;tablir les cr&#x00E9;dits de la participation &#x00E0; la vie culturelle en baisse de pr&#x00E8;s de 13 millions d&#x2019;euros pour 2026.</p><p style=\"text-align: justify;\">Politiques territoriales, Politique en faveur des publics en situation sp&#x00E9;cifique, Soutien aux pratiques amateurs, Actions de m&#x00E9;diation num&#x00E9;rique, Education populaire sont autant de missions de cette sous action essentielle qu&#x2019;est la participation de tous &#x00E0; la vie culturelle.</p><p style=\"text-align: justify;\">&#x00C0;&nbsp;rebours de tous les discours officiels, ce budget tourne le dos &#x00E0; la jeunesse, aux droits culturels et aux territoires populaires. Il est donc propos&#x00E9; de r&#x00E9;tablir les cr&#x00E9;dits &#x00E0; hauteur de 2025.</p><p style=\"text-align: justify;\">Afin d&#x2019;&#x00EA;tre recevable, cet amendement propose de&nbsp;</p><p style=\"text-align: justify;\">&#x2013;&nbsp;abonder de&nbsp;12&nbsp;694&nbsp;336&nbsp;euros, en AE et CP, l&#x2019;action 02 du programme n&#x00B0; 361&nbsp;&#x2013;&nbsp;Transmission des savoirs et d&#x00E9;mocratisation de la culture</p><p style=\"text-align: justify;\">&#x2013;&nbsp;retirer le m&#x00EA;me montant, en AE et CP, &#x00E0; l&#x2019;action n&#x00B0; 03 &#x00AB;&nbsp;Patrimoine des mus&#x00E9;es de France&nbsp;&#x00BB;&nbsp;du programme 175 &#x00AB;&nbsp;Patrimoine&nbsp;&#x00BB;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-25", "datePublication": "2025-10-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-29T21:47:35+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B1906P2D1N000233?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instan
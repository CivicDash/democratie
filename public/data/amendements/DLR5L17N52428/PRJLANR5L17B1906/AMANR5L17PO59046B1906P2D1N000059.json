{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59046B1906P2D1N000059", "chronotag": "1761764503280", "legislature": "17", "identification": {"numeroLong": "II-DN59", "numeroOrdreDepot": "59", "prefixeOrganeExamen": "CION_DEF", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59046B1906P2D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "abwaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabw", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA774960", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794270", "PA333285", "PA341981", "PA826635", "PA841243", "PA719930", "PA841681"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Santiago, rapporteure,  Mme&#160;Pic, M.&#160;Garot, Mme&#160;R&#233;calde, M.&#160;Aur&#233;lien&#160;Rousseau, M.&#160;Saint-Pasteur, M.&#160;Vallaud et M.&#160;Sother"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ARTICLE 68", "articleDesignationCourte": "APR\u00c8S ART. 68", "articleDesignation": "APR\u00c8S L'ARTICLE 68, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906C.asp#D_ARTICLE_68"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "DA", "libelleMission": "D\u00e9fense", "missionRef": "3327"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Chaque ann&#x00E9;e, le Gouvernement remet au Parlement un rapport sur l&#x2019;ex&#x00E9;cution de la loi de programmation militaire 2024&#x2011;2030, int&#x00E9;grant les enseignements du retour d&#x2019;exp&#x00E9;rience du conflit en Ukraine, notamment en mati&#x00E8;re de dronisation, de guerre &#x00E9;lectronique et de logistique.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Socialistes et apparent&#x00E9;s vise &#x00E0; renforcer le contr&#x00F4;le parlementaire et la r&#x00E9;activit&#x00E9; de la politique de d&#x00E9;fense face aux &#x00E9;volutions strat&#x00E9;giques. Le retour d&#x2019;exp&#x00E9;rience ukrainien impose une adaptation rapide de la programmation militaire. Ce rapport annuel permettra d&#x2019;assurer un suivi pr&#x00E9;cis et d&#x2019;anticiper les ajustements n&#x00E9;cessaires.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-24", "datePublication": "2025-10-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-29T20:01:23+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59046B1906P2D1N000059?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "zzrapport", "divisionCodeLoi": "LPM"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
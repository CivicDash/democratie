ionnel": "false", "urlDivisionTexteVise": "/17/textes/1281.asp#D_Article_16"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer la&nbsp;seconde phrase de l&#x2019;alin&#x00E9;a&nbsp;4.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Le fonctionnement de la proc&#x00E9;dure coll&#x00E9;giale est d&#x00E9;j&#x00E0; pr&#x00E9;vu par les alin&#x00E9;as pr&#x00E9;c&#x00E9;dents, il n'est donc pas n&#x00E9;cessaire de le soumettre &#x00E0; la voie r&#x00E9;glementaire.&nbsp;</p><p>Par ailleurs, la possibilit&#x00E9;&nbsp;de modification du fonctionnement de la proc&#x00E9;dure par voie r&#x00E9;glementaire permet un contournement du Parlement et un d&#x00E9;tournement de l'esprit de cette loi.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-07", "datePublication": "2025-05-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-16T18:18:29+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO83
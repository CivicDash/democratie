{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1229P0D1N000016", "chronotag": "1757948159299", "legislature": "17", "identification": {"numeroLong": "AS16", "numeroOrdreDepot": "16", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1229P0D1", "texteLegislatifRef": "PIONANR5L17B1229", "triAmendement": "jaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794778", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793708", "PA795010", "PA796018", "PA795076", "PA793452"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Regol,  M.&#160;Peytavie, Mme&#160;Garin, Mme&#160;Simonnet, Mme&#160;Sandrine&#160;Rousseau et M.&#160;Davi"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "APR\u00c8S ART. 3", "articleDesignation": "APR\u00c8S L'ARTICLE 3, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1229.asp#D_Article_3"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s le troisi&#x00E8;me alin&#x00E9;a de l&#x2019;article&nbsp;L.&nbsp;227&#x2011;5 du code de l&#x2019;action sociale et des familles, il est ins&#x00E9;r&#x00E9; un alin&#x00E9;a ainsi r&#x00E9;dig&#x00E9;&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Les personnes organisant l&#x2019;accueil des mineurs mentionn&#x00E9; &#x00E0; l&#x2019;article&nbsp;L.&nbsp;227&#x2011;4 sont tenues de suivre une formation en mati&#x00E8;re de secourisme.&nbsp;&#x00BB;&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement des d&#x00E9;put&#x00E9;-es &#x00E9;cologistes vise &#x00E0; instaurer une obligation de formation en mati&#x00E8;re de secourisme pour les personnes organisant l&#x2019;accueil des mineurs dans diff&#x00E9;rentes structures, &#x00E0; savoir celles d&#x00E9;taill&#x00E9;es par l&#x2019;article&nbsp;R.&nbsp;227&#x2011;1 du code de l&#x2019;action sociale et des familles (s&#x00E9;jours de vacances, s&#x00E9;jours de coh&#x00E9;sion, accueils de loisir etc.)</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-30", "datePublication": "2025-05-07", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-05-07T16:39:13+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmln
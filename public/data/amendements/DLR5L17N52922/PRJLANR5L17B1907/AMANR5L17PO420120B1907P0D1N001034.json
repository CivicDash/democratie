{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1907P0D1N001034", "chronotag": "1761908233308", "legislature": "17", "identification": {"numeroLong": "AS1034", "numeroOrdreDepot": "1034", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "ilaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaak", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793174", "groupePolitiqueRef": "PO845514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA774962"}, "suffixe": "et les membres du groupe Gauche D\u00e9mocrate et R\u00e9publicaine", "libelle": "M.&#160;Monnet,  Mme&#160;Lebon et les membres du groupe Gauche D&#233;mocrate et R&#233;publicaine"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 32", "articleDesignationCourte": "APR\u00c8S ART. 32", "articleDesignation": "APR\u00c8S L'ARTICLE 32, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_32"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s la premi&#x00E8;re phrase de l&#x2019;article&nbsp;L.&nbsp;5121&#x2011;29 du code de la sant&#x00E9; publique, est ins&#x00E9;r&#x00E9;e une phrase ainsi r&#x00E9;dig&#x00E9;e&nbsp;: &#x00AB;&nbsp;Pour les m&#x00E9;dicaments d&#x2019;int&#x00E9;r&#x00EA;ts th&#x00E9;rapeutique majeur, cette limite ne peut &#x00EA;tre inf&#x00E9;rieure &#x00E0; quatre mois de couverture des besoins en m&#x00E9;dicament, calcul&#x00E9;s sur la base du volume des ventes de la sp&#x00E9;cialit&#x00E9; concern&#x00E9;e au cours des douze derniers mois glissants&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement, issu de propositions formul&#x00E9;es par France Assos Sant&#x00E9;, vise &#x00E0; tenir compte de l&#x2019;importance des m&#x00E9;dicaments d&#x2019;int&#x00E9;r&#x00EA;t th&#x00E9;rapeutique majeur reconnue par la loi en pr&#x00E9;voyant que pour ces derniers la limite des stocks ne puisse &#x00EA;tre inf&#x00E9;rieure &#x00E0; quatre mois de couverture des besoins.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-20", "datePublication": "2025-10-29", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-10-31T11:56:57+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "co
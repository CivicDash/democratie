{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B1415P0D1N000006", "chronotag": "1757948787601", "legislature": "17", "identification": {"numeroLong": "CL6", "numeroOrdreDepot": "6", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B1415P0D1", "texteLegislatifRef": "PIONANR5L17B1415", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaav", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841351", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA610968", "PA841701", "PA794022", "PA794778"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Duplessy,  M.&#160;Amirshahi, Mme&#160;Balage&#160;El&#160;Mariky, M.&#160;Iordanoff et Mme&#160;Regol"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1415.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "2", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter cet article par les deux alin&#x00E9;as suivants&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;Le m&#x00EA;me article est compl&#x00E9;t&#x00E9; par un alin&#x00E9;a ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00AB;&nbsp;Les peines mentionn&#x00E9;es au pr&#x00E9;sent article ne peuvent faire l&#x2019;objet d&#x2019;une ex&#x00E9;cution provisoire.&nbsp;&#x00BB;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe &#x00C9;cologiste et Social vise &#x00E0; exclure l&#x2019;ex&#x00E9;cution provisoire pour toutes les peines compl&#x00E9;mentaires. Alors que l&#x2019;article&nbsp;6 de la D&#x00E9;claration des droits de l&#x2019;Homme et du Citoyen dispose que la loi &#x00AB;&nbsp;doit &#x00EA;tre la m&#x00EA;me pour tous, soit qu&#x2019;elle prot&#x00E8;ge, soit qu&#x2019;elle punisse&nbsp;&#x00BB;, la proposition de loi vise &#x00E0; n&#x2019;exclure l&#x2019;ex&#x00E9;cution provisoire que pour la peine d&#x2019;in&#x00E9;ligibilit&#x00E9;, peine prononc&#x00E9;e exclusivement contre les &#x00E9;lus. &#x00C0; l&#x2019;inverse, la peine d&#x2019;interdiction d&#x2019;exercer, par exemple, serait toujours applicable, alors qu&#x2019;elle prive non d&#x2019;un mandat, mais d&#x2019;un emploi. Cet amendement entend ainsi permettre l&#x2019;application d&#x2019;une loi identique pour tous.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-11", "datePublication": "2025-06-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "RT", "libelle": "Retir\u00e9"}, "sousEtat": {"code": "RT", "libelle": "Retir\u00e9 apr\u00e8s publication"}}, "dateSort": "2025-06-16T18:05:14+02:00", "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B1415P0D1N000006?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
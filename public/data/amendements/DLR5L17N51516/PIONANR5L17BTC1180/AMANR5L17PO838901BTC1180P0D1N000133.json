{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1180P0D1N000133", "chronotag": "1743631197520", "legislature": "17", "identification": {"numeroLong": "133 (Rect)", "numeroOrdreDepot": "133", "prefixeOrganeExamen": "AN", "numeroRect": "1"}, "examenRef": "EXANR5L17PO838901BTC1180P0D1", "texteLegislatifRef": "PIONANR5L17BTC1180", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabbi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": "AMANR5L17PO838901BTC1180P0D1N000056", "signataires": {"auteur": {"typeAuteur": "Gouvernement", "gouvernementRef": "PO855052", "acteurRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "groupePolitiqueRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "le Gouvernement"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "APR\u00c8S ART. PREMIER", "articleDesignation": "APR\u00c8S L'ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1180.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;5, supprimer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;par sp&#x00E9;cialit&#x00E9;&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;6, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;pour chaque sp&#x00E9;cialit&#x00E9; m&#x00E9;dicale&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00E0; partir de donn&#x00E9;es publi&#x00E9;es par le minist&#x00E8;re charg&#x00E9; de la sant&#x00E9; au&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">III.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la fin du m&#x00EA;me alin&#x00E9;a&nbsp;6, supprimer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;,&nbsp;par l&#x2019;agence r&#x00E9;gionale de sante&#x0301;, en lien avec les communaut&#x00E9;s professionnelles territoriales de sant&#x00E9;, de mani&#x00E8;re a&#x0300; couvrir l&#x2019;int&#x00E9;gralit&#x00E9;&#x0301; de son ressort territorial&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">IV.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la fin de l&#x2019;alin&#x00E9;a&nbsp;7, supprimer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, et a&#x0300; la d&#x00E9;cision d&#x2019;ouverture, de transfert ou de regroupement des cabinets de m&#x00E9;decins lib&#x00E9;raux&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">V.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, supprimer l&#x2019;alin&#x00E9;a&nbsp;8.&nbsp;</p><p style=\"text-align: justify;\">VI.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, supprimer les alin&#x00E9;as 10 et 11.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement pr&#x00E9;cise les modalit&#x00E9;s d&#x2019;&#x00E9;laboration et le champ d&#x2019;application du nouvel indicateur territorial de l&#x2019;offre de soins (ITOS), afin de l&#x2019;inscrire en coh&#x00E9;rence avec les dispositifs concourant &#x00E0; une meilleure r&#x00E9;partition territoriale des professionnels de sant&#x00E9;. Cet indicateur a vocation &#x00E0; devenir un outil d&#x2019;aide &#x00E0; la d&#x00E9;cision pour les acteurs des politiques de sant&#x00E9;, en pr&#x00E9;sentant une estimation de l&#x2019;offre de soins au niveau local selon une approche multi-professionnelle.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-02", "datePublication": "2025-04-02", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-02T23:59:27+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1180P0D1N000133?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29263", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
isionTexteVise": "/17/textes/1364.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>&#x00C0; l'alin&#x00E9;a 4, substituer aux mots :</p><p>&#x00AB; l'aide &#x00E0; mourir &#x00BB;,</p><p>les mots :</p><p>&#x00AB; la mise &#x00E0; mort demand&#x00E9;e &#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>L&#x2019;expression &#x00AB; aide &#x00E0; mourir &#x00BB; est un euph&#x00E9;misme volontairement ambigu, qui tend &#x00E0; infantiliser et &#x00E0; induire en erreur nos concitoyens. <br>Le l&#x00E9;gislateur doit nommer clairement les actes pour assumer son intention, &#x00E9;viter les d&#x00E9;rives d&#x2019;interpr&#x00E9;tation et garantir tant l&#x2019;intelligibilit&#x00E9; de la norme que la s&#x00E9;curit&#x00E9; juridique. <br>En l&#x2019;esp&#x00E8;ce, l&#x2019;aide &#x00E0; mourir constitue une mise &#x00E0; mort qui ne r&#x00E9;sulte pas d&#x2019;un jugement mais qui est institutionnalis&#x00E9;e et n&#x00E9;cessite le consentement des personnes concern&#x00E9;es (celles qui ex&#x00E9;cutent la mise &#x00E0; mort et celle qui la sollicite).</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-20", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-20T19:46:46+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N002420?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29391", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "30890", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
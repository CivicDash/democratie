{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1437P0D1N003461", "chronotag": "1757949480205", "legislature": "17", "identification": {"numeroLong": "3461", "numeroOrdreDepot": "3461", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1437P0D1", "texteLegislatifRef": "PIONANR5L17BTC1437", "triAmendement": "esaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaahu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841927", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA722366", "PA719890", "PA794330", "PA719472", "PA719118", "PA267200", "PA719372", "PA345722", "PA719388", "PA712014", "PA841601", "PA794206", "PA719080", "PA677483", "PA793270", "PA794802", "PA720190", "PA719550", "PA721584", "PA607395", "PA722102", "PA2960", "PA793992", "PA794466", "PA721844", "PA841931", "PA695512", "PA793194"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Ronceret,  M.&#160;Fugit, Mme&#160;Brulebois, M.&#160;Brosse, M.&#160;Jean-Ren&#233;&#160;Cazeneuve, M.&#160;Cormier-Bouligeon, M.&#160;Huyghe, M.&#160;Kasbarian, Mme&#160;Le&#160;Grip, Mme&#160;Le&#160;Meur, M.&#160;Ledoux, Mme&#160;Levasseur, M.&#160;Marion, Mme&#160;Marsaud, M.&#160;Mazars, M.&#160;Rousset, M.&#160;Sitzenstuhl, M.&#160;Sorre, Mme&#160;Liliana&#160;Tanguy, M.&#160;Terlier, M.&#160;Travert, Mme&#160;Vidal, M.&#160;Woerth, M.&#160;Alfandari, M.&#160;Moulliere, Mme&#160;Piron, M.&#160;Mazaury, M.&#160;Lepers et M.&#160;Vuibert"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5 quinquies", "articleDesignationCourte": "ART. 5 QUINQUIES", "articleDesignation": "ARTICLE 5 QUINQUIES", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1437.asp#D_Article_5_quinquies"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p>Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; supprimer cet article, qui introduit des contraintes suppl&#x00E9;mentaires pour les projets d&#x2019;ouvrages de stockage d&#x2019;eau &#x00E0; des fins d&#x2019;irrigation, en conditionnant leur autorisation &#x00E0; des &#x00E9;tudes hydrologiques syst&#x00E9;matiques.</p><p style=\"text-align: justify;\">Or, ces projets sont d&#x00E9;j&#x00E0; soumis &#x00E0; des &#x00E9;tudes d'impact environnemental rigoureuses et &#x00E0; des consultations avec les parties prenantes locales, comme l'exigent le Code de l'environnement et la directive-cadre sur l'eau. Ajouter des obligations suppl&#x00E9;mentaires sans tenir compte des r&#x00E9;alit&#x00E9;s locales alourdit inutilement les proc&#x00E9;dures et freine des projets essentiels pour s&#x00E9;curiser l'acc&#x00E8;s &#x00E0; l'eau des agriculteurs face aux &#x00E9;pisodes de s&#x00E9;cheresse.</p><p style=\"text-align: justify;\">Cet article doit donc &#x00EA;tre supprim&#x00E9; pour &#x00E9;viter des contraintes disproportionn&#x00E9;es et permettre un d&#x00E9;veloppement plus pragmatique et &#x00E9;quilibr&#x00E9; des infrastructures hydrauliques.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-22", "datePublication": "2025-05-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1437P0D1N003461?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
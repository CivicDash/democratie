{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1326P0D1N000049", "chronotag": "1757948407944", "legislature": "17", "identification": {"numeroLong": "AS49", "numeroOrdreDepot": "49", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1326P0D1", "texteLegislatifRef": "PIONANR5L17B1326", "triAmendement": "iaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaawzk", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719756", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719404", "PA642695", "PA720154", "PA719736", "PA720022", "PA720446", "PA795962", "PA720066", "PA841927", "PA793270", "PA722102"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Le&#160;Nabour,  M.&#160;Le&#160;Gac, M.&#160;Bothorel, Mme&#160;Dubr&#233;-Chirat, Mme&#160;Galliard-Minier, M.&#160;Lauzzana, Mme&#160;Liso, Mme&#160;Missoffe, Mme&#160;Rist, Mme&#160;Ronceret, M.&#160;Rousset et Mme&#160;Vidal"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1326.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "14", "alineaDesignation": "Alin\u00e9a 14"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;14, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;les crit&#x00E8;res retenus pour fixer le montant du financement du montant de la r&#x00E9;mun&#x00E9;ration des emplois n&#x00E9;cessaires &#x00E0; l&#x2019;installation et &#x00E0; l&#x2019;animation du comit&#x00E9; local pour l&#x2019;emploi et de&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; revenir sur la prise en charge par l&#x2019;&#x00C9;tat et le d&#x00E9;partement du financement des ressources humaines charg&#x00E9;es de l&#x2019;animation des comit&#x00E9;s locaux pour l&#x2019;emploi, qui constitue, dans un contexte de ma&#x00EE;trise des finances publiques, une nouveaut&#x00E9; au regard du cadre actuel de l&#x2019;exp&#x00E9;rimentation.</p><p style=\"text-align: justify;\">Par coh&#x00E9;rence avec l&#x2019;int&#x00E9;gration des comit&#x00E9;s locaux pour l&#x2019;emploi issus de l&#x2019;exp&#x00E9;rimentation &#x00AB;&nbsp;Territoire z&#x00E9;ro ch&#x00F4;meur de longue dur&#x00E9;e&nbsp;&#x00BB; dans la gouvernance territoriale du r&#x00E9;seau pour l&#x2019;emploi issue de la loi n&#x00B0; 2023&#x2011;1196 du&nbsp;18&nbsp;d&#x00E9;cembre&nbsp;2023 pour le plein emploi, il n&#x2019;y a pas lieu de pr&#x00E9;voir le financement du fonctionnement de cette instance.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-23", "datePublication": "2025-05-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-05-28T17:55:03+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1326P0D1N000049?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
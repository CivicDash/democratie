ramme": [{"libelle": "D\u00e9veloppement des entreprises et r\u00e9gulations", "programmeRef": "655717", "autorisationEngagement": "5000000", "creditPaiement": "5000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Plan France Tr\u00e8s haut d\u00e9bit", "programmeRef": "655719", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Statistiques et \u00e9tudes \u00e9conomiques", "programmeRef": "655721", "autorisationEngagement": "-5000000", "creditPaiement": "-5000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Strat\u00e9gies \u00e9conomiques", "programmeRef": "655723", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Financement des op\u00e9rations patrimoniales en 2026 sur le compte d'affectation sp\u00e9ciale \u00ab Participations financi\u00e8res de l'\u00c9tat \u00bb", "programmeRef": "655725", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement des d&#x00E9;put&#x00E9;s Socialistes et apparent&#x00E9;s, adopt&#x00E9; en commission des affaires &#x00E9;conomiques sous le num&#x00E9;ro II-CE121, vise &#x00E0; transformer l&#x2019;initiative France Num en un bras arm&#x00E9; de la mont&#x00E9;e en comp&#x00E9;titivit&#x00E9; de nos entreprises en la dotant de 5M&#x20AC; d&#x2019;euros de cr&#x00E9;dits pour le soutien &#x00E0; la transformation num&#x00E9;rique des TPE et PME.</p><p style=\"text-align: justify;\">L&#x2019;insuffisance de la num&#x00E9;risation et de la robotisation de nos entreprises est au coeur de notre retard en mati&#x00E8;re de productivit&#x00E9; et de comp&#x00E9;titivit&#x00E9; d&#x2019;avec nos voisins et concurrents. L&#x2019;initiative France Num a acquis une connaissance des acteurs et une visibilit&#x00E9; utile &#x00E0; nos entreprises mais elle ne dispose pas aujourd&#x2019;hui des moyens r&#x00E9;els pour accompagner tant en ing&#x00E9;nierie qu&#x2019;en &#x00E9;ventuelles subventions, nos TPE et PME &#x00E0; la hauteur de leurs besoins.</p><p style=\"text-align: justify;\">Ces 5 M&#x20AC; de cr&#x00E9;dits permettraient d&#x2019;engager cette transformation de France Num vers un accompagnement renforc&#x00E9; de nos entreprises, en visant une mont&#x00E9;e en charge progressive dans les prochaines ann&#x00E9;es.&nbsp;</p><p style=\"text-align: justify;\">Afin d&#x2019;assurer la recevabilit&#x00E9; financi&#x00E8;re du pr&#x00E9;sent amendement il est donc propos&#x00E9; de majorer de 5M&#x20AC;, en AE et CP, les cr&#x00E9;dits de l&#x2019;action 04 du programme 134, par la minoration &#x00E0; due concurrence des m&#x00EA;mes cr&#x00E9;dits de l&#x2019;action 09 du programme 220.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-11-06", "datePublication": "2025-11-08", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1364P0D1N002065", "chronotag": "1757949084086", "legislature": "17", "identification": {"numeroLong": "2065 (Rect)", "numeroOrdreDepot": "2065", "prefixeOrganeExamen": "AN", "numeroRect": "1"}, "examenRef": "EXANR5L17PO838901BTC1364P0D1", "texteLegislatifRef": "PIONANR5L17BTC1364", "triAmendement": "laaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaam", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841067", "groupePolitiqueRef": "PO847173", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA840657", "PA309643", "PA840701", "PA267561", "PA794770", "PA840721", "PA795966", "PA840967", "PA840685", "PA842279", "PA840983", "PA793354", "PA842255", "PA840915", "PA840075", "PA796042", "PA842097"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Allegret-Pilot,  Mme&#160;Ricourt&#160;Vaginay, Mme&#160;Besse, M.&#160;Chaix, Mme&#160;Bar&#232;ges, Mme&#160;Martinez, M.&#160;Chavent, M.&#160;Rambaud, M.&#160;Lioret, M.&#160;Tr&#233;buchet, Mme&#160;Bamana, M.&#160;Lenoir, Mme&#160;M&#233;lin, Mme&#160;Sicard, M.&#160;Monnier, M.&#160;Casterman, M.&#160;Boccaletti et Mme&#160;Roy"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 17", "articleDesignationCourte": "APR\u00c8S ART. 17", "articleDesignation": "APR\u00c8S L'ARTICLE 17, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1364.asp#D_Article_17"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Nul ne peut promouvoir l&#x2019;aide &#x00E0; mourir par voie de t&#x00E9;moignages, campagnes ou m&#x00E9;dia. Une telle promotion est punie de cinq ans d&#x2019;emprisonnement et de 300&nbsp;000&nbsp;euros d&#x2019;amende.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;Toute association re&#x0301;gulie&#x0300;rement de&#x0301;clare&#x0301;e depuis au moins cinq ans a&#x0300; la date des faits dont l&#x2019;objet statutaire comporte la de&#x0301;fense des droits des personnes a&#x0300; la vie, la promotion des soins palliatifs, la lutte contre le suicide ou la lutte contre l&#x2019;euthanasie peut exercer les droits reconnus a&#x0300; la partie civile en ce qui concerne les infractions pre&#x0301;vues au&nbsp;I du pre&#x0301;sent article lorsque les faits ont e&#x0301;te&#x0301; commis en vue de favoriser le recours &#x00E0; l&#x2019;aide a&#x0300; mourir ou les actes pre&#x0301;alables pre&#x0301;vus a&#x0300; la section 2&nbsp;<i>bis</i> du chapitre I<sup>er</sup>&nbsp;de la premi&#x00E8;re partie du code de la sant&#x00E9; publique.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; garantir une d&#x00E9;cision sans pression ext&#x00E9;rieure et assurer le libre exercice d&#x00E9;mocratique.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-24", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-24T11:47:37+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N002065?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29513", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "31106", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B1906P2D1N000614", "chronotag": "1762557556523", "legislature": "17", "identification": {"numeroLong": "II-CF614", "numeroOrdreDepot": "614", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B1906P2D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "ieoaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaal", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794982", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA795010"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Delpech et Mme&#160;Garin"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ETAT G - LISTE DES OBJECTIFS ET DES INDICATEURS DE PERFORMANCE", "articleDesignationCourte": "ART. 52", "articleDesignation": "ARTICLE 52", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE 52", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906C.asp#D_Etat_G__Liste_des_objectifs_et_des_ind"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "SE", "libelleMission": "Solidarit\u00e9, insertion et \u00e9galit\u00e9 des chances", "missionRef": "3367"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "1455", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 1455"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;1455, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Taux d&#x2019;appels trait&#x00E9;s par la permanence t&#x00E9;l&#x00E9;phonique de r&#x00E9;f&#x00E9;rence pour les victimes de violences sexuelles</i>&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">En cr&#x00E9;ant un indicateur sur le taux d&#x2019;appels trait&#x00E9;s par la plateforme t&#x00E9;l&#x00E9;phonique Viols-Femmes-Information g&#x00E9;r&#x00E9;e par le collectif f&#x00E9;ministe contre le viol (CFCV), le pr&#x00E9;sent amendement permet de mieux distinguer les diff&#x00E9;rentes mesures financ&#x00E9;es par le programme 137 pour lutter contre les violences sexistes et sexuelles.</p><p style=\"text-align: justify;\">Cet amendement, d&#x00E9;j&#x00E0; adopt&#x00E9; en premi&#x00E8;re lecture au PLF 2025, est &#x00E0; nouveau port&#x00E9; par les co-rapporteures de la D&#x00E9;l&#x00E9;gation aux droits des femmes sur le PLF 2026.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-29", "datePublication": "2025-11-07", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-11-06T23:01:52+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P2D1N000614?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "83711", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
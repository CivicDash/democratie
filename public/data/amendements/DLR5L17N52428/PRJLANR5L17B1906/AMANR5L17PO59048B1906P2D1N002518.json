gramme": [{"libelle": "D\u00e9veloppement des entreprises et r\u00e9gulations", "programmeRef": "656055", "autorisationEngagement": "9000000", "creditPaiement": "9000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Plan France Tr\u00e8s haut d\u00e9bit", "programmeRef": "656057", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Statistiques et \u00e9tudes \u00e9conomiques", "programmeRef": "656059", "autorisationEngagement": "-9000000", "creditPaiement": "-9000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Strat\u00e9gies \u00e9conomiques", "programmeRef": "656061", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Financement des op\u00e9rations patrimoniales en 2026 sur le compte d'affectation sp\u00e9ciale \u00ab Participations financi\u00e8res de l'\u00c9tat \u00bb", "programmeRef": "656063", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement des d&#x00E9;put&#x00E9;s Socialistes et apparent&#x00E9;s vise &#x00E0; soutenir les p&#x00F4;les de comp&#x00E9;titivit&#x00E9;.</p><p style=\"text-align: justify;\">Les p&#x00F4;les de comp&#x00E9;titivit&#x00E9;, au c&#x0153;ur de la dynamique d&#x2019;innovations en France, jouent un r&#x00F4;le essentiel pour attirer des investissements priv&#x00E9;s et renforcer la comp&#x00E9;titivit&#x00E9; nationale. En phase 5 de leur mission (2023&#x2011;2026), l&#x2019;&#x00C9;tat s&#x2019;&#x00E9;tait engag&#x00E9; &#x00E0; leur fournir une dotation annuelle de 9 millions d&#x2019;euros pour garantir leur fonctionnement et soutenir l&#x2019;&#x00E9;conomie locale via les collaborations entre PME, ETI et grands groupes. Supprimer cette dotation affaiblirait l&#x2019;impact des p&#x00F4;les dans les territoires, risquant de freiner&nbsp;l&#x2019;innovation et l&#x2019;attractivit&#x00E9; de la France.&nbsp;</p><p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise donc &#x00E0; prendre dans l&#x2019;action n&#x00B0;01 &#x00AB;&nbsp;infrastructures statistiques et missions r&#x00E9;galiennes&nbsp;&#x00BB; du programme n&#x00B0;220 &#x00AB;&nbsp;statistiques et &#x00E9;tudes &#x00E9;conomiques&nbsp;&#x00BB; la somme de 9 millions d&#x2019;euros en AE et en CP pour l&#x2019;attribuer &#x00E0; l&#x2019;action n&#x00B0;23 &#x00AB;&nbsp;industries et services&nbsp;&#x00BB; du programme n&#x00B0;134 &#x00AB;&nbsp;d&#x00E9;veloppement des entreprises et r&#x00E9;gulations&nbsp;&#x00BB; en AE et en CP.&nbsp;</p><p style=\"text-align: justify;\">Le gage, destin&#x00E9; &#x00E0; satisfaire aux exigences de la recevabilit&#x00E9; financi&#x00E8;re, a vocation &#x00E0; &#x00EA;tre lev&#x00E9; par le Gouvernement.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-11-06", "datePublication": "2025-11-08", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P2D1N002518?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "DER", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "84611", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
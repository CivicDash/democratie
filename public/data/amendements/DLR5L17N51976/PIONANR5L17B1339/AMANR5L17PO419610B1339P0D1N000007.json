rd": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;avant dernier alin&#x00E9;a&nbsp;de l&#x2019;article&nbsp;L.&nbsp;32&#x2011;1 du code des postes et des communications &#x00E9;lectroniques, il est ins&#x00E9;r&#x00E9; un alin&#x00E9;a ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Les personnes mentionn&#x00E9;es au premier alin&#x00E9;a du&nbsp;I de l&#x2019;article&nbsp;L.&nbsp;34&#x2011;8&#x2011;3 du code des postes et des communications &#x00E9;lectroniques doivent publier chaque semestre un rapport d&#x00E9;taillant les taux d&#x2019;&#x00E9;checs de raccordement, les pannes et les d&#x00E9;lais d&#x2019;intervention, sur chaque commune.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Afin d&#x2019;am&#x00E9;liorer la transparence et la responsabilit&#x00E9; des op&#x00E9;rateurs ainsi qu&#x2019;assurer les concitoyens et les pouvoirs publics de la qualit&#x00E9; du service public rendu, il semble pertinent &#x00E0; ce que les op&#x00E9;rateurs soient tenus de publier chaque semestre un rapport d&#x00E9;taillant les taux d&#x2019;&#x00E9;checs de raccordement, les pannes et les d&#x00E9;lais d&#x2019;intervention, par commune.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-23", "datePublication": "2025-05-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-27T17:39:35+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B1339P0D1N000007?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c postes", "divisionCodeLoi": "L34-8-3"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
nformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;12, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x00E9;lai&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;minimum&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement de pr&#x00E9;cision vise &#x00E0; ce que le m&#x00E9;decin ait un d&#x00E9;lai minimum&nbsp;de r&#x00E9;flexion dans la prise de d&#x00E9;cision.</p><p>Aucun d&#x00E9;lai minimum&nbsp;de r&#x00E9;flexion n'est demand&#x00E9; pour confirmer son choix.</p><p>La r&#x00E9;daction actuelle ne s&#x00E9;curise pas suffisamment le d&#x00E9;lai de consultation de l'ensemble des professionnels.</p><p>Tel est le sens de cet amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-07", "datePublication": "2025-05-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-05-22T16:32:52+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N000524?format=pdf"}, "dateDispoRepresentation"
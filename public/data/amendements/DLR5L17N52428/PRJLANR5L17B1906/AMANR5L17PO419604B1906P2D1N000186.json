nformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Formations sup\u00e9rieures et recherche universitaire", "programmeRef": "584491", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Vie \u00e9tudiante", "programmeRef": "584493", "autorisationEngagement": "19691707", "creditPaiement": "19691707", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherches scientifiques et technologiques pluridisciplinaires", "programmeRef": "584495", "autorisationEngagement": "-19691707", "creditPaiement": "-19691707", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche spatiale", "programmeRef": "584497", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche dans les domaines de l'\u00e9nergie, du d\u00e9veloppement et de la mobilit\u00e9 durables", "programmeRef": "584499", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche et enseignement sup\u00e9rieur en mati\u00e8re \u00e9conomique et industrielle", "programmeRef": "584501", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche duale (civile et militaire)", "programmeRef": "584503", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Enseignement sup\u00e9rieur et recherche agricoles", "programmeRef": "584505", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Socialistes et apparent&#x00E9;s vise &#x00E0; cr&#x00E9;er 13 services de sant&#x00E9; &#x00E9;tudiante suppl&#x00E9;mentaires, afin de garantir l&#x2019;acc&#x00E8;s &#x00E0; des soins pour tous les &#x00E9;tudiants, sur leur lieu d&#x2019;&#x00E9;tude et de vie.</p><p style=\"text-align: justify;\">La sant&#x00E9; des &#x00E9;tudiants constitue un enjeu majeur largement justifi&#x00E9; par le nombre de consultations annonc&#x00E9;es sur l&#x2019;ann&#x00E9;e 2024 . Si la cr&#x00E9;ation d&#x2019;un service de sant&#x00E9; suppl&#x00E9;mentaire t&#x00E9;moigne de l&#x2019;int&#x00E9;r&#x00EA;t partag&#x00E9; pour la sant&#x00E9; des &#x00E9;tudiants, il faut aller plus loin.&nbsp;L&#x2019;augmentation du nombre de services de sant&#x00E9; ne doit pas &#x00EA;tre fait &#x00E0; budget constant.&nbsp;</p><p style=\"text-align: justify;\">Ces derni&#x00E8;res ann&#x00E9;es, la d&#x00E9;gradation des conditions de vie &#x00E9;tudiante, la crise sanitaire, la pr&#x00E9;carit&#x00E9; et l&#x2019;augmentation des troubles psychologiques ont mis en lumi&#x00E8;re les insuffisances du maillage sanitaire universitaire.</p><p style=\"text-align: justify;\">Les services de sant&#x00E9; universit&#x00E9; pr&#x00E9;cieux, sont aujourd&#x2019;hui in&#x00E9;galement r&#x00E9;partis ne permettant pas &#x00E0; l&#x2019;ensemble des &#x00E9;tudiants de b&#x00E9;n&#x00E9;ficier de ce service. Dans un certain nombre de territoires, les &#x00E9;tudiants ne disposent pas d&#x2019;acc&#x00E8;s &#x00E0; des services d&#x00E9;di&#x00E9;s, ce manque venant s&#x2019;ajouter &#x00E0; celle du manque de m&#x00E9;decins dans certaines zones rurales.</p><p style=\"text-align: justify;\">Ainsi, le pr&#x00E9;sent amendement vise donc &#x00E0; cr&#x00E9;er 13 services de sant&#x00E9; &#x00E9;tudiants suppl&#x00E9;mentaires afin de garantir un &#x00E9;gal acc&#x00E8;s &#x00E0; la sant&#x00E9; et &#x00E0; la pr&#x00E9;vention .</p><p style=\"text-align: justify;\">Cet amendement proc&#x00E8;de au mouvement de cr&#x00E9;dits suivant&nbsp;:</p><p style=\"text-align: justify;\">Il abonde l&#x2019;action n&#x00B0; 03 &#x00AB;&nbsp;Sant&#x00E9; des &#x00E9;tudiants et activit&#x00E9; associatives, culturelles et sportives&nbsp;&#x00BB; du Programme 231 &#x00AB;&nbsp;Vie &#x00E9;tudiante&nbsp;&#x00BB; de 19&nbsp;691&nbsp;707&nbsp;&#x20AC;.&nbsp;</p><p style=\"text-align: justify;\">Il minore l&#x2019;action n&#x00B0; 02 &#x00AB;&nbsp;Agence nationale de la recherche&nbsp;&#x00BB; du programme 172 &#x00AB;&nbsp;Recherches scientifiques et technologiques pluridisciplinaires&nbsp;&#x00BB; &#x00E0; hauteur de 19&nbsp;691&nbsp;707&nbsp;&#x20AC;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-24", "datePublication": "2025-10-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-10-28T23:17:00+01:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B1906P2D1N000186?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
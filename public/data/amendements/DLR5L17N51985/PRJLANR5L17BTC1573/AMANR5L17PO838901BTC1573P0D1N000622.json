{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1573P0D1N000622", "chronotag": "1757950212272", "legislature": "17", "identification": {"numeroLong": "622", "numeroOrdreDepot": "622", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1573P0D1", "texteLegislatifRef": "PRJLANR5L17BTC1573", "triAmendement": "epaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaah", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA331582", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA267780", "PA266797"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Philippe&#160;Vigier,  Mme&#160;Firmin&#160;Le&#160;Bodo et M.&#160;Gosselin"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 19", "articleDesignationCourte": "ART. 19", "articleDesignation": "ARTICLE 19", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1573.asp#D_Article_19"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la fin, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;n&#x00E9;cessaire &#x00E0; la r&#x00E9;alisation des infrastructures portuaires et a&#x00E9;roportuaires.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;n&#x00E9;cessaires&nbsp;:&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter cet article par les deux alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;Aux op&#x00E9;rations de reconstruction conduites et coordonn&#x00E9;es par l&#x2019;&#x00E9;tablissement public cr&#x00E9;&#x00E9; en application de l&#x2019;article&nbsp;1<sup>er</sup> de la loi&nbsp;n&#x00B0;&nbsp;2025&#x2011;176 du&nbsp;24&nbsp;f&#x00E9;vrier&nbsp;2025 d&#x2019;urgence pour Mayotte&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;&#x00C0; la r&#x00E9;alisation des infrastructures portuaires et a&#x00E9;roportuaires, des ouvrages et installations des r&#x00E9;seaux publics d&#x2019;eau et d&#x2019;assainissement, des constructions, ouvrages et installations &#x00E0; l&#x2019;usage des forces de s&#x00E9;curit&#x00E9; int&#x00E9;rieure, des &#x00E9;tablissements p&#x00E9;nitentiaires, des &#x00E9;tablissements de sant&#x00E9; et m&#x00E9;dico&#x2011;sociaux ainsi que des ouvrages et installations de production et de distribution d&#x2019;&#x00E9;lectricit&#x00E9;.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; r&#x00E9;tablir l'article 19 du projet de loi dans sa version vot&#x00E9;e par le S&#x00E9;nat.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-19", "datePublication": "2025-06-27", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-06-27T10:02:57+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1573P0D1N000622?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29587", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "80368", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
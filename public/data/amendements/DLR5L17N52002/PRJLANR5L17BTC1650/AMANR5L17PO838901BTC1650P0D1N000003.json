l": "false", "urlDivisionTexteVise": "/17/textes/1650.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "26", "alineaDesignation": "Alin\u00e9a 26"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;la fin de l&#x2019;alin&#x00E9;a&nbsp;26, substituer au mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;consultative&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x00E9;lib&#x00E9;rative&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Les auteurs de cet amendement proposent d'accorder&nbsp;aux repr&#x00E9;sentants du personnel de l&#x2019;&#x00E9;tablissement public du commerce et de l&#x2019;industrie une voix d&#x00E9;lib&#x00E9;rative au sein du conseil d&#x2019;administration.</p><p>Au regard de l'importance du transfert de la tutelle de la CCI vers le nouvel &#x00E9;tablissement public, ainsi qu&#x2019;aux r&#x00E9;percussions directes sur les employ&#x00E9;s, nous consid&#x00E9;rons qu&#x2019;il est pleinement justifi&#x00E9; d&#x2019;octroyer une voix d&#x00E9;cisionnelle aux repr&#x00E9;sentants du personnel.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-07-01", "datePublication": "2025-07-02", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-07-02T22:53:15+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "sta
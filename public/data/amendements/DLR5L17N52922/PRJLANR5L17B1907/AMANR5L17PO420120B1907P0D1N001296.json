{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1907P0D1N001296", "chronotag": "1761557263243", "legislature": "17", "identification": {"numeroLong": "AS1296", "numeroOrdreDepot": "1296", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "aqaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaao", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720838", "groupePolitiqueRef": "PO845514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719736", "PA793174"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Bourouaha,  Mme&#160;Galliard-Minier et M.&#160;Monnet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "16", "alineaDesignation": "Alin\u00e9a 16"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>Supprimer les alin&#x00E9;as 16 &#x00E0; 18.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Ces trois alin&#x00E9;as visent &#x00E0; contraindre les artistes-auteurs &#x00E0; d&#x00E9;clarer leurs cotisations par voie d&#x00E9;mat&#x00E9;rialis&#x00E9;e, et ne leur laissent plus le choix du mode de d&#x00E9;claration.</p><p style=\"text-align: justify;\">Pourtant, l&#x2019;illectronisme touche un grand nombre de fran&#x00E7;aises et de fran&#x00E7;ais. On estime que huit millions de personnes sont concern&#x00E9;s. Les personnes &#x00E2;g&#x00E9;es et les personnes les plus pr&#x00E9;caires sont les premiers touch&#x00E9;s.</p><p style=\"text-align: justify;\">Ces trois alin&#x00E9;as risquent d&#x2019;avoir des cons&#x00E9;quences importantes car tous les artistes-auteurs ne sont pas en mesure de souscrire &#x00E0; une d&#x00E9;claration sociale par voie &#x00E9;lectronique, ni de payer leurs cotisations par voix d&#x00E9;mat&#x00E9;rialis&#x00E9;e et pourraient donc &#x00EA;tre priv&#x00E9;s de leurs droits sociaux.</p><p style=\"text-align: justify;\">C&#x2019;est pourquoi, les auteurs de cet amendement estiment qu&#x2019;il est important de garantir l&#x2019;acc&#x00E8;s aux droits des artistes-auteurs par quelque moyen que ce soit en conservant le choix de leur mode de d&#x00E9;claration.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-20", "datePublication": "2025-10-26", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-10-27T10:27:21+01:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1907P0D1N001296?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "s16-18", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "83111", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
if": "<p style=\"text-align: justify;\">L&#x2019;&#x00E9;volution de la mise en &#x0153;uvre de cette strat&#x00E9;gie d&#x00E9;cennale des soins palliatifs et d&#x2019;accompagnement peut faire l&#x2019;objet d&#x2019;un d&#x00E9;bat chaque ann&#x00E9;e devant le Parlement avant l&#x2019;examen du projet de loi de finances.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; instaurer un d&#x00E9;bat annuel devant le Parlement sur la mise en &#x0153;uvre de la strat&#x00E9;gie d&#x00E9;cennale des soins palliatifs. Ce rendez-vous permettra un suivi r&#x00E9;gulier et une meilleure transparence envers le l&#x00E9;gislateur avant l&#x2019;examen du projet de loi de finances.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-05", "datePublication": "2025-04-09", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-04-09T22:09:51+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1102P0D1N000463?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeL
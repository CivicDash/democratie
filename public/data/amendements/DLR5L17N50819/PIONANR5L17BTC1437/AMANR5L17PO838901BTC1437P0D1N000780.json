{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1437P0D1N000780", "chronotag": "1757949505263", "legislature": "17", "identification": {"numeroLong": "780", "numeroOrdreDepot": "780", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1437P0D1", "texteLegislatifRef": "PIONANR5L17BTC1437", "triAmendement": "esaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaacqac", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722390", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794994", "PA721442", "PA718884", "PA840979", "PA608416", "PA793094", "PA794042"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Corneloup,  M.&#160;Portier, Mme&#160;Petex, Mme&#160;Bazin-Malgras, M.&#160;Le&#160;Fur, M.&#160;Hetzel, M.&#160;Ray et Mme&#160;Dezarnaud"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5 quinquies", "articleDesignationCourte": "ART. 5 QUINQUIES", "articleDesignation": "ARTICLE 5 QUINQUIES", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1437.asp#D_Article_5_quinquies"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement propose la suppression de cet article, qui interdit l&#x2019;irrigation des cultures interm&#x00E9;diaires &#x00E0; vocation &#x00E9;nerg&#x00E9;tique &#x00E0; partir de pr&#x00E9;l&#x00E8;vements dans les eaux superficielles ou souterraines.&nbsp;</p><p style=\"text-align: justify;\">Une telle restriction impose une contrainte suppl&#x00E9;mentaire aux agriculteurs, en limitant leur capacit&#x00E9; &#x00E0; diversifier leurs cultures et &#x00E0; s&#x00E9;curiser leurs revenus. Elle ne prend pas en compte les besoins concrets des exploitations ni les enjeux li&#x00E9;s aux fili&#x00E8;res &#x00E9;nerg&#x00E9;tiques, pourtant essentielles &#x00E0; la transition &#x00E9;cologique. Interdire toute forme d&#x2019;irrigation, m&#x00EA;me raisonn&#x00E9;e, revient &#x00E0; freiner le potentiel de production agricole et &#x00E0; fragiliser la souverainet&#x00E9; alimentaire.</p><p style=\"text-align: justify;\">La suppression de cet article permettrait de r&#x00E9;tablir un &#x00E9;quilibre plus juste, tenant compte des r&#x00E9;alit&#x00E9;s du terrain et des d&#x00E9;fis &#x00E9;conomiques et environnementaux auxquels font face les agriculteurs.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-21", "datePublication": "2025-05-23", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "RT", "libelle": "Retir\u00e9"}, "sousEtat": {"code": "RT", "libelle": "Retir\u00e9 apr\u00e8s publication"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1437P0D1N000780?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
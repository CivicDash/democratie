{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1100P0D1N000008", "chronotag": "1757947972822", "legislature": "17", "identification": {"numeroLong": "AS8", "numeroOrdreDepot": "8", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1100P0D1", "texteLegislatifRef": "PIONANR5L17B1100", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaz", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720386", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Di&#160;Filippo"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1100.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "18", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 18"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter cet article par l&#x2019;alin&#x00E9;a suivant&nbsp;: </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;VII.&nbsp;&#x2013;&nbsp;En aucun cas, la possibilit&#x00E9; d&#x2019;avoir recours au suicide assist&#x00E9; et &#x00E0; l&#x2019;euthanasie ne peut &#x00EA;tre ouverte aux personnes mineures. Toute d&#x00E9;marche pros&#x00E9;lyte &#x00E0; leur &#x00E9;gard concernant le faire-mourir est totalement interdite.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Plusieurs d&#x00E9;put&#x00E9;s ont d&#x00E9;pos&#x00E9; des amendements lors de l&#x2019;examen du texte en juin dernier, afin que l&#x2019;euthanasie et le suicide assist&#x00E9; puissent &#x00EA;tre pratiqu&#x00E9;es sur des mineurs. Il est essentiel de graver dans le marbre l&#x2019;interdiction totale et d&#x00E9;finitive d&#x2019;une telle d&#x00E9;rive, qui exposerait nos enfants &#x00E0; de graves dangers et porterait fortement atteinte au principe de protection des plus faibles</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-01", "datePublication": "2025-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-04-30T12:47:47+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N000008?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B0842P0D1N000056", "chronotag": "1742990757337", "legislature": "17", "identification": {"numeroLong": "CL56", "numeroOrdreDepot": "56", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B0842P0D1", "texteLegislatifRef": "PIONANR5L17B0842", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaami", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795010", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA721426"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Garin, rapporteure et Mme&#160;Riotton, rapporteure"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0842.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I. &#x2013;&nbsp;<span> </span>&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;6, supprimer le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;notamment&nbsp;&#x00BB;.</p><p>II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter ce m&#x00EA;me alin&#x00E9;a par les mots&nbsp;:&nbsp;</p><p>&#x00AB;&nbsp;, quelles que soient leurs natures&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Conform&#x00E9;ment &#x00E0; l&#x2019;avis rendu par le Conseil d&#x2019;&#x00C9;tat le 6 mars 2025 et afin de garantir le respect du principe de l&#x00E9;galit&#x00E9; des d&#x00E9;lits et des peines, le pr&#x00E9;sent amendement supprime le terme &#x00AB;&nbsp;notamment&nbsp;&#x00BB;, afin de ne pas introduire une ind&#x00E9;termination quant &#x00E0; la d&#x00E9;finition d&#x2019;autres circonstances de fait potentielles susceptibles de caract&#x00E9;riser le comportement illicite de l&#x2019;auteur.</p><p style=\"text-align: justify;\">Pour toutefois souligner la diversit&#x00E9; des situations pouvant &#x00EA;tre prises en compte au travers des quatre modes de r&#x00E9;alisation de l&#x2019;infraction que sont la violence, la contrainte, la menace ou la surprise, le pr&#x00E9;sent amendement ajoute les mots&nbsp;: &#x00AB;&nbsp;quelles que soient leurs natures&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-24", "datePublication": "2025-03-25", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-26T13:05:44+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0842P0D1N000056?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "divisionCodeLoi": "id"}, "discussionCommune": {"idDiscussion": "29416", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "76502", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
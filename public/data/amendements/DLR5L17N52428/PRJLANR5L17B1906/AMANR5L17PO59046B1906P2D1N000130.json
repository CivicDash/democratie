{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59046B1906P2D1N000130", "chronotag": "1761763903292", "legislature": "17", "identification": {"numeroLong": "II-DN130", "numeroOrdreDepot": "130", "prefixeOrganeExamen": "CION_DEF", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59046B1906P2D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "abwaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794270", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA333285", "PA341981", "PA826635", "PA841243", "PA774960", "PA841681", "PA719930"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Pic,  M.&#160;Garot, Mme&#160;R&#233;calde, M.&#160;Aur&#233;lien&#160;Rousseau, M.&#160;Saint-Pasteur, Mme&#160;Santiago, M.&#160;Sother et M.&#160;Vallaud"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ARTICLE 68", "articleDesignationCourte": "APR\u00c8S ART. 68", "articleDesignation": "APR\u00c8S L'ARTICLE 68, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906C.asp#D_ARTICLE_68"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "DA", "libelleMission": "D\u00e9fense", "missionRef": "3327"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Dans un d&#x00E9;lai de six mois &#x00E0; compter de la promulgation de la pr&#x00E9;sente loi, le Gouvernement remet au Parlement un rapport sur les cons&#x00E9;quences budg&#x00E9;taires relatives &#x00E0; la commande d&#x2019;un deuxi&#x00E8;me porte-avions de nouvelle g&#x00E9;n&#x00E9;ration, ainsi que les besoins humains n&#x00E9;cessaires &#x00E0; son fonctionnement.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Socialistes et apparent&#x00E9;s vise &#x00E0; obtenir un rapport &#x00E9;valuant le co&#x00FB;t financier et les besoins humains n&#x00E9;cessaires &#x00E0; la commande et au fonctionnement d&#x2019;un deuxi&#x00E8;me porte-avions de nouvelle g&#x00E9;n&#x00E9;ration.</p><p style=\"text-align: justify;\">Loin de remettre en cause l&#x2019;int&#x00E9;r&#x00EA;t strat&#x00E9;gique d&#x2019;un deuxi&#x00E8;me porte-avions pour notre Marine nationale, ces &#x00E9;l&#x00E9;ments seraient particuli&#x00E8;rement pertinents pour &#x00E9;clairer la repr&#x00E9;sentation nationale dans la perspective de l&#x2019;actualisation de la pr&#x00E9;sente LPM et de son &#x00E9;ventuelle r&#x00E9;orientation.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-25", "datePublication": "2025-10-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-29T19:51:19+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59046B1906P2D1N000130?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "zzrapport", "divisionCodeLoi": "porte avions"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
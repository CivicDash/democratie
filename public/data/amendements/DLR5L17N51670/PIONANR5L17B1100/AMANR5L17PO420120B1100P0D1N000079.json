{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1100P0D1N000079", "chronotag": "1757947881366", "legislature": "17", "identification": {"numeroLong": "AS79", "numeroOrdreDepot": "79", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1100P0D1", "texteLegislatifRef": "PIONANR5L17B1100", "triAmendement": "ciaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaui", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA608416", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA722390", "PA720386", "PA330008", "PA840979", "PA793508", "PA606098", "PA774954", "PA266797", "PA794058"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Hetzel,  Mme&#160;Corneloup, M.&#160;Di&#160;Filippo, M.&#160;Breton, M.&#160;Le&#160;Fur, M.&#160;Brigand, M.&#160;Marleix, Mme&#160;Blin, M.&#160;Gosselin et Mme&#160;Gruet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 7", "articleDesignationCourte": "ART. 7", "articleDesignation": "ARTICLE 7", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1100.asp#D_Article_7"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;7 pr&#x00E9;cise les droits de la personne dans le cadre d&#x2019;une proc&#x00E9;dure d&#x2019;euthanasie et de suicide assist&#x00E9;.</p><p style=\"text-align: justify;\">L&#x2019;administration du geste l&#x00E9;tal doit &#x00EA;tre encadr&#x00E9;e. Or rien n&#x2019;est pr&#x00E9;cis&#x00E9; dans cet article sur l&#x2019;endroit o&#x00F9; ce geste peut &#x00EA;tre pratiqu&#x00E9;. Cela peut conduire &#x00E0; un manque de transparence et des abus qui seront incontr&#x00F4;lables puisque cela pourra &#x00EA;tre effectu&#x00E9; sur la voie publique, dans les &#x00E9;tablissements de sant&#x00E9;, les &#x00E9;tablissements m&#x00E9;dico sociaux, les &#x00E9;tablissements d&#x2019;enseignement, les &#x00E9;tablissements p&#x00E9;nitentiaires, les &#x00E9;tablissements psychiatriques.</p><p style=\"text-align: justify;\">Aussi, il convient de supprimer cet article.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-01", "datePublication": "2025-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-30T15:06:50+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N000079?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "77132", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Infrastructures et services de transports", "programmeRef": "569475", "autorisationEngagement": "-1932807", "creditPaiement": "-1932807", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Affaires maritimes, p\u00eache et aquaculture", "programmeRef": "569477", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Paysages, eau et biodiversit\u00e9", "programmeRef": "569479", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Expertise, information g\u00e9ographique et m\u00e9t\u00e9orologie", "programmeRef": "569481", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Pr\u00e9vention des risques", "programmeRef": "569483", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "\u00c9nergie, climat et apr\u00e8s-mines", "programmeRef": "569485", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Service public de l'\u00e9nergie", "programmeRef": "569487", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage des politiques de l'\u00e9cologie, du d\u00e9veloppement et de la mobilit\u00e9 durables", "programmeRef": "569489", "autorisationEngagement": "1932807", "creditPaiement": "1932807", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Fonds d'acc\u00e9l\u00e9ration de la transition \u00e9cologique dans les territoires", "programmeRef": "569491", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00fbret\u00e9 nucl\u00e9aire et radioprotection", "programmeRef": "569493", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Ecologie \u2013 mise en extinction du plan de relance", "programmeRef": "569495", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement du groupe Socialistes et apparent&#x00E9;s vise &#x00E0; r&#x00E9;tablir les effectifs et les cr&#x00E9;dits du programme 217 &#x00E0; leur niveau de la loi de finances initiale pour 2025 s&#x2019;agissant des personnels &#x0153;uvrant pour les politiques du programme Urbanisme, territoires et am&#x00E9;nagement de l&#x2019;habitat.</p><p style=\"text-align: justify;\">Les suppressions d&#x2019;emplois et les coupes budg&#x00E9;taires successives dans les effectifs du minist&#x00E8;re de la Transition &#x00E9;cologique d&#x00E9;gradent les conditions de travail et fragilisent la mise en &#x0153;uvre des politiques publiques. Les agents t&#x00E9;moignent d&#x2019;un profond mal-&#x00EA;tre&nbsp;: manque de moyens, surcharge de travail, absence de perspectives, et injonctions paradoxales entre objectifs environnementaux et restrictions budg&#x00E9;taires.</p><p style=\"text-align: justify;\">Ces services sont essentiels pour planifier, suivre et &#x00E9;valuer les politiques publiques environnementales. Leur affaiblissement met en p&#x00E9;ril la capacit&#x00E9; de l&#x2019;&#x00C9;tat &#x00E0; piloter la transition &#x00E9;cologique et &#x00E0; r&#x00E9;pondre aux engagements climatiques et internationaux.</p><p style=\"text-align: justify;\">Il est propos&#x00E9; de r&#x00E9;tablir les cr&#x00E9;dits de personnel &#x00E0; hauteur de 1&nbsp;932&nbsp;807&nbsp;euros (en AE et en CP), correspondant au diff&#x00E9;rentiel entre 2025 et 2026.</p><p style=\"text-align: justify;\">Ainsi, le pr&#x00E9;sent amendement&nbsp;:</p><p style=\"text-align: justify;\">&#x2013;&nbsp;minore de 1&nbsp;932&nbsp;807&nbsp;euros en AE et en CP l&#x2019;action 04&nbsp;&#x00AB;&nbsp;Routes&nbsp;&#x2013;&nbsp;Entretien&nbsp;&#x00BB; du programme 203 &#x00AB;&nbsp;Infrastructures et services de transport&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">&#x2013;&nbsp;majore de 1&nbsp;932&nbsp;807&nbsp;euros en AE et en CP l&#x2019;action 28&#x00AB;&nbsp;Personnels &#x0153;uvrant dans le domaine de la strat&#x00E9;gie et de la connaissance des politiques de transition &#x00E9;cologique&nbsp;&#x00BB; du programme 217 &#x00AB;&nbsp;Conduite et pilotage des politiques de l&#x2019;&#x00E9;cologie, du d&#x00E9;veloppement et de la mobilit&#x00E9; durables&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">Nous proposons ce transfert de cr&#x00E9;dits pour respecter les r&#x00E8;gles impos&#x00E9;es par l&#x2019;article&nbsp;40&nbsp;mais nous ne souhaitons pas r&#x00E9;duire les cr&#x00E9;dits du programme &#x00AB;&nbsp;Infrastructures de transport&nbsp;&#x00BB;, &#x00E9;galement essentiels, et proposons que le Gouvernement l&#x00E8;ve le gage.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-24", "datePublication": "2025-10-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-29T11:58:53+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419865B1906P2D1N000135?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "217", "divisionCodeLoi": "15"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
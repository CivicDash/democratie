tif": "<p>&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;14, substituer aux mots&nbsp;:</p><p>&#x00AB;&nbsp;de trois&nbsp;&#x00BB;&nbsp;</p><p>les mots&nbsp;:&nbsp;</p><p>&#x00AB;&nbsp;d&#x2019;un&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le d&#x00E9;lai de trois mois est trop long pour garantir qu&#x2019;une nouvelle &#x00E9;valuation du caract&#x00E8;re libre et &#x00E9;clair&#x00E9; de la volont&#x00E9; de la personne soit effectu&#x00E9;e.&nbsp;</p><p style=\"text-align: justify;\">Il est donc n&#x00E9;cessaire de le ramener &#x00E0; un mois afin de s&#x2019;assurer que le consentement de la personne reste toujours valide.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-07", "datePublication": "2025-05-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-22T19:14:22+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N000522?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29396", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dis
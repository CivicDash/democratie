{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B1037P0D1N000031", "chronotag": "1761121243484", "legislature": "17", "identification": {"numeroLong": "AC31", "numeroOrdreDepot": "31", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B1037P0D1", "texteLegislatifRef": "PIONANR5L17B1037", "triAmendement": "aiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaap", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841553", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Tesson, rapporteur"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1037.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;4, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;internationaux, soci&#x00E9;taux et environnementaux&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La mention de la compr&#x00E9;hension &#x00AB;&nbsp;des enjeux du monde contemporain&nbsp;&#x00BB; qui r&#x00E9;sultera de la suppression propos&#x00E9;e, plus large, permettra d&#x2019;inclure l&#x2019;ensemble de ceux-ci.&nbsp;</p><p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;inverse, qualifier d&#x2019;embl&#x00E9;e les enjeux concern&#x00E9;s conduit &#x00E0; en exclure certains (comme les enjeux nationaux dans la r&#x00E9;daction actuelle) et &#x00E0; alourdir la r&#x00E9;daction, l&#x00E0; o&#x00F9; l&#x2019;un des objectifs de la r&#x00E9;&#x00E9;criture &#x00E9;tait &#x00E9;tait de simplifier cet article.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-21", "datePublication": "2025-10-21", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-10-22T10:20:29+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B1037P0D1N000031?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1281P0D1N000146", "chronotag": "1757948284765", "legislature": "17", "identification": {"numeroLong": "146 (Rect)", "numeroOrdreDepot": "146", "prefixeOrganeExamen": "AN", "numeroRect": "1"}, "examenRef": "EXANR5L17PO838901BTC1281P0D1", "texteLegislatifRef": "PIONANR5L17BTC1281", "triAmendement": "auaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaae", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA608416", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719968", "PA722390", "PA720386", "PA794058", "PA346782", "PA330008", "PA840979", "PA606098", "PA793508", "PA774954", "PA266797", "PA842141", "PA721442", "PA794994", "PA793094"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Hetzel,  Mme&#160;Sylvie&#160;Bonnet, Mme&#160;Corneloup, M.&#160;Di&#160;Filippo, Mme&#160;Gruet, M.&#160;Juvin, M.&#160;Breton, M.&#160;Le&#160;Fur, M.&#160;Marleix, M.&#160;Brigand, Mme&#160;Blin, M.&#160;Gosselin, Mme&#160;de&#160;Maistre, Mme&#160;Petex, M.&#160;Portier et M.&#160;Ray"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 4", "articleDesignationCourte": "APR\u00c8S ART. 4", "articleDesignation": "APR\u00c8S L'ARTICLE 4, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1281.asp#D_Article_4"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Un rapport annuel sur la mise en &#x0153;uvre du plan pluriannuel de d&#x00E9;veloppement des soins palliatifs est &#x00E9;tabli par le centre national de la fin de vie et est remis au Parlement.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Un d&#x00E9;veloppement r&#x00E9;el et concret des soins palliatifs et de la culture palliative est, aujourd&#x2019;hui, une ardente obligation. Il y a toujours 21 d&#x00E9;partements qui ne sont pas dot&#x00E9;s d&#x2019;unit&#x00E9;s de soins palliatifs.</p><p style=\"text-align: justify;\">Il revient &#x00E0; la repr&#x00E9;sentation nationale, dans le cadre de son travail d&#x2019;&#x00E9;valuation des politiques publiques, de suivre la concr&#x00E9;tisation des engagements pris.</p><p style=\"text-align: justify;\">En cons&#x00E9;quence, et par souci de simplification, le rapport annex&#x00E9; au projet de loi de finances tous les deux ans pour pr&#x00E9;senter la politique suivie en mati&#x00E8;re de soins palliatifs et d&#x2019;accompagnement &#x00E0; domicile, dans les &#x00E9;tablissements de sant&#x00E9; et dans les &#x00E9;tablissements m&#x00E9;dico-sociaux, pr&#x00E9;vu par l&#x2019;article&nbsp;15 de la loi du 22 avril 2005, est supprim&#x00E9;.</p><p style=\"text-align: justify;\">Tel est le sens de cet amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-02", "datePublication": "2025-05-14", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-05-14T16:45:13+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1281P0D1N000146?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29380", "article99": "false", "loiReference": {"codeLoi": "loi 2005-370", "divisionCodeLoi": "15"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1102P0D1N000476", "chronotag": "1744295517429", "legislature": "17", "identification": {"numeroLong": "AS476", "numeroOrdreDepot": "476", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1102P0D1", "texteLegislatifRef": "PIONANR5L17B1102", "triAmendement": "fiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabpo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA267780", "groupePolitiqueRef": "PO845470", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Firmin&#160;Le&#160;Bodo"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1102.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "17", "alineaDesignation": "Alin\u00e9a 17"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>Supprimer l'alin&#x00E9;a 17.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; supprimer l&#x2019;alin&#x00E9;a pr&#x00E9;voyant l&#x2019;&#x00E9;tude de la cr&#x00E9;ation de 101 maisons d&#x2019;accompagnement, correspondant &#x00E0; un d&#x00E9;ploiement dans chaque d&#x00E9;partement d&#x2019;ici 2034.</p><p style=\"text-align: justify;\">D&#x2019;une part, cette disposition repose sur une confusion entre d&#x00E9;partements et territoires&nbsp;: si la France ne compte que 101 d&#x00E9;partements, ce chiffre n&#x2019;inclut pas l&#x2019;ensemble des collectivit&#x00E9;s d&#x2019;outre-mer, dont plusieurs ne rel&#x00E8;vent pas du statut d&#x00E9;partemental (telles que la Polyn&#x00E9;sie fran&#x00E7;aise, Saint-Pierre-et-Miquelon, Wallis-et-Futuna, la Nouvelle-Cal&#x00E9;donie). Lier m&#x00E9;caniquement le d&#x00E9;ploiement &#x00E0; un crit&#x00E8;re d&#x00E9;partemental uniforme ne refl&#x00E8;te donc ni la diversit&#x00E9; institutionnelle des territoires, ni leurs besoins r&#x00E9;els.</p><p style=\"text-align: justify;\">D&#x2019;autre part, une telle programmation n&#x2019;a aucune port&#x00E9;e contraignante dans une proposition de loi, qui ne peut ni engager l&#x2019;&#x00C9;tat sur un plan budg&#x00E9;taire, ni imposer des cr&#x00E9;ations de structures ou des calendriers d&#x2019;ex&#x00E9;cution. Elle risque ainsi d&#x2019;entretenir des attentes sans traduction juridique ou financi&#x00E8;re r&#x00E9;elle.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-05", "datePublication": "2025-04-08", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-04-10T16:31:42+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1102P0D1N000476?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "divisionCodeLoi": "s17"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "77940", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
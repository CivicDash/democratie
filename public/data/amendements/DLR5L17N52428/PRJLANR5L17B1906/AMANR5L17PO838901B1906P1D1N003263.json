"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906A.asp#D_ARTICLE_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Le 1 du&nbsp;I de l&#x2019;article&nbsp;223&nbsp;<i>sexies</i> du code g&#x00E9;n&#x00E9;ral des imp&#x00F4;ts est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">1&#x00B0;&nbsp;Au d&#x00E9;but du deuxi&#x00E8;me alin&#x00E9;a, le taux&nbsp;: &#x00AB;&nbsp;3&nbsp;%&nbsp;&#x00BB; est remplac&#x00E9; par le taux&nbsp;: &#x00AB;&nbsp;6&nbsp;%&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">2&#x00B0;&nbsp;Au d&#x00E9;but du dernier alin&#x00E9;a, le taux&nbsp;: &#x00AB;&nbsp;4&nbsp;%&nbsp;&#x00BB; est remplac&#x00E9; par le taux&nbsp;: &#x00AB;&nbsp;8&nbsp;%&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement propose de renforcer la contribution exceptionnelle des hauts revenus en portant les deux taux de 3 % et 4 % &#x00E0; 6 % et 8 %.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-22", "datePublication": "2025-10-24", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-27T09:50:01+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org
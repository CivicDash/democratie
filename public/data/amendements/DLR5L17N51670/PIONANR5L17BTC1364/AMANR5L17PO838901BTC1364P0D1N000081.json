{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1364P0D1N000081", "chronotag": "1757948536158", "legislature": "17", "identification": {"numeroLong": "81", "numeroOrdreDepot": "81", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1364P0D1", "texteLegislatifRef": "PIONANR5L17BTC1364", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaajkdnub", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA608416", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA346782", "PA642847", "PA720386", "PA330008", "PA840979", "PA793508", "PA606098", "PA774954", "PA266797", "PA794058", "PA719968", "PA718850", "PA793094"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Hetzel,  M.&#160;Juvin, M.&#160;Bazin, M.&#160;Di&#160;Filippo, M.&#160;Breton, M.&#160;Le&#160;Fur, M.&#160;Brigand, M.&#160;Marleix, Mme&#160;Blin, M.&#160;Gosselin, Mme&#160;Gruet, Mme&#160;Sylvie&#160;Bonnet, M.&#160;Cordier et M.&#160;Ray"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1364.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la seconde phrase de l&#x2019;alin&#x00E9;a&nbsp;6, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et il examine celle-ci, sauf s&#x2019;il ne l&#x2019;estime pas n&#x00E9;cessaire,&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;qu&#x2019;il examine&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter cet article par l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;VII.&nbsp;&#x2013;&nbsp;L&#x2019;article&nbsp;20 de la pr&#x00E9;sente loi n&#x2019;est pas applicable aux professionnels mentionn&#x00E9;s au&nbsp;II de l&#x2019;article&nbsp;L.&nbsp;1111&#x2011;12&#x2011;4 du code de la sant&#x00E9; publique.&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Dans la r&#x00E9;daction actuelle de l&#x2019;alin&#x00E9;a&nbsp;6, l&#x2019;examen par le m&#x00E9;decin, qui suit la demande d&#x2019;aide &#x00E0; mourir du patient, ne serait pas dans l&#x2019;obligation de l&#x2019;examiner. Or, il semble &#x00E9;tonnant qu&#x2019;&#x00E9;tant donn&#x00E9; la caract&#x00E8;re irr&#x00E9;versible d&#x2019;une aide &#x00E0; mourir, le m&#x00E9;decin qui joue un r&#x00F4;le central dans l&#x2019;analyse de la demande du patient, ne soit pas dans l&#x2019;obligation de l&#x2019;examiner. Cet amendement vise donc &#x00E0; corriger ce qui semble &#x00EA;tre une faille dans la proc&#x00E9;dure actuelle.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-05", "datePublication": "2025-05-22", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-22T12:21:57+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N000081?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29395", "article99": "false", "loiReference": {"codeLoi": "2", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "31088", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
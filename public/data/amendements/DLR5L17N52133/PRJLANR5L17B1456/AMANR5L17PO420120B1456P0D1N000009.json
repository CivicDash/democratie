{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1456P0D1N000009", "chronotag": "1757949869393", "legislature": "17", "identification": {"numeroLong": "AS9", "numeroOrdreDepot": "9", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1456P0D1", "texteLegislatifRef": "PRJLANR5L17B1456", "triAmendement": "baaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaas", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793354", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe Rassemblement National", "libelle": "Mme&#160;M&#233;lin et les membres du groupe Rassemblement National"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article liminaire", "articleDesignationCourte": "ART. LIMINAIRE", "articleDesignation": "ARTICLE LIMINAIRE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1456.asp#D_Article_liminaire"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">En 2024, le solde de ces administrations de s&#x00E9;curit&#x00E9; sociale s&#x2019;&#x00E9;l&#x00E8;verait &#x00E0; 0&nbsp;% du PIB.</p><p style=\"text-align: justify;\">Ce solde rel&#x00E8;ve d&#x2019;une logique purement comptable et ne repose que sur la notion abstraite d&#x2019;exc&#x00E9;dent de la CADES.</p><p style=\"text-align: justify;\">Le rapport de Cour des Comptes sur l&#x2019;application des lois de financement de la s&#x00E9;curit&#x00E9; sociale de l&#x2019;ann&#x00E9;e 2024 &#x00E9;voque pourtant &#x00AB;&nbsp;une trajectoire des comptes sociaux hors de contr&#x00F4;le&nbsp;&#x00BB; avec un d&#x00E9;ficit de la s&#x00E9;curit&#x00E9; sociale, hors fonds de solidarit&#x00E9; vieillesse d&#x2019;un montant de 16,8 Mds &#x20AC; en 2024.</p><p style=\"text-align: justify;\">Nous demandons donc la suppression de cet article.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-30", "datePublication": "2025-06-04", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-06-04T12:50:06+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1456P0D1N000009?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "htt
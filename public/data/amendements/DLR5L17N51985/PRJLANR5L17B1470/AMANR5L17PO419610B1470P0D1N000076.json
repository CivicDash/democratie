{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B1470P0D1N000076", "chronotag": "1757949973236", "legislature": "17", "identification": {"numeroLong": "CE76", "numeroOrdreDepot": "76", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B1470P0D1", "texteLegislatifRef": "PRJLANR5L17B1470", "triAmendement": "fcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaju", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA2940", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA840997", "PA793980", "PA794146", "PA722142", "PA841713"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Voynet,  M.&#160;Biteau, M.&#160;Fournier, Mme&#160;Laernoes, M.&#160;Ruffin et M.&#160;Tavernier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 19", "articleDesignationCourte": "ART. 19", "articleDesignation": "ARTICLE 19", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1470.asp#D_Article_19"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">A&#x0300; l&#x2019;aline&#x0301;a 3, apr&#x00E8;s la seconde occurrence du mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;e&#x0301;tablissements&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">inse&#x0301;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;publics&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; pr&#x00E9;ciser que les &#x00E9;tablissements de sant&#x00E9; concern&#x00E9;s par le r&#x00E9;gime de prise de possession anticip&#x00E9;e sont exclusivement des &#x00E9;tablissements publics de sant&#x00E9;. Cette restriction se justifie par la nature m&#x00EA;me de ce dispositif d&#x00E9;rogatoire, qui doit &#x00EA;tre r&#x00E9;serv&#x00E9; &#x00E0; des projets d&#x2019;int&#x00E9;r&#x00EA;t g&#x00E9;n&#x00E9;ral r&#x00E9;pondant &#x00E0; un besoin collectif urgent. Pour assurer la continuit&#x00E9; du service public hospitalier &#x00E0; Mayotte, la priorit&#x00E9; doit &#x00EA;tre donn&#x00E9;e &#x00E0; la r&#x00E9;alisation d&#x2019;un second site hospitalier, annonc&#x00E9; d&#x00E8;s 2019 mais dont le chantier n&#x2019;a toujours pas &#x00E9;t&#x00E9; lanc&#x00E9;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-05", "datePublication": "2025-06-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-06-11T10:39:18+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistreme
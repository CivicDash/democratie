{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1237P0D1N000025", "chronotag": "1757948236550", "legislature": "17", "identification": {"numeroLong": "AS25", "numeroOrdreDepot": "25", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1237P0D1", "texteLegislatifRef": "PIONANR5L17B1237", "triAmendement": "jaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaav", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793696", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA842279", "PA794434", "PA841645", "PA840119", "PA794762", "PA842073", "PA840143", "PA794678", "PA793608", "PA840967", "PA793672", "PA793354", "PA794322", "PA793656", "PA793230", "PA793382"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Muller,  Mme&#160;Bamana, M.&#160;Bentz, M.&#160;Bernhardt, Mme&#160;Delannoy, Mme&#160;Dogor-Such, M.&#160;Dussausaye, M.&#160;Florquin, M.&#160;Frapp&#233;, Mme&#160;Levavasseur, M.&#160;Lioret, Mme&#160;Loir, Mme&#160;M&#233;lin, M.&#160;M&#233;nag&#233;, Mme&#160;Pollet, Mme&#160;Ranc et M.&#160;Tach&#233;&#160;de&#160;la&#160;Pagerie"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "APR\u00c8S ART. 3", "articleDesignation": "APR\u00c8S L'ARTICLE 3, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1237.asp#D_Article_3"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Tous les trois ans, le Gouvernement remet au Parlement un rapport d&#x2019;&#x00E9;valuation des politiques de lutte contre la mortalit&#x00E9; infantile assorti d&#x2019;indicateurs de performance nationaux et d&#x00E9;partementaux et d&#x2019;un bilan des mesures mises en &#x0153;uvre par territoire.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Un suivi rigoureux des politiques publiques est indispensable pour garantir leur efficacit&#x00E9;. En publiant un rapport unique, mais d&#x00E9;clin&#x00E9; &#x00E0; l&#x2019;&#x00E9;chelle d&#x00E9;partementale, cet amendement permet d&#x2019;identifier les zones les plus en difficult&#x00E9; afin d&#x2019;ajuster les efforts. Il s&#x2019;agit &#x00E9;galement d&#x2019;un outil pour les &#x00E9;lus locaux, afin d&#x2019;appuyer leurs demandes de moyens. Ce rapport permet enfin au Parlement de jouer pleinement son r&#x00F4;le de contr&#x00F4;le.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-03", "datePublication": "2025-05-07", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-07T12:42:00+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1237P0D1N000025?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "zzz", "divisionCodeLoi": null}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1281P0D1N000272", "chronotag": "1757948308693", "legislature": "17", "identification": {"numeroLong": "272", "numeroOrdreDepot": "272", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1281P0D1", "texteLegislatifRef": "PIONANR5L17BTC1281", "triAmendement": "laaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaad", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA840979", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA330008", "PA774954", "PA722390", "PA793094"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Le&#160;Fur,  M.&#160;Breton, Mme&#160;Blin, Mme&#160;Corneloup et M.&#160;Ray"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 15", "articleDesignationCourte": "ART. 15", "articleDesignation": "ARTICLE 15", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1281.asp#D_Article_15"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer les alin&#x00E9;as 5 &#x00E0; 8.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Les alin&#x00E9;as 5 &#x00E0; 8 propose de modifier l&#x2019;article L.&nbsp;1111&#x2011;11 du code de la sant&#x00E9; publique afin de pr&#x00E9;ciser que toute personne majeure <b>peut produire</b> des directives anticip&#x00E9;es pour le cas o&#x00F9; elle serait un jour hors d&#x2019;&#x00E9;tat d&#x2019;exprimer sa volont&#x00E9;.</p><p style=\"text-align: justify;\">Il appara&#x00EE;t n&#x00E9;cessaire de conserver la r&#x00E9;daction actuelle de l&#x2019;article L.&nbsp;1111&#x2011;11 du code de la sant&#x00E9; publique qui pr&#x00E9;voit en l&#x2019;&#x00E9;tat que toute personne majeure <b>peut r&#x00E9;diger</b> des directives anticip&#x00E9;es pour le cas o&#x00F9; elle serait un jour hors d&#x2019;&#x00E9;tat d&#x2019;exprimer sa volont&#x00E9;.</p><p style=\"text-align: justify;\">En effet, seul l'&#x00E9;crit est authentique.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-05", "datePublication": "2025-05-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-16T15:26:28+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1281P0D1N000272?format=pdf"}, "dateDispoRe
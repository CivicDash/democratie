{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B1906P1D1N003634", "chronotag": "1763374151826", "legislature": "17", "identification": {"numeroLong": "I-3634", "numeroOrdreDepot": "3634", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B1906P1D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "caaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaeds", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA267285", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA408578", "PA607090", "PA368", "PA718884", "PA774954", "PA266793", "PA330909", "PA794042", "PA720386", "PA266797", "PA1630", "PA608416", "PA695512", "PA793342", "PA721442", "PA794994", "PA267794", "PA643089", "PA794118"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Wauquiez,  M.&#160;Berger, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Barnier, Mme&#160;Bazin-Malgras, Mme&#160;Blin, M.&#160;Bony, M.&#160;Descoeur, Mme&#160;Dezarnaud, M.&#160;Di&#160;Filippo, M.&#160;Gosselin, M.&#160;Herbillon, M.&#160;Hetzel, M.&#160;Lepers, Mme&#160;Alexandra&#160;Martin, Mme&#160;Petex, M.&#160;Portier, Mme&#160;Tabarot, M.&#160;Thi&#233;riot et M.&#160;Vermorel-Marques"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ARTICLE 13", "articleDesignationCourte": "ART. 13", "articleDesignation": "ARTICLE 13", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906A.asp#D_ARTICLE_13"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>Supprimer cet article.<br><br></p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\"><b>Cet amendement du groupe Droite R&#x00E9;publicaine vise &#x00E0; faire obstacle au durcissement des malus automobiles.</b></p><p style=\"text-align: justify;\">Cet article constitue&nbsp;une nouvelle &#x00E9;tape&nbsp;d'accroissement&nbsp;de la fiscalit&#x00E9; sur les v&#x00E9;hicules, &#x00E0; travers notamment le durcissement du bar&#x00E8;me&nbsp;du malus CO2&nbsp; et la suppression du plafonnement du malus masse et du malus CO2 en 2028.</p><p style=\"text-align: justify;\">Dans ce d&#x00E9;bat budg&#x00E9;taire, la Droite R&#x00E9;publicaine s'opposera &#x00E0; toute hausse d'imp&#x00F4;ts et de taxes sur les entreprises et les Fran&#x00E7;ais en rappelant une &#x00E9;vidence : dans le pays qui d&#x00E9;tient d&#x00E9;j&#x00E0; le record des pr&#x00E9;l&#x00E8;vements obligatoires, le redressement de nos comptes publics ne peut s'op&#x00E9;rer que par la baisse des d&#x00E9;penses publiques.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-23", "datePublication": "2025-10-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-11-17T11:08:49+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B1906P1D1N003634?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2026IDS30029", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "81689", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
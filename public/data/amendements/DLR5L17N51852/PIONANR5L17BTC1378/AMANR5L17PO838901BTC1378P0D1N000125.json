{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1378P0D1N000125", "chronotag": "1757948933634", "legislature": "17", "identification": {"numeroLong": "125", "numeroOrdreDepot": "125", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1378P0D1", "texteLegislatifRef": "PIONANR5L17BTC1378", "triAmendement": "gaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaajsi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA842117", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA267285", "PA642847", "PA718884", "PA408578", "PA774954", "PA719968", "PA721410", "PA266793", "PA721816", "PA342240", "PA330008", "PA793508", "PA718838", "PA841047", "PA718850", "PA722390", "PA332523", "PA842141", "PA330909", "PA794042", "PA720386", "PA712015", "PA608826", "PA1327", "PA266797", "PA794058", "PA1630", "PA608416", "PA346782", "PA840979", "PA695512", "PA794750", "PA642725", "PA606098", "PA793342", "PA719272", "PA720644", "PA718784", "PA721442", "PA794994", "PA793094", "PA266808", "PA267794", "PA794178", "PA643089", "PA794118", "PA607090"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jeanbrun,  M.&#160;Wauquiez, M.&#160;Bazin, Mme&#160;Bazin-Malgras, M.&#160;Berger, Mme&#160;Blin, Mme&#160;Sylvie&#160;Bonnet, Mme&#160;Bonnivard, M.&#160;Bony, M.&#160;Boucard, M.&#160;Bourgeaux, M.&#160;Breton, M.&#160;Brigand, M.&#160;Fabrice&#160;Brun, M.&#160;Ceccoli, M.&#160;Cordier, Mme&#160;Corneloup, Mme&#160;Dalloz, Mme&#160;de&#160;Maistre, M.&#160;Descoeur, Mme&#160;Dezarnaud, M.&#160;Di&#160;Filippo, M.&#160;Dive, Mme&#160;Duby-Muller, M.&#160;Forissier, M.&#160;Gosselin, Mme&#160;Gruet, M.&#160;Herbillon, M.&#160;Hetzel, M.&#160;Juvin, M.&#160;Le&#160;Fur, M.&#160;Lepers, M.&#160;Liger, M.&#160;Li&#233;geon, M.&#160;Marleix, Mme&#160;Alexandra&#160;Martin, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Nury, M.&#160;Pauget, Mme&#160;Petex, M.&#160;Portier, M.&#160;Ray, M.&#160;Rolland, Mme&#160;Tabarot, M.&#160;Taite, M.&#160;Thi&#233;riot, M.&#160;Vermorel-Marques et M.&#160;Jean-Pierre&#160;Vigier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1378.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l'alin&#x00E9;a 2 par les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ou de d&#x00E9;veloppement &#x00E9;conomique&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Droite R&#x00E9;publicaine vise &#x00E0; &#x00E9;largir l&#x2019;adaptation du cadre de la r&#x00E9;sidence h&#x00F4;teli&#x00E8;re &#x00E0; vocation sociale pr&#x00E9;vue par ce texte, aux besoins de d&#x00E9;veloppement &#x00E9;conomique des territoires.</p><p style=\"text-align: justify;\">Adapter le cadre de la r&#x00E9;sidence h&#x00F4;teli&#x00E8;re &#x00E0; vocation sociale, afin de r&#x00E9;pondre aux besoins li&#x00E9;s &#x00E0; des projets de r&#x00E9;industrialisation, est une mesure pertinente. Toutefois, les enjeux &#x00E9;conomiques auxquels sont confront&#x00E9;s les territoires ne se limitent pas aux seules dynamiques de r&#x00E9;industrialisation. De nombreux territoires n&#x00E9;cessitent &#x00E9;galement des solutions temporaires et flexibles d&#x2019;h&#x00E9;bergement pour accompagner des projets de d&#x00E9;veloppement &#x00E9;conomique.</p><p style=\"text-align: justify;\">La r&#x00E9;ussite de ces projets repose largement sur la capacit&#x00E9; &#x00E0; accueillir rapidement une main-d'&#x0153;uvre souvent mobile, venue temporairement pour des chantiers, des missions de courte dur&#x00E9;e ou des phases d&#x2019;amor&#x00E7;age d&#x2019;activit&#x00E9;. Or, dans ces contextes, l&#x2019;offre locative traditionnelle est souvent insuffisante, inadapt&#x00E9;e ou trop rigide.</p><p style=\"text-align: justify;\">Tel est le sens de cet amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-12", "datePublication": "2025-05-15", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-05-15T12:59:36+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1378P0D1N000125?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29382", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
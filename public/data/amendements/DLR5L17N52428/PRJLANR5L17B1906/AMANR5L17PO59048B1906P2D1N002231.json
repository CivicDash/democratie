ogrammeRef": "641715", "autorisationEngagement": "-600000", "creditPaiement": "-400000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Affaires maritimes, p\u00eache et aquaculture", "programmeRef": "641717", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Paysages, eau et biodiversit\u00e9", "programmeRef": "641719", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Expertise, information g\u00e9ographique et m\u00e9t\u00e9orologie", "programmeRef": "641721", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Pr\u00e9vention des risques", "programmeRef": "641723", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "\u00c9nergie, climat et apr\u00e8s-mines", "programmeRef": "641725", "autorisationEngagement": "600000", "creditPaiement": "400000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Service public de l'\u00e9nergie", "programmeRef": "641727", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage des politiques de l'\u00e9cologie, du d\u00e9veloppement et de la mobilit\u00e9 durables", "programmeRef": "641729", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Fonds d'acc\u00e9l\u00e9ration de la transition \u00e9cologique dans les territoires", "programmeRef": "641731", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00fbret\u00e9 nucl\u00e9aire et radioprotection", "programmeRef": "641733", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Ecologie \u2013 mise en extinction du plan de relance", "programmeRef": "641735", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "false"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement abonde les cr&#x00E9;dits de l&#x2019;action n&#x00B0; 4 &#x00AB;&nbsp;Lutte contre le changement climatique et pour la qualit&#x00E9; de l&#x2019;air&nbsp;&#x00BB; du programme 174 &#x00AB;&nbsp;&#x00C9;nergie, climat et apr&#x00E8;s-mines&nbsp;&#x00BB; de 600&nbsp;000&nbsp;&#x20AC; en autorisations d&#x2019;engagement (AE) et de 400&nbsp;000&nbsp;&#x20AC; en cr&#x00E9;dits de paiement (CP), afin de porter &#x00E0; 9 M&#x20AC; les montants pr&#x00E9;vus pour le contr&#x00F4;le des certificats d&#x2019;&#x00E9;conomies d&#x2019;&#x00E9;nergie (CEE).</p><p style=\"text-align: justify;\">Votre rapporteur tient par cet amendement &#x00E0; souligner l&#x2019;importance de renforcer les moyens humains et financiers allou&#x00E9;s au contr&#x00F4;le des CEE, en particulier car de nouvelles politiques publiques vont d&#x00E9;sormais &#x00EA;tre financ&#x00E9;es par ce m&#x00E9;canisme. Il en va ainsi du soutien &#x00E0; l&#x2019;&#x00E9;lectrification des v&#x00E9;hicules.</p><p style=\"text-align: justify;\">Pour garantir la recevabilit&#x00E9; financi&#x00E8;re de l&#x2019;amendement, il est propos&#x00E9; un pr&#x00E9;l&#x00E8;vement&nbsp;du m&#x00EA;me montant d&#x2019;AE et de CP sur l&#x2019;action n&#x00B0;&nbsp;41 &#x00AB;&nbsp;Ferroviaire&nbsp;&#x00BB; du programme 203 &#x00AB;&nbsp;Infrastructures et services de transports&nbsp;&#x00BB;. Le rapporteur invite le Gouvernement &#x00E0; lever le gage.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-11-04", "datePublication": "2025-11-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-11-12T23:13:16+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P2D1N002231?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "6", "divisionCodeLoi": "4"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://w
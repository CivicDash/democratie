issionVisee": {"codeEtat": "B", "codeMissionMinefi": "VA", "libelleMission": "Coh\u00e9sion des territoires", "missionRef": "3319"}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "H\u00e9bergement, parcours vers le logement et insertion des personnes vuln\u00e9rables", "programmeRef": "645211", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Aide \u00e0 l'acc\u00e8s au logement", "programmeRef": "645213", "autorisationEngagement": "-400000000", "creditPaiement": "-400000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Urbanisme, territoires et am\u00e9lioration de l'habitat", "programmeRef": "645215", "autorisationEngagement": "400000000", "creditPaiement": "400000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Impulsion et coordination de la politique d'am\u00e9nagement du territoire", "programmeRef": "645217", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Politique de la ville", "programmeRef": "645219", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Interventions territoriales de l'\u00c9tat", "programmeRef": "645221", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Au Congr&#x00E8;s HLM de 2023, la promesse a &#x00E9;t&#x00E9; faite d&#x2019;abonder le budget du FNAP d&#x2019;1,2 milliards d&#x2019;euros sur 3 ans pour acc&#x00E9;l&#x00E9;rer la r&#x00E9;novation &#x00E9;nerg&#x00E9;tique du parc HLM. En r&#x00E9;alit&#x00E9;, les investissements ont &#x00E9;t&#x00E9; moindres, et le FNAP est dans une situation de financement critique. Cet amendement demande &#x00E0; ce que l&#x2019;investissement annuel de l&#x2019;&#x00C9;tat dans le FNAP progresse de fa&#x00E7;on &#x00E0; permettre la r&#x00E9;novation HLM &#x00E0; la hauteur des engagements pris.</p><p style=\"text-align: justify;\">Nous proposons en cons&#x00E9;quence de diminuer de 400 millions d&#x2019;euros en cr&#x00E9;dits de paiement et en autorisations d&#x2019;engagement les cr&#x00E9;dits de l&#x2019;action 01 &#x00AB;&nbsp;aides personnelles&nbsp;&#x00BB;&nbsp;du programme 109 &#x00AB;&nbsp;Aide &#x00E0; l&#x2019;acc&#x00E8;s au logement&nbsp;&#x00BB; pour abonder du m&#x00EA;me montant ceux de l&#x2019;action 01 &#x00AB;&nbsp;Construction locative et am&#x00E9;lioration du parc&nbsp;&#x00BB; du programme 135 &#x00AB;&nbsp;Urbanisme, territoires et am&#x00E9;lioration de l&#x2019;habitat. Nous invitons le Gouvernement &#x00E0; lever le gage pour garantir les moyens de l&#x2019;aide au logement, que nous ne souhaitons &#x00E9;videmment pas r&#x00E9;duire.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-30", "datePublication": "2025-11-06", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-11-06T17:18:03+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P2D1N000952?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "3", "divisionCodeLoi": "1"}, "discussionCommune": {"idDiscussion": "33763", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
leAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1437.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "20", "alineaDesignation": "Alin\u00e9a 20"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;20&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le d&#x00E9;cret mentionn&#x00E9; au pr&#x00E9;sent II&nbsp;<i>ter</i> est pris apr&#x00E8;s un avis de l&#x2019;Agence nationale de s&#x00E9;curit&#x00E9; sanitaire de l&#x2019;alimentation, de l&#x2019;environnement et du travail sur les risques pour la sant&#x00E9; humaine et l&#x2019;environnement li&#x00E9;s aux d&#x00E9;rogations envisag&#x00E9;es. Cet avis est rendu public.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Amendement de repli.&nbsp;</p><p>Le conseil de surveillance est avant tout une instance politique. &#x00C0; ce titre, elle n&#x2019;a aucune comp&#x00E9;tence au sujet des risques li&#x00E9;s aux pesticides.&nbsp;</p><p>Le&nbsp;rapport d&#x2019;information n&#x00B0;1530 de la Commission des affaires &#x00E9;conomiques de l&#x2019;Assembl&#x00E9;e nationale souligne&nbsp;&#x00E0; cet &#x00E9;gard que &#x00AB;&nbsp;Le conseil de surveillance a n&#x00E9;anmoins &#x00E9;chou&#x00E9; &#x00E0; &#x00EA;tre un v&#x00E9;ritable lieu de dialogue et a &#x00E9;t&#x00E9; per&#x00E7;u par certains de ses membres comme une &#x00AB;&nbsp;chambre d&#x2019;enregistrement&nbsp;&#x00BB; de d&#x00E9;cisions de d&#x00E9;rogation prise hors de son sein&nbsp;&#x00BB;.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-22", "datePublication": "2025-05-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1437P0D1N002912?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "0.1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
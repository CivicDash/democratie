ndard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;L.&nbsp;213&#x2011;8 du code de l&#x2019;environnement est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">1&#x00B0;&nbsp;Au&nbsp;2&#x00B0;, le taux&nbsp;: &#x00AB;&nbsp;20&nbsp;%&nbsp;&#x00BB; est remplac&#x00E9; par le taux&nbsp;: &#x00AB;&nbsp;10&nbsp;%&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">2&#x00B0;&nbsp;Au&nbsp;2&#x00B0;&nbsp;<i>bis</i>, le taux&nbsp;: &#x00AB;&nbsp;20&nbsp;%&nbsp;&#x00BB; est remplac&#x00E9; par le taux&nbsp;: &#x00AB;&nbsp;30&nbsp;%&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Le pr&#x00E9;sent amendement vise &#x00E0; r&#x00E9;tablir la r&#x00E9;duction de la redevance pour pr&#x00E9;l&#x00E8;vement d&#x2019;eau &#x00E0; usage agricole. Dans un contexte d&#x2019;augmentation des charges, cet all&#x00E8;gement contribue &#x00E0; maintenir la viabilit&#x00E9; &#x00E9;conomique des exploitations irrigu&#x00E9;es.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-22", "datePublication": "2025-05-26", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"code": "R", "libelle": "R"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "tr
nEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Vie \u00e9tudiante", "programmeRef": "584215", "autorisationEngagement": "200000000", "creditPaiement": "200000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherches scientifiques et technologiques pluridisciplinaires", "programmeRef": "584217", "autorisationEngagement": "-200000000", "creditPaiement": "-200000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche spatiale", "programmeRef": "584219", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche dans les domaines de l'\u00e9nergie, du d\u00e9veloppement et de la mobilit\u00e9 durables", "programmeRef": "584221", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche et enseignement sup\u00e9rieur en mati\u00e8re \u00e9conomique et industrielle", "programmeRef": "584223", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche duale (civile et militaire)", "programmeRef": "584225", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Enseignement sup\u00e9rieur et recherche agricoles", "programmeRef": "584227", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le sport &#x00E0; l&#x2019;Universit&#x00E9; et plus g&#x00E9;n&#x00E9;ralement dans le sup&#x00E9;rieur est dans une situation extr&#x00EA;mement difficile. Nous ne pouvons pas pr&#x00E9;tendre construire une nation sportive avec un taux de pratique aussi bas chez les jeunes &#x00E9;tudiantes et &#x00E9;tudiants. En effet, seuls 26&nbsp;% des &#x00E9;tudiantes et &#x00E9;tudiants pratiquent alors que, selon plusieurs enqu&#x00EA;tes, pr&#x00E8;s de 70&nbsp;% souhaiteraient le faire. Les barri&#x00E8;res &#x00E0; la pratique sont nombreuses&nbsp;: manque de temps, d&#x2019;infrastructures de proximit&#x00E9;, d&#x2019;argent &#x00E9;galement. Pour les casser, il faut des moyens financiers et humains, une v&#x00E9;ritable prise de conscience que le sport doit &#x00EA;tre partie int&#x00E9;grante des cursus.&nbsp;</p><p style=\"text-align: justify;\">C&#x2019;est ce que porte le groupe GDR depuis des ann&#x00E9;es, pour construire le service public du sport en faveur d&#x2019;une culture sportive pour tous et toute que nous souhaitons.&nbsp;</p><p style=\"text-align: justify;\">Aussi, en reprenant les travaux du SNEP-FSU, nous demandons, &#x00E0; travers cet amendement, des moyens suppl&#x00E9;mentaires, &#x00E0; hauteur de 200 millions d&#x2019;euros, pour le sport universitaire.</p><p style=\"text-align: justify;\">Afin de se conformer aux r&#x00E8;gles de la LOLF, l&#x2019;amendement est ainsi r&#x00E9;dig&#x00E9;&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">L&#x2019;action 03 du programme 231 est abond&#x00E9; en AE et CP de 200 millions d&#x2019;euros&nbsp;</p><p style=\"text-align: justify;\">Les cr&#x00E9;dits sont pr&#x00E9;lev&#x00E9;s, hors titre 2, sur l&#x2019;action 02 du programme 172.</p><p style=\"text-align: justify;\">Le Gouvernement est invit&#x00E9; &#x00E0; lever le gage, cet amendement ne visant pas &#x00E0; diminuer les moyens de l&#x2019;ANR mais &#x00E0; renforcer les moyens de l&#x2019;action culturelle et sportive du programme 231.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-24", "datePublication": "2025-10-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-28T22:19:50+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B1906P2D1N000160?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
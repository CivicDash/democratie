{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B1906P2D1N001431", "chronotag": "1762557556318", "legislature": "17", "identification": {"numeroLong": "II-CF1431", "numeroOrdreDepot": "1431", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B1906P2D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "dfaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaac", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720560", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719024", "PA794658", "PA605694", "PA719558"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Perrine&#160;Goulet,  M.&#160;Blanchet, M.&#160;Fait, M.&#160;Falorni et M.&#160;Balanant"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ETAT G - LISTE DES OBJECTIFS ET DES INDICATEURS DE PERFORMANCE", "articleDesignationCourte": "ART. 52", "articleDesignation": "ARTICLE 52", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE 52", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906C.asp#D_Etat_G__Liste_des_objectifs_et_des_ind"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "JA", "libelleMission": "Justice", "missionRef": "3345"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "979", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 979"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;979, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Nombre de d&#x00E9;cisions d&#x2019;aides juridictionnelles d&#x00E9;livr&#x00E9;es &#x00E0; des mineurs au civil et au p&#x00E9;nal&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement propose de cr&#x00E9;er deux nouveaux indicateurs de performance au sein de l&#x2019;objectif &#x00AB;&nbsp;Garantir l&#x2019;efficience du dispositif d&#x2019;aide juridictionnelle&nbsp;&#x00BB; du programme 101 &#x00AB;&nbsp;Acc&#x00E8;s au droit et &#x00E0; la justice&nbsp;&#x00BB; de la mission Justice, afin de conna&#x00EE;tre le nombre de d&#x00E9;cisions d&#x2019;aides juridictionnelles d&#x00E9;livr&#x00E9;es pour les mineurs ainsi que les majeurs au civil et au p&#x00E9;nal. Ils permettront de mieux quantifier et de mieux questionner la mise en &#x0153;uvre de ce type de d&#x00E9;cisions, en compl&#x00E9;ment des indicateurs d&#x00E9;j&#x00E0; existants.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-30", "datePublication": "2025-11-06", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-11-06T12:45:55+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P2D1N001431?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1907P0D1N001770", "chronotag": "1761946393315", "legislature": "17", "identification": {"numeroLong": "AS1770", "numeroOrdreDepot": "1770", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "obaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaus", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA642847", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bazin, rapporteur g&#233;n&#233;ral"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 45 bis", "articleDesignationCourte": "APR\u00c8S ART. 45 BIS", "articleDesignation": "APR\u00c8S L'ARTICLE 45 BIS, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_45_bis"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;la fin du&nbsp;II de l&#x2019;article&nbsp;88 de la loi n&#x00B0;&nbsp;2025&#x2011;199 du&nbsp;28&nbsp;f&#x00E9;vrier&nbsp;2025 de financement de la s&#x00E9;curit&#x00E9; sociale pour 2025, la date&nbsp;: &#x00AB;&nbsp;1<sup>er</sup>&nbsp;janvier&nbsp;2028&nbsp;&#x00BB; est remplac&#x00E9;e par la date &#x00AB;&nbsp;1<sup>er</sup>&nbsp;janvier&nbsp;2027&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La lutte contre la fraude doit &#x00EA;tre un objectif prioritaire des pouvoirs publics et du l&#x00E9;gislateur. Afin de fiabiliser le versement des pensions des assur&#x00E9;s r&#x00E9;sidant &#x00E0; l&#x2019;&#x00E9;tranger, la loi de financement de la s&#x00E9;curit&#x00E9; sociale pour 2025 a pr&#x00E9;vu de faire de l&#x2019;utilisation de dispositifs techniques permettant l&#x2019;usage de donn&#x00E9;es biom&#x00E9;triques le mode de preuve par d&#x00E9;faut de leur existence.</p><p style=\"text-align: justify;\">L&#x2019;entr&#x00E9;e en vigueur de cette mesure est pr&#x00E9;vue pour le 1<sup>er</sup>&nbsp;janvier&nbsp;2028. Afin d&#x2019;acc&#x00E9;l&#x00E9;rer son d&#x00E9;ploiement, le pr&#x00E9;sent article propose d&#x2019;avancer l&#x2019;&#x00E9;ch&#x00E9;ance d&#x2019;un an, au&nbsp;1<sup>er</sup>&nbsp;janvier&nbsp;2027.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-28", "datePublication": "2025-10-31", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-31T22:32:56+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1907P0D1N001770?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "l2025-199", "divisionCodeLoi": "88"}, "discussionCommune": {"idDiscussion": "33673", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
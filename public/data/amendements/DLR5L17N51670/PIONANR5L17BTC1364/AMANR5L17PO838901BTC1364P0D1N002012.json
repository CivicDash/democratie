ditionnel": "false", "urlDivisionTexteVise": "/17/textes/1364.asp#D_Article_9"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;la fin de l&#x2019;alin&#x00E9;a&nbsp;5, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ou l&#x2019;administre&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Si le m&#x00E9;decin ou l&#x2019;infirmier&nbsp;deviennent potentiellement&nbsp;ceux qui &#x00AB;&#x202F;mettent fin&#x202F;&#x00BB; &#x00E0; la vie, la dimension protectrice et apaisante de la relation soignant&#x2011;soign&#x00E9;&nbsp;en deviendrait&nbsp;profond&#x00E9;ment alt&#x00E9;r&#x00E9;e.</p><p>En coh&#x00E9;rence avec mon amendement d&#x00E9;pos&#x00E9; &#x00E0; l'article 2, cet amendement vise &#x00E0; ce que les soignants ne soient jamais impliqu&#x00E9;s dans l'acte de donner la mort, dans le respect de leur Serment de ne jamais nuire.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-23", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-23T11:17:37+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B1470P0D1N000396", "chronotag": "1757949791194", "legislature": "17", "identification": {"numeroLong": "CL396", "numeroOrdreDepot": "396", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B1470P0D1", "texteLegislatifRef": "PRJLANR5L17B1470", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaazznc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA331582", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA267780", "PA266797"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Philippe&#160;Vigier, rapporteur,  Mme&#160;Firmin&#160;Le&#160;Bodo, rapporteure et M.&#160;Gosselin, rapporteur"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "RAPPORT ANNEX\u00c9", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE PREMIER", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1470.asp#D_RAPPORT_ANNEXE"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "278", "alineaDesignation": "Alin\u00e9a 278"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Substituer &#x00E0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;278&nbsp;la phrase suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L&#x2019;&#x00C9;tat s&#x2019;engage dans la transformation du port de Longoni en grand port maritime &#x00E0; l&#x2019;issue de la d&#x00E9;l&#x00E9;gation de service public.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement pr&#x00E9;cise la feuille de route s&#x2019;agissant de la transformation du port de Longoni en grand port maritime. Cette infrastructure essentielle n&#x2019;est pas exploit&#x00E9;e &#x00E0; son plein potentiel.</p><p style=\"text-align: justify;\">D&#x00E8;s 2017, la Cour des comptes soulignait l&#x2019;int&#x00E9;r&#x00EA;t de doter le port de Longoni du statut de grand port maritime.</p><p style=\"text-align: justify;\">Les difficult&#x00E9;s pr&#x00E9;sent&#x00E9;es par la d&#x00E9;l&#x00E9;gation de service public, illustr&#x00E9;es encore r&#x00E9;cemment par les d&#x00E9;bats au tribunal administratif sur une &#x00E9;ventuelle r&#x00E9;siliation, doivent poser la question de son &#x00E9;ventuelle r&#x00E9;siliation &#x00E0; horizon tr&#x00E8;s proche. Il n&#x2019;est plus temps de produire des &#x00E9;tudes&nbsp;: il faut d&#x00E8;s maintenant s&#x2019;atteler &#x00E0; faire de Longoni un grand port maritime.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-09", "datePublication": "2025-06-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-06-11T18:59:04+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B1470P0D1N000396?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
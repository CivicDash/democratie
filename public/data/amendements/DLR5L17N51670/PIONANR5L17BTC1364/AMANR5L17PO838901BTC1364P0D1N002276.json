{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1364P0D1N002276", "chronotag": "1757948517823", "legislature": "17", "identification": {"numeroLong": "2276", "numeroOrdreDepot": "2276", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1364P0D1", "texteLegislatifRef": "PIONANR5L17BTC1364", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaajki", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793672", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793354", "PA721932", "PA795900", "PA840075", "PA840915", "PA842255", "PA841645", "PA793656", "PA793230", "PA794762", "PA793146", "PA841981", "PA794562", "PA793182", "PA794886", "PA793616", "PA642988", "PA842279", "PA796118", "PA793548", "PA841737", "PA840135", "PA840869", "PA793158", "PA794354", "PA841575", "PA841621", "PA793290", "PA840781", "PA794678", "PA793608", "PA794442", "PA841873"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Loir,  Mme&#160;M&#233;lin, Mme&#160;Lorho, Mme&#160;Hamelet, M.&#160;Casterman, M.&#160;Monnier, Mme&#160;Sicard, M.&#160;Bernhardt, Mme&#160;Pollet, Mme&#160;Ranc, Mme&#160;Dogor-Such, M.&#160;Dragon, Mme&#160;Rimbert, M.&#160;Ballard, M.&#160;Bovet, Mme&#160;Blanc, M.&#160;Mauvieux, M.&#160;de&#160;L&#233;pinau, Mme&#160;Bamana, Mme&#160;Auzanot, Mme&#160;Colombier, M.&#160;Gery, M.&#160;Dufosset, Mme&#160;Joubert, M.&#160;Beaurain, Mme&#160;Laporte, M.&#160;Vos, M.&#160;Evrard, Mme&#160;Lelouis, M.&#160;Tonussi, M.&#160;Frapp&#233;, Mme&#160;Levavasseur, Mme&#160;Robert-Dehault et M.&#160;Limongi"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1364.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "16", "alineaDesignation": "Alin\u00e9a 16"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;16, substituer au mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;accompagner&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;assister&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Les mots ont un sens, et dans un texte de loi, chaque terme employ&#x00E9; doit &#x00EA;tre pr&#x00E9;cis afin d&#x2019;&#x00E9;viter toute ambigu&#x00EF;t&#x00E9;.</p><p style=\"text-align: justify;\">Dans la r&#x00E9;daction initiale, la phrase &#x00AB;&nbsp;Il d&#x00E9;termine, en accord avec la personne, le m&#x00E9;decin ou l&#x2019;infirmier charg&#x00E9; de l&#x2019;accompagner pour l&#x2019;administration de la substance l&#x00E9;tale.&nbsp;&#x00BB; pouvait pr&#x00EA;ter &#x00E0; confusion. L&#x2019;usage du verbe &#x00AB;&nbsp;accompagner&nbsp;&#x00BB; pouvait en effet laisser entendre une implication plus large du professionnel de sant&#x00E9;. Or, dans le cadre du suicide assist&#x00E9;, le professionnel n&#x2019;accompagne pas le patient dans un processus global, il assiste techniquement la personne vis&#x00E9;e dans l&#x2019;administration de la substance l&#x00E9;tale, conform&#x00E9;ment &#x00E0; sa demande.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-22", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-22T21:41:07+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N002276?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29397", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "78972", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1907P0D1N001198", "chronotag": "1761673813554", "legislature": "17", "identification": {"numeroLong": "AS1198", "numeroOrdreDepot": "1198", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "beaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaow", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720334", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719412", "PA795820", "PA719404", "PA841295", "PA795240", "PA795270", "PA841327", "PA720342", "PA795908", "PA795026", "PA605694", "PA719218", "PA719338", "PA720038", "PA722374", "PA719138", "PA335612", "PA719558", "PA757225"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Pahun,  Mme&#160;Le&#160;Feur, M.&#160;Nadeau, M.&#160;Le&#160;Gac, M.&#160;Courbon, Mme&#160;Reid&#160;Arbelot, Mme&#160;Poussier-Winsback, M.&#160;Roussel, Mme&#160;Le&#160;Peih, M.&#160;Chenevard, Mme&#160;Violland, M.&#160;Falorni, M.&#160;Berville, Mme&#160;Melchior, Mme&#160;Josso, M.&#160;Isaac-Sibille, M.&#160;Castellani, M.&#160;Lecoq, M.&#160;Balanant et M.&#160;Seo"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 9", "articleDesignationCourte": "APR\u00c8S ART. 9", "articleDesignation": "APR\u00c8S L'ARTICLE 9, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_9"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Le deuxi&#x00E8;me alin&#x00E9;a de l&#x2019;article&nbsp;L.&nbsp;5553&#x2011;11 du code des transports est compl&#x00E9;t&#x00E9; par les mots&nbsp;: &#x00AB;&nbsp;,&nbsp;ou de navires &#x00E0; propulsion principale v&#x00E9;lique&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;La perte de recettes pour les organismes de s&#x00E9;curit&#x00E9; sociale est compens&#x00E9;e &#x00E0; due concurrence par la majoration de l&#x2019;accise sur les tabacs pr&#x00E9;vue au chapitre IV du titre I<sup>er</sup> du livre III du code des impositions sur les biens et services.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; r&#x00E9;instaurer le r&#x00E9;gime d&#x2019;exon&#x00E9;ration de charges sociales pour les navires &#x00E0; propulsion principale v&#x00E9;lique.&nbsp;</p><p style=\"text-align: justify;\">Le PLFSS pour 2025 avait supprim&#x00E9; ce r&#x00E9;gime &#x00E0; l&#x2019;exception des navires c&#x00E2;bliers et de service EMR. Cet amendement ajoute donc une exception afin de prot&#x00E9;ger une fili&#x00E8;re industrielle d&#x2019;avenir.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-20", "datePublication": "2025-10-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-28T18:49:55+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1907P0D1N001198?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c transports", "divisionCodeLoi": "L5553-11"}, "discussionCommune": {"idDiscussion": "33053", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
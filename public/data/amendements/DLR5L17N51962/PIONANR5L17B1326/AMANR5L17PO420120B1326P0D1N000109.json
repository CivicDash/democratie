{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1326P0D1N000109", "chronotag": "1757948517383", "legislature": "17", "identification": {"numeroLong": "AS109", "numeroOrdreDepot": "109", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1326P0D1", "texteLegislatifRef": "PIONANR5L17B1326", "triAmendement": "baaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaazv", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720560", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Perrine&#160;Goulet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1326.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "19", "alineaDesignation": "Alin\u00e9a 19"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;la fin de l&#x2019;alin&#x00E9;a&nbsp;19, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;depuis au moins un an malgr&#x00E9; l&#x2019;accomplissement d&#x2019;actes positifs de recherche d&#x2019;emploi et domicili&#x00E9;es depuis au moins six mois dans la zone g&#x00E9;ographique du territoire susmentionn&#x00E9;&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et r&#x00E9;pondant aux crit&#x00E8;res d&#x2019;&#x00E9;ligibilit&#x00E9; des personnes &#x00E0; un parcours d&#x2019;insertion par l&#x2019;activit&#x00E9; &#x00E9;conomique et des prescripteurs mentionn&#x00E9;s &#x00E0; l&#x2019;article&nbsp;L.&nbsp;5132&#x2011;3 du pr&#x00E9;sent code&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement entend, d&#x2019;une part, consid&#x00E9;rer qu&#x2019;un ch&#x00F4;mage de longue dur&#x00E9;e est sup&#x00E9;rieur &#x00E0; un an et d&#x00E9;pend de de la conjoncture &#x00E9;conomique et d&#x2019;autre part, de mettre le dispositif en coh&#x00E9;rence avec le titre de la proposition de loi.&nbsp;Pour ce faire, il renvoie en coh&#x00E9;rence aux crit&#x00E8;res permettant d&#x2019;&#x00EA;tre &#x00E9;ligible &#x00E0; un parcours d&#x2019;insertion par l&#x2019;activit&#x00E9; &#x00E9;conomique.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-23", "datePublication": "2025-05-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-05-28T16:57:15+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1364P0D1N002501", "chronotag": "1757948777995", "legislature": "17", "identification": {"numeroLong": "2501", "numeroOrdreDepot": "2501", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1364P0D1", "texteLegislatifRef": "PIONANR5L17BTC1364", "triAmendement": "blaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaky", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA346782", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642847", "PA774954", "PA840979", "PA330008", "PA793508", "PA1327", "PA719968", "PA608416", "PA606098", "PA842141", "PA720386", "PA793094", "PA794058"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Juvin,  M.&#160;Bazin, Mme&#160;Blin, M.&#160;Le&#160;Fur, M.&#160;Breton, M.&#160;Brigand, M.&#160;Forissier, Mme&#160;Sylvie&#160;Bonnet, M.&#160;Hetzel, M.&#160;Marleix, Mme&#160;de&#160;Maistre, M.&#160;Di&#160;Filippo, M.&#160;Ray et Mme&#160;Gruet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1364.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;4, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;activit&#x00E9;&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&nbsp;&#x00AB;&nbsp;, inscrit sur une liste nationale de m&#x00E9;decins volontaires disponible dans chaque agence r&#x00E9;gionale de sant&#x00E9;,&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, r&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;9&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0; Pour les &#x00E9;ventuels besoins mat&#x00E9;riels et sociaux, et pour s&#x2019;assurer de l&#x2019;absence de situations d&#x2019;abus de faiblesse, oriente vers un assistant social qui figure sur une liste mise &#x00E0; disposition par l&#x2019;agence r&#x00E9;gionale de sant&#x00E9; ;&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">III&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, au d&#x00E9;but de l'alin&#x00E9;a&nbsp;11, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Propose &#x00E0; la personne de l&#x2019;orienter&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Oriente la personne&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">IV.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;13 par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, en particulier les modalit&#x00E9;s d&#x2019;administration et d&#x2019;action, conform&#x00E9;ment au V de l&#x2019;article L.&nbsp;1111&#x2011;12&#x2011;4 et les complications mentionn&#x00E9;es au III de l&#x2019;article L.&nbsp;1111&#x2011;12&#x2011;7&nbsp;;&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">V.&nbsp;&#x2013;&nbsp;Compl&#x00E9;ter cet article par l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;6&#x00B0;&nbsp;Propose &#x00E0; la personne de l&#x2019;orienter vers une association de pr&#x00E9;vention du suicide.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;objectif de cet amendement est de proposer une version plus pr&#x00E9;cise de la proc&#x00E9;dure d&#x2019;acc&#x00E8;s au&nbsp;droit &#x00E0; l&#x2019;aide &#x00E0; mourir, afin de limiter autant que possible d&#x2019;&#x00E9;ventuelles d&#x00E9;rives.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-20", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-05-20T22:37:09+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N002501?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29392", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0856P0D1N000581", "chronotag": "1757947675571", "legislature": "17", "identification": {"numeroLong": "CE581", "numeroOrdreDepot": "581", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0856P0D1", "texteLegislatifRef": "PIONANR5L17B0856", "triAmendement": "aoaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaavzt", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794710", "groupePolitiqueRef": "PO845514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA721270", "PA795820", "PA795868", "PA720838", "PA796106", "PA721896", "PA795998", "PA774962", "PA335612", "PA796010", "PA842271", "PA793174", "PA795240", "PA795876", "PA605745", "PA842299"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Brugerolles,  M.&#160;Peu, M.&#160;Nadeau, M.&#160;Castor, Mme&#160;Bourouaha, M.&#160;B&#233;nard, Mme&#160;Faucillon, Mme&#160;K/Bidi, Mme&#160;Lebon, M.&#160;Lecoq, M.&#160;Maillot, M.&#160;Maurel, M.&#160;Monnet, Mme&#160;Reid&#160;Arbelot, M.&#160;Rimane, M.&#160;Sansu et M.&#160;Tjibaou"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0856.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "49", "alineaDesignation": "Alin\u00e9a 49"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;49, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;conseillers comp&#x00E9;tents&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;conseillers certifi&#x00E9;s notamment pour leurs connaissances&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article 1er propose la mise en place d'un &#x00AB; conseil strat&#x00E9;gique global &#x00BB; dans le prolongement de plusieurs rapports parlementaires (rapports Potier-Travert et Potier-Descrozaille). Il consiste en l&#x2019;occurrence en un conseil malheureusement facultatif, dont le conseil strat&#x00E9;gique &#x00E0; l&#x2019;utilisation des produits pharmaceutiques ne constitue qu&#x2019;un volet. Si l&#x2019;Inrae a soulign&#x00E9; que la condition du succ&#x00E8;s de ce conseil n&#x2019;&#x00E9;tait pas tant sa gratuit&#x00E9; que sa qualit&#x00E9;, via des obligations pouss&#x00E9;es de formation des conseillers qui en sont charg&#x00E9;s, le S&#x00E9;nat a souhait&#x00E9; en s&#x00E9;ance publique supprimer l&#x2019;obligation de certification des conseillers pour lui substituer la notion de conseillers &#x00AB; comp&#x00E9;tents en agronomie, en protection des v&#x00E9;g&#x00E9;taux, en gestion &#x00E9;conome des ressources ou en strat&#x00E9;gie de valorisation et de fili&#x00E8;re &#x00BB;. Le pr&#x00E9;sent amendement propose de revenir a minima &#x00E0; l'exigence de disposer de conseillers certifi&#x00E9;s.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-14", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-14T00:01:06+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0856P0D1N000581?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "30656", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
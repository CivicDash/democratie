{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1364P0D1N001323", "chronotag": "1757949070602", "legislature": "17", "identification": {"numeroLong": "1323", "numeroOrdreDepot": "1323", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1364P0D1", "texteLegislatifRef": "PIONANR5L17BTC1364", "triAmendement": "laaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaavz", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA840809", "groupePolitiqueRef": "PO847173", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Verny"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 17", "articleDesignationCourte": "APR\u00c8S ART. 17", "articleDesignation": "APR\u00c8S L'ARTICLE 17, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1364.asp#D_Article_17"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Le chapitre V du titre I<sup>er</sup> du livre I<sup>er</sup> de la premi&#x00E8;re partie du code de la sant&#x00E9; publique, tel qu&#x2019;il r&#x00E9;sulte de l&#x2019;article&nbsp;17 de la pr&#x00E9;sente loi, est compl&#x00E9;t&#x00E9; par un article&nbsp;L.&nbsp;1115&#x2011;5 ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Art.&nbsp;L.&nbsp;1115&#x2011;5</i>.&nbsp;&#x2013;&nbsp;Est puni de deux ans d&#x2019;emprisonnement et de 30&nbsp;000&nbsp;euros d&#x2019;amende le fait, par quelque moyen que ce soit, de faire publiquement l&#x2019;apologie, la promotion ou l&#x2019;incitation &#x00E0; recourir &#x00E0; l&#x2019;aide &#x00E0; mourir, en dehors de l&#x2019;information strictement m&#x00E9;dicale, l&#x00E9;gale ou institutionnelle pr&#x00E9;vue par la loi.</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;La peine est port&#x00E9;e &#x00E0; trois ans d&#x2019;emprisonnement et 45&nbsp;000&nbsp;euros d&#x2019;amende lorsqu&#x2019;il s&#x2019;agit de mineurs ou de personnes vuln&#x00E9;rables.</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Cette interdiction s&#x2019;applique notamment &#x00E0; la publicit&#x00E9;, &#x00E0; la diffusion de contenu &#x00E0; des fins promotionnelles sur tout support, ainsi qu&#x2019;&#x00E0; l&#x2019;organisation d&#x2019;actions visant &#x00E0; influencer le recours &#x00E0; l&#x2019;aide &#x00E0; mourir.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement cr&#x00E9;e un d&#x00E9;lit autonome d&#x2019;incitation ou de promotion de l&#x2019;aide &#x00E0; mourir, afin de pr&#x00E9;server le caract&#x00E8;re exceptionnel, encadr&#x00E9; et non id&#x00E9;ologis&#x00E9; de ce dispositif.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-23", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "RT", "libelle": "Retir\u00e9"}, "sousEtat": {"code": "RT", "libelle": "Retir\u00e9 apr\u00e8s publication"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N001323?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c sant\u00e9 publique", "divisionCodeLoi": "L1115-5"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1907P0D1N001730", "chronotag": "1761944413366", "legislature": "17", "identification": {"numeroLong": "AS1730", "numeroOrdreDepot": "1730", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "obaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720386", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA722390", "PA721816", "PA642725", "PA721442", "PA793094"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Di&#160;Filippo,  Mme&#160;Corneloup, M.&#160;Boucard, M.&#160;Li&#233;geon, Mme&#160;Petex et M.&#160;Ray"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 45 bis", "articleDesignationCourte": "APR\u00c8S ART. 45 BIS", "articleDesignation": "APR\u00c8S L'ARTICLE 45 BIS, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_45_bis"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Le neuvi&#x00E8;me alin&#x00E9;a de l&#x2019;article&nbsp;L.&nbsp;221&#x2011;2-4 du code de l&#x2019;action sociale et des familles est ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Si la personne se pr&#x00E9;sentant comme mineure et priv&#x00E9;e temporairement ou d&#x00E9;finitivement de la protection de sa famille s&#x2019;oppose au recueil de ses empreintes, elle est consid&#x00E9;r&#x00E9;e comme majeure et ne peut b&#x00E9;n&#x00E9;ficier des dispositions pr&#x00E9;vues pour les mineurs non accompagn&#x00E9;s en mati&#x00E8;re d&#x2019;acc&#x00E8;s &#x00E0; l&#x2019;Assurance maladie et &#x00E0; la compl&#x00E9;mentaire sant&#x00E9; solidaire.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Les mineurs non accompagn&#x00E9;s ont le droit de b&#x00E9;n&#x00E9;ficier de l&#x2019;Assurance maladie et de la Compl&#x00E9;mentaire sant&#x00E9; solidaire (CSS) sans participation financi&#x00E8;re et sans d&#x00E9;lai. Dans un contexte extr&#x00EA;mement tendu pour nos finances publiques et alors que certains Fran&#x00E7;ais renoncent parfois &#x00E0; se soigner pour des raisons financi&#x00E8;res, cet amendement vise &#x00E0; emp&#x00EA;cher que des personnes majeures en situation irr&#x00E9;guli&#x00E8;re ne puissent b&#x00E9;n&#x00E9;ficier des dispositions pr&#x00E9;vues pour ces mineurs en termes d&#x2019;acc&#x00E8;s aux soins.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-24", "datePublication": "2025-10-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-10-31T21:59:50+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1907P0D1N001730?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c action sociale", "divisionCodeLoi": "L221-2-4"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
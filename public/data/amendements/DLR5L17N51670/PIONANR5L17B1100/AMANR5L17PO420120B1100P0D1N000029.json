"corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>R&#x00E9;diger ainsi cet article :</p><p>&#x00AB; L&#x2019;assistance au suicide est l&#x2019;acte accompli dans l&#x2019;intention de permettre &#x00E0; une personne capable de discernement de mettre fin &#x00E0; ses jours, apr&#x00E8;s la prescription de m&#x00E9;dicaments par un m&#x00E9;decin &#x00E0; des fins de suicide. &#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cette r&#x00E9;daction inspir&#x00E9;e de la d&#x00E9;finition du suicide assist&#x00E9; par l&#x2019;Acad&#x00E9;mie des sciences m&#x00E9;dicales Suisse vise &#x00E0; rendre intelligible l&#x2019;objectif poursuivi par cette proposition de loi.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-01", "datePublication": "2025-04-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-11T15:09:03+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N000029?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "a
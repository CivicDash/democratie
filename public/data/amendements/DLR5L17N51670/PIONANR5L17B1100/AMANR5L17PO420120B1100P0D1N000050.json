{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1100P0D1N000050", "chronotag": "1757947875079", "legislature": "17", "identification": {"numeroLong": "AS50", "numeroOrdreDepot": "50", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1100P0D1", "texteLegislatifRef": "PIONANR5L17B1100", "triAmendement": "beaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaui", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA608416", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA346782", "PA722390", "PA720386", "PA330008", "PA840979", "PA793508", "PA606098", "PA774954", "PA266797", "PA794058"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Hetzel,  M.&#160;Juvin, Mme&#160;Corneloup, M.&#160;Di&#160;Filippo, M.&#160;Breton, M.&#160;Le&#160;Fur, M.&#160;Brigand, M.&#160;Marleix, Mme&#160;Blin, M.&#160;Gosselin et Mme&#160;Gruet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 4", "articleDesignationCourte": "ART. 4", "articleDesignation": "ARTICLE 4", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1100.asp#D_Article_4"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "9", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter cet article par l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;6&#x00B0;&nbsp;Ne pas faire l&#x2019;objet d&#x2019;une mesure de protection juridique.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; prot&#x00E9;ger les personnes les plus vuln&#x00E9;rables. Dans un rapport, le Professeur Sicard exprimait les inqui&#x00E9;tudes suivantes&nbsp;: &#x00AB;&nbsp;La pratique euthanasique d&#x00E9;veloppe sa propre dynamique r&#x00E9;sistant &#x00E0; tout contr&#x00F4;le efficace et tend n&#x00E9;cessairement &#x00E0; s&#x2019;&#x00E9;largir, avec un curseur qualitatif sans cesse mouvant qui ne revient jamais en arri&#x00E8;re. Elle int&#x00E9;riorise des repr&#x00E9;sentations soci&#x00E9;tales n&#x00E9;gatives d&#x2019;un certain nombre de situations de vieillesse, de maladie et de handicap. Les contraintes &#x00E9;conomiques qui vont dans tous les cas augmenter peuvent susciter un sentiment de culpabilit&#x00E9; chez les personnes en perte d&#x2019;autonomie pouvant les conduire &#x00E0; formuler une demande d&#x2019;euthanasie.&nbsp;&#x00BB;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-01", "datePublication": "2025-04-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-28T19:56:38+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1100P0D1N000050?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "30064", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
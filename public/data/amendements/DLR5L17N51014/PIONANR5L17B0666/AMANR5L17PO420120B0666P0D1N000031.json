{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0666P0D1N000031", "chronotag": "1743001317464", "legislature": "17", "identification": {"numeroLong": "AS31", "numeroOrdreDepot": "31", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0666P0D1", "texteLegislatifRef": "PIONANR5L17B0666", "triAmendement": "gaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaad", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA267780", "groupePolitiqueRef": "PO845470", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Firmin&#160;Le&#160;Bodo, rapporteure"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0666.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Substituer &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;1 les cinq alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le chapitre VI du titre III du livre VI de la troisi&#x00E8;me partie du code de l&#x2019;&#x00E9;ducation est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;Est ins&#x00E9;r&#x00E9;e une section 1 intitul&#x00E9;e&nbsp;: &#x00AB;&nbsp;Dispositions g&#x00E9;n&#x00E9;rales&nbsp;&#x00BB; et comprenant l&#x2019;article L.&nbsp;636&#x2011;1&nbsp;; </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;Apr&#x00E8;s la section 1 telle qu&#x2019;elle r&#x00E9;sulte du 1&#x00B0;&nbsp;du pr&#x00E9;sent article, est ins&#x00E9;r&#x00E9;e une section 2 ainsi r&#x00E9;dig&#x00E9;e&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Section 2</i> </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Contrat d&#x2019;engagement de service public&nbsp;&#x00BB;. </p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, au d&#x00E9;but de l&#x2019;alin&#x00E9;a&nbsp;2, ajouter la r&#x00E9;f&#x00E9;rence&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Art.&nbsp;L.&nbsp;636&#x2011;2</i>.&nbsp;&#x2013;&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">III&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;3, substituer au mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;deuxi&#x00E8;me&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;premier&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">IV.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, proc&#x00E9;der &#x00E0; la m&#x00EA;me substitution &#x00E0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;4.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Amendement r&#x00E9;dactionnel, visant &#x00E0; modifier l&#x2019;insertion dans le code de l&#x2019;&#x00E9;ducation de l&#x2019;article relatif au contrat d&#x2019;engagement de service public (CESP) des orthophonistes, de fa&#x00E7;on &#x00E0; le faire figurer dans une section d&#x00E9;di&#x00E9;e, laquelle pourrait ult&#x00E9;rieurement &#x00EA;tre &#x00E9;tendue &#x00E0; d&#x2019;autres professions de sant&#x00E9;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-25", "datePublication": "2025-03-26", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-26T16:01:55+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0666P0D1N000031?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
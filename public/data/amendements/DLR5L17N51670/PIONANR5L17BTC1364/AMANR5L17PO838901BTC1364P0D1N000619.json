{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1364P0D1N000619", "chronotag": "1757948845915", "legislature": "17", "identification": {"numeroLong": "619", "numeroOrdreDepot": "619", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1364P0D1", "texteLegislatifRef": "PIONANR5L17BTC1364", "triAmendement": "ioaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaakf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794762", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA721932", "PA795552", "PA841837", "PA841981", "PA642988", "PA793656", "PA840837", "PA841605", "PA793608", "PA840135", "PA720668", "PA794030", "PA796034", "PA841451", "PA794886", "PA793616", "PA841553", "PA795900", "PA841737", "PA794770", "PA841621", "PA840869", "PA794354", "PA793852", "PA796050", "PA794678", "PA795966", "PA840801", "PA796118", "PA842279", "PA793354", "PA796026", "PA841131", "PA793548", "PA793102", "PA841575"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Dogor-Such,  Mme&#160;Lorho, M.&#160;Odoul, M.&#160;Le&#160;Bourgeois, Mme&#160;Rimbert, M.&#160;de&#160;L&#233;pinau, Mme&#160;Pollet, M.&#160;Markowsky, M.&#160;Golliot, Mme&#160;Levavasseur, M.&#160;Dufosset, M.&#160;Blairy, M.&#160;Jolly, M.&#160;Giletti, M.&#160;Guibert, Mme&#160;Blanc, M.&#160;Mauvieux, M.&#160;Tesson, Mme&#160;Hamelet, M.&#160;Gery, Mme&#160;Martinez, M.&#160;Evrard, Mme&#160;Joubert, Mme&#160;Laporte, M.&#160;Meurin, M.&#160;Schreck, M.&#160;Frapp&#233;, M.&#160;Rambaud, M.&#160;Fouquart, Mme&#160;Auzanot, Mme&#160;Bamana, Mme&#160;M&#233;lin, Mme&#160;Lechanteux, Mme&#160;Bouquin, Mme&#160;Colombier, M.&#160;Christian&#160;Girard et M.&#160;Vos"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1364.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;4, apr&#x00E8;s la r&#x00E9;f&#x00E9;rence&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L.&nbsp;1111&#x2011;12&#x2011;4&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ou&nbsp;exer&#x00E7;ant dans les&nbsp;pharmacies &#x00E0; usage int&#x00E9;rieur mentionn&#x00E9;es au premier alin&#x00E9;a de l&#x2019;article&nbsp;L.&nbsp;111&#x2011;12&#x2011;6&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cette proposition de loi pr&#x00E9;voit l&#x2019;instauration d&#x2019;une aide &#x00E0; mourir pour les personnes qui en expriment la demande. Entre la prise de d&#x00E9;cision et sa mise en oeuvre, des pharmaciens interviennent en r&#x00E9;alisant la pr&#x00E9;paration l&#x00E9;tale et en d&#x00E9;livrant cette substance en officine ou au sein d&#x2019;un &#x00E9;tablissement de sant&#x00E9;. Pourtant, d&#x2019;apr&#x00E8;s le code de sant&#x00E9; publique, le &#x00AB;&nbsp;pharmacien exerce sa mission dans le respect de la vie et de la personne humaine&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">Certes, une clause de conscience est pr&#x00E9;vue pour les professionnels de sant&#x00E9; qui ne souhaiteraient pas participer &#x00E0; la proc&#x00E9;dure d&#x2019;aide &#x00E0; mourir, mais les pharmaciens sont exclus de cette disposition. Or, le pharmacien n&#x2019;est pas un simple ex&#x00E9;cutant. Il dispose d&#x2019;une conscience au m&#x00EA;me titre que les autres professionnels de sant&#x00E9;. Cet amendement vise &#x00E0; leur accorder cette clause de conscience.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-07", "datePublication": "2025-05-23", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-23T19:36:27+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N000619?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29399", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "30990", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "78868", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
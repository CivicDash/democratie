{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1364P0D1N002517", "chronotag": "1757948423122", "legislature": "17", "identification": {"numeroLong": "2517", "numeroOrdreDepot": "2517", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1364P0D1", "texteLegislatifRef": "PIONANR5L17BTC1364", "triAmendement": "blaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafozys", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA346782", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642847", "PA774954", "PA330008", "PA793508", "PA1327", "PA719968", "PA608416", "PA606098", "PA793094", "PA794058"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Juvin,  M.&#160;Bazin, Mme&#160;Blin, M.&#160;Breton, M.&#160;Brigand, M.&#160;Forissier, Mme&#160;Sylvie&#160;Bonnet, M.&#160;Hetzel, M.&#160;Marleix, M.&#160;Ray et Mme&#160;Gruet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1364.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "11", "alineaDesignation": "Alin\u00e9a 11"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;11&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;3&#x00B0;&nbsp;Oriente la personne vers un psychologue clinicien ou un psychiatre qui s&#x2019;assure que le demandeur ne pr&#x00E9;sente pas de pathologie ou d&#x2019;&#x00E9;tat affectant son jugement&nbsp;;&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter cet article par l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;II.&nbsp;&#x2013;&nbsp;L&#x2019;article&nbsp;18 de la pr&#x00E9;sente loi n&#x2019;est pas applicable pas aux consultations mentionn&#x00E9;es au&nbsp;3&#x00B0;&nbsp;du&nbsp;II de l&#x2019;article&nbsp;L.&nbsp;1111&#x2011;12&#x2011;3.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;objectif de cet amendement est de pr&#x00E9;ciser la proc&#x00E9;dure de demande d&#x2019;aide &#x00E0; mourir.</p><p style=\"text-align: justify;\">En effet, le recours &#x00E0; une aide &#x00E0; mourir n&#x2019;est pas anodin puisqu&#x2019;il s&#x2019;agit de se faire injecter un produit l&#x00E9;tal pour mourir. Il est indispensable que la personne souhaitant recourir &#x00E0; une aide &#x00E0; mourir soit orient&#x00E9; vers un sp&#x00E9;cialiste de la sant&#x00E9; mentale qui pourra l&#x2019;&#x00E9;couter et l&#x2019;accompagner.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-21", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-21T18:28:48+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N002517?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29393", "article99": "false", "loiReference": {"codeLoi": "0.1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "30928", "typePosition": "Dernier"}, "discussionIdentique": {"idDiscussion": "78722", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
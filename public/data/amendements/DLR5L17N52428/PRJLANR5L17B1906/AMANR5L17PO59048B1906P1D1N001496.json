{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B1906P1D1N001496", "chronotag": "1761175453490", "legislature": "17", "identification": {"numeroLong": "I-CF1496", "numeroOrdreDepot": "1496", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B1906P1D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "pcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaan", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720992", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA643143", "PA842211", "PA795762", "PA643157"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;William,  M.&#160;Baptiste, Mme&#160;Bellay, M.&#160;Califer et M.&#160;Naillet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ARTICLE 46", "articleDesignationCourte": "ART. 46", "articleDesignation": "ARTICLE 46", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906A.asp#D_ARTICLE_46"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet article pr&#x00E9;voit la mise &#x00E0; la charge de tout ou partie des frais d&#x2019;enqu&#x00EA;tes &#x00E0; la charge de la personne physique majeure ou de la personne morale condamn&#x00E9;e. Une telle mesure aurait pour effet de faire peser sur les personnes d&#x00E9;tenues ou condamn&#x00E9;es une &#x00AB;&nbsp;dette p&#x00E9;nale suppl&#x00E9;mentaire&nbsp;&#x00BB;, susceptible de limiter leur capacit&#x00E9; &#x00E0; indemniser les victimes et d&#x2019;aggraver les in&#x00E9;galit&#x00E9;s &#x00E9;conomiques.</p><p style=\"text-align: justify;\">Cette mesure compromet directement la r&#x00E9;insertion sociale, professionnelle et personnelle des personnes d&#x00E9;tenues. Les auteurs de cet amendement souhaitent supprimer cette disposition, qui introduit une logique de p&#x00E9;nalisation financi&#x00E8;re, incompatible avec les principes de notre proc&#x00E9;dure p&#x00E9;nale.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-17", "datePublication": "2025-10-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-10-23T01:23:47+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P1D1N001496?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "81429", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
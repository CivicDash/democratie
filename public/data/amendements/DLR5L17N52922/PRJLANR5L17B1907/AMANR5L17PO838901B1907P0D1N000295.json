articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_8"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Les 1&#x00B0;, 2&#x00B0; et 6&#x00B0; du&nbsp;II de l&#x2019;article&nbsp;L.&nbsp;242&#x2011;1 du code de la s&#x00E9;curit&#x00E9; sociale sont abrog&#x00E9;s.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Dans son avis rendu le 27 octobre dernier, le Conseil des pr&#x00E9;l&#x00E8;vements obligatoires a conclu que les \"exemptions d'assiette de contributions et de cotisations sociales non compens&#x00E9;es nuisent au financement de la s&#x00E9;curit&#x00E9; sociale. Leur efficacit&#x00E9; pour remplir les objectifs de politique publique qui leur ont &#x00E9;t&#x00E9; assign&#x00E9;s n'est souvent pas &#x00E9;valu&#x00E9;e; quand elle l'est, ces &#x00E9;valuations sont rarement positives\". Sur cette base, les auteurs de cet amendement proposent d'inclure l'int&#x00E9;ressement, la participation et les actions gratuites dans l'assiette des cotisations sociales.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-28", "datePublication": "2025-11-07", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-11-06T23:30:29+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B1907P0D1N000295?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2026IDS29876", "article99": "false", "loiReference": {"codeLoi": "c s\u00e9curit\u00e9 sociale", "divisionCodeLoi": "L242-1"}, "discussionCommune": {"idDiscussion": "34013", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
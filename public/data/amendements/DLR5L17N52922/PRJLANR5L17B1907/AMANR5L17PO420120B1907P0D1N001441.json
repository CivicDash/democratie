{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1907P0D1N001441", "chronotag": "1761907333277", "legislature": "17", "identification": {"numeroLong": "AS1441", "numeroOrdreDepot": "1441", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "ifaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaao", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720386", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA722390", "PA642725", "PA721442", "PA793094", "PA721816"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Di&#160;Filippo,  Mme&#160;Corneloup, M.&#160;Li&#233;geon, Mme&#160;Petex, M.&#160;Ray et M.&#160;Boucard"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 31", "articleDesignationCourte": "APR\u00c8S ART. 31", "articleDesignation": "APR\u00c8S L'ARTICLE 31, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_31"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;L.&nbsp;425&#x2011;9 du code de l&#x2019;entr&#x00E9;e et du s&#x00E9;jour des &#x00E9;trangers et du droit d&#x2019;asile est abrog&#x00E9;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le titre de s&#x00E9;jour pour soins, un dispositif pens&#x00E9; pour des cas exceptionnels d&#x2019;&#x00E9;trangers malades, est l&#x2019;objet de d&#x00E9;rives (20&nbsp;600 b&#x00E9;n&#x00E9;ficiaires en 2022), et est utilis&#x00E9; par de nombreux &#x00E9;trangers non-r&#x00E9;sidents en France pour profiter de soins on&#x00E9;reux et non-vitaux, pris en charge par l&#x2019;argent public, ou pour venir en France et obtenir une r&#x00E9;gularisation. Certains juges administratifs ont en effet &#x00E9;tendu son champ d&#x2019;application, et ce qui devait &#x00EA;tre contenu au pronostic vital s&#x2019;est &#x00E9;largi &#x00E0; l&#x2019;absence d&#x2019;acc&#x00E8;s aux soins dans le pays d&#x2019;origine. Une femme de Djibouti a ainsi eu l&#x2019;autorisation de venir en France pour pratiquer une quatri&#x00E8;me PMA (Procr&#x00E9;ation m&#x00E9;dicale assist&#x00E9;e) pour son sixi&#x00E8;me enfant. Des Japonais et des Am&#x00E9;ricains sont &#x00E9;galement venus en France pour profiter de soins gratuits contre plusieurs dizaines de milliers d&#x2019;euros dans leur pays. Ce titre de s&#x00E9;jour doit donc &#x00EA;tre supprim&#x00E9;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-20", "datePublication": "2025-10-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-31T11:41:44+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1907P0D1N001441?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "CESEDA", "divisionCodeLoi": "L425-9"}, "discussionCommune": {"idDiscussion": "33583", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "8", "alineaDesignation": "Alin\u00e9a 8"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;8, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;peut&nbsp;saisir&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;saisit&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;obligation de saisir la chambre disciplinaire en cas de manquement dans le cadre d&#x2019;une aide &#x00E0; mourir r&#x00E9;pond &#x00E0; des imp&#x00E9;ratifs &#x00E9;thiques, juridiques et de transparence publique. Elle garantit un contr&#x00F4;le strict d&#x2019;une proc&#x00E9;dure particuli&#x00E8;rement sensible et assure une &#x00E9;galit&#x00E9; de traitement, pr&#x00E9;venant ainsi tout risque d&#x2019;arbitraire.</p><p style=\"text-align: justify;\">En outre, cette saisine contribue &#x00E0; la protection des patients et &#x00E0; la pr&#x00E9;vention des abus. Pour ces raisons, il est indispensable qu&#x2019;elle soit syst&#x00E9;matique d&#x00E8;s lors que la commission rel&#x00E8;ve des faits susceptibles de contrevenir aux crit&#x00E8;res r&#x00E9;gissant l&#x2019;injection l&#x00E9;tale.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-05", "datePublication": "2025-05-23", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-05-23T23:34:03+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N000074?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29400", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "31020", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "78238", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
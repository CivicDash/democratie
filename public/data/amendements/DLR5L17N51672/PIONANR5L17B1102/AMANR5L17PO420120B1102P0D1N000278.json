{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B1102P0D1N000278", "chronotag": "1744270167438", "legislature": "17", "identification": {"numeroLong": "AS278", "numeroOrdreDepot": "278", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B1102P0D1", "texteLegislatifRef": "PIONANR5L17B1102", "triAmendement": "doaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794762", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794434", "PA841645", "PA840075", "PA842073", "PA840143", "PA794678", "PA795900", "PA793672", "PA721932", "PA793354", "PA794322", "PA793696", "PA795552", "PA793656", "PA793230"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Dogor-Such,  M.&#160;Bentz, M.&#160;Bernhardt, M.&#160;Casterman, M.&#160;Dussausaye, M.&#160;Florquin, M.&#160;Frapp&#233;, Mme&#160;Hamelet, Mme&#160;Loir, Mme&#160;Lorho, Mme&#160;M&#233;lin, M.&#160;M&#233;nag&#233;, M.&#160;Muller, M.&#160;Odoul, Mme&#160;Pollet et Mme&#160;Ranc"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 8", "articleDesignationCourte": "ART. 8", "articleDesignation": "ARTICLE 8", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1102.asp#D_Article_8"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;formation&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">r&#x00E9;diger ainsi la fin de l&#x2019;alin&#x00E9;a&nbsp;1&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;aux soins palliatifs d&#x00E9;finis &#x00E0; l&#x2019;article L 1110&#x2011;10 du code de la sant&#x00E9; publique et &#x00E0; l&#x2019;accompagnement de la fin de vie.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;1, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;I <i>bis</i>.&nbsp;Au troisi&#x00E8;me alin&#x00E9;a de l&#x2019;article L.&nbsp;632&#x2011;1 du code de l&#x2019;&#x00E9;ducation, les mots&nbsp;: &#x00AB;&nbsp;code de la sant&#x00E9; publique&nbsp;&#x00BB; sont remplac&#x00E9;s par les mots&nbsp;: &#x00AB;&nbsp;m&#x00EA;me code&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Il s&#x2019;agit de revaloriser les termes soins palliatifs qui se trouvent d&#x00E9;class&#x00E9;s par cette proposition de loi, et dans cet article, qui indique seulement &#x00AB;&nbsp;approche palliative&nbsp;&#x00BB;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-03", "datePublication": "2025-04-08", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-10T09:29:16+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B1102P0D1N000278?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "29816", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
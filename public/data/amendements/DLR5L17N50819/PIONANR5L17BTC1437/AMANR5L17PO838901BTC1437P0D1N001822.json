 {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1437.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;tablir le 1&#x00B0; de l&#x2019;alin&#x00E9;a&nbsp;2 dans la r&#x00E9;daction suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0; Les articles L.&nbsp;253&#x2011;5&#x2011;1 et L.&nbsp;253&#x2011;5&#x2011;2 sont abrog&#x00E9;s&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Droite R&#x00E9;publicaine vise &#x00E0; autoriser &#x00E0; nouveau les remises, rabais, ristournes, &#x00E0; l'occasion de la vente de produits phytopharmaceutiques</p><p style=\"text-align: justify;\">L&#x2019;interdiction a eu pour effet de rigidifier le march&#x00E9;, de limiter la capacit&#x00E9; de n&#x00E9;gociation des exploitants agricoles et de rench&#x00E9;rir le co&#x00FB;t des intrants. Elle a &#x00E9;galement restreint la concurrence entre fournisseurs, au d&#x00E9;triment du pouvoir d&#x2019;achat des agriculteurs et de la comp&#x00E9;titivit&#x00E9; des fili&#x00E8;res agricoles fran&#x00E7;aises.</p><p style=\"text-align: justify;\">En r&#x00E9;tablissant ces pratiques commerciales, l&#x2019;amendement vise &#x00E0; redonner de la souplesse au march&#x00E9;, &#x00E0; permettre aux agriculteurs de b&#x00E9;n&#x00E9;ficier de meilleures conditions d&#x2019;achat.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-22", "datePublication": "2025-05-26", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1437P0D1N001822?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "31246", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "79200", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
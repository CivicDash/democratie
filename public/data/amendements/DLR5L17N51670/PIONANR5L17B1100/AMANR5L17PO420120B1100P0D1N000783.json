rmatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;6,&nbsp;supprimer&nbsp;les&nbsp;mots&nbsp;:&nbsp;</p><p>&#x00AB;&nbsp;lorsqu&#x2019;elle n&#x2019;est pas en mesure physiquement d&#x2019;y proc&#x00E9;der&nbsp;&#x00BB;.</p><p>II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence,&nbsp;compl&#x00E9;ter cet article par l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p>&#x00AB;&nbsp;II.&nbsp;&#x2013;&nbsp;Quand la personne se fait administrer la substance l&#x00E9;tale par un m&#x00E9;decin, un infirmier ou une personne majeure qu&#x2019;elle d&#x00E9;signe et qui se manifeste pour le faire, l&#x2019;article&nbsp;18 de&nbsp;la pr&#x00E9;sente loi ne s&#x2019;applique pas.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Le pr&#x00E9;sent amendement propos&#x00E9; par l&#x2019;ADMD a pour but d&#x2019;&#x00E9;tablir la libert&#x00E9; pour le demandeur de choisir entre l&#x2019;auto-administration du produit l&#x00E9;tal et l&#x2019;administration par un tiers, sans justification.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-05", "datePublication": "2025-04-11", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-04-11T17:48:54+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/
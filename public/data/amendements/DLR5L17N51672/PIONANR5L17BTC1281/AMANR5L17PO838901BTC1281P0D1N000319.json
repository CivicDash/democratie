{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1281P0D1N000319", "chronotag": "1757948069092", "legislature": "17", "identification": {"numeroLong": "319", "numeroOrdreDepot": "319", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1281P0D1", "texteLegislatifRef": "PIONANR5L17BTC1281", "triAmendement": "boaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaahtx", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA721410", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA840979", "PA794750", "PA722390", "PA794994", "PA1327", "PA606098", "PA608826"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Bonnivard,  M.&#160;Le&#160;Fur, M.&#160;Liger, Mme&#160;Corneloup, M.&#160;Portier, M.&#160;Forissier, M.&#160;Marleix et Mme&#160;Duby-Muller"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 7", "articleDesignationCourte": "ART. 7", "articleDesignation": "ARTICLE 7", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1281.asp#D_Article_7"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "2", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l'alin&#x00E9;a 2, ins&#x00E9;rer&nbsp;l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le budget annuel d&#x00E9;di&#x00E9;, de 2025 &#x00E0; 2033, est augment&#x00E9; d&#x2019;un million d&#x2019;euros&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Un 12e poste &#x00E0; financer ayant &#x00E9;t&#x00E9; ajout&#x00E9; en commission des Affaires sociales (alin&#x00E9;a 15),&nbsp;l&#x2019;objectif de cet amendement est que la &#x00AB; structuration d&#x2019;une fili&#x00E8;re universitaire d&#x00E9;di&#x00E9;e aux&nbsp;soins palliatifs et d&#x2019;accompagnement et la cr&#x00E9;ation d&#x2019;un dipl&#x00F4;me d&#x2019;&#x00E9;tudes sp&#x00E9;cialis&#x00E9;es en&nbsp;m&#x00E9;decine palliative et en soins palliatifs et d&#x2019;accompagnement &#x00BB; soient effectives.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-06", "datePublication": "2025-05-14", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-05-14T19:01:05+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1281P0D1N000319?format=pdf"}, "dateDispoRepresentation": {"@xmlns:x
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B1906P2D1N001433", "chronotag": "1762456363990", "legislature": "17", "identification": {"numeroLong": "II-CF1433", "numeroOrdreDepot": "1433", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B1906P2D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "auaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabv", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719372", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA795920", "PA721852", "PA717379", "PA795176", "PA795528", "PA817203"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Kasbarian,  Mme&#160;Thevenot, Mme&#160;Lebec, M.&#160;Maillard, M.&#160;Metzdorf, M.&#160;Rodwell et Mme&#160;Miller"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ARTICLE 67", "articleDesignationCourte": "ART. 67", "articleDesignation": "ARTICLE 67", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906C.asp#D_ARTICLE_67"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "VA", "libelleMission": "Coh\u00e9sion des territoires", "missionRef": "3319"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;5&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;3&#x00B0; Les personnes de nationalit&#x00E9; &#x00E9;trang&#x00E8;re non ressortissantes d&#x2019;un &#x00C9;tat membre de l&#x2019;Union europ&#x00E9;enne, d&#x2019;un autre &#x00C9;tat partie &#x00E0; l&#x2019;accord sur l&#x2019;Espace &#x00E9;conomique europ&#x00E9;en ou de la Conf&#x00E9;d&#x00E9;ration suisse, justifiant d&#x2019;au moins cinq ann&#x00E9;es d&#x2019;affiliation &#x00E0; un r&#x00E9;gime de s&#x00E9;curit&#x00E9; sociale fran&#x00E7;ais au titre d&#x2019;une activit&#x00E9; professionnelle exerc&#x00E9;e en France.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; red&#x00E9;finir les conditions d&#x2019;acc&#x00E8;s aux aides personnelles au logement pour les ressortissants &#x00E9;trangers.</p><p style=\"text-align: justify;\">Il distingue&nbsp;: Les ressortissants fran&#x00E7;ais, qui demeurent &#x00E9;ligibles sans condition nouvelle&nbsp;;les ressortissants europ&#x00E9;ens (Union europ&#x00E9;enne, Espace &#x00E9;conomique europ&#x00E9;en et Conf&#x00E9;d&#x00E9;ration suisse), pour lesquels les conditions actuelles de l&#x2019;article&nbsp;L.&nbsp;512&#x2011;2 du code de la s&#x00E9;curit&#x00E9; sociale sont maintenues&nbsp;; et les ressortissants extra-communautaires, pour lesquels une condition suppl&#x00E9;mentaire d&#x2019;au moins cinq ann&#x00E9;es d&#x2019;affiliation &#x00E0; un r&#x00E9;gime de s&#x00E9;curit&#x00E9; sociale fran&#x00E7;ais est instaur&#x00E9;e, au titre d&#x2019;une activit&#x00E9; professionnelle exerc&#x00E9;e en France.</p><p style=\"text-align: justify;\">&nbsp;Cette &#x00E9;volution vise &#x00E0; r&#x00E9;server les aides personnelles au logement aux personnes durablement ins&#x00E9;r&#x00E9;es dans l&#x2019;&#x00E9;conomie nationale et contribuant effectivement au financement du syst&#x00E8;me social, tout en respectant les engagements europ&#x00E9;ens de la France.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-30", "datePublication": "2025-11-05", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-11-06T19:17:31+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P2D1N001433?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "33783", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
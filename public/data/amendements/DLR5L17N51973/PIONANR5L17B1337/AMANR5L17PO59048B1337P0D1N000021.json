{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B1337P0D1N000021", "chronotag": "1757948401661", "legislature": "17", "identification": {"numeroLong": "CF21", "numeroOrdreDepot": "21", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B1337P0D1", "texteLegislatifRef": "PIONANR5L17B1337", "triAmendement": "caaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaab", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795664", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Midy, rapporteur"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1337.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Avant", "numero": "1", "alineaDesignation": "Avant l'alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Avant l&#x2019;alin&#x00E9;a&nbsp;1, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;IA.&nbsp;&#x2013;&nbsp;Au deuxi&#x00E8;me alin&#x00E9;a du&nbsp;1 de l&#x2019;article&nbsp;231 du code g&#x00E9;n&#x00E9;ral des imp&#x00F4;ts, apr&#x00E8;s les mots&nbsp;: &#x00AB;&nbsp;aux&nbsp;I&nbsp;&#x00BB;, est ins&#x00E9;r&#x00E9;e la r&#x00E9;f&#x00E9;rence&nbsp;: &#x00AB;&nbsp;I&nbsp;<i>bis</i>&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter cet article par les trois alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;II.&nbsp;&#x2013;&nbsp;L&#x2019;article&nbsp;32 de la loi n&#x00B0; 2025&#x2011;127 du&nbsp;14&nbsp;f&#x00E9;vrier&nbsp;2025 de finances pour 2025 est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;Le 1&#x00B0; du&nbsp;I est supprim&#x00E9;&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;Au premier alin&#x00E9;a du&nbsp;II, les mots&nbsp;: &#x00AB;&nbsp;,&nbsp;&#x00E0; l&#x2019;exception du&nbsp;1&#x00B0; qui entre en vigueur le 1<sup>er</sup>&nbsp;janvier&nbsp;2026&nbsp;&#x00BB; sont supprim&#x00E9;s.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Amendement de coordination.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-26", "datePublication": "2025-05-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-05-28T10:40:12+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true",
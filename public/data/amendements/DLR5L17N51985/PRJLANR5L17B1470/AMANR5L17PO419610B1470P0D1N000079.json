{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B1470P0D1N000079", "chronotag": "1757949981669", "legislature": "17", "identification": {"numeroLong": "CE79", "numeroOrdreDepot": "79", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B1470P0D1", "texteLegislatifRef": "PRJLANR5L17B1470", "triAmendement": "fcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaajc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA2940", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA840997", "PA793980", "PA794146", "PA722142", "PA841713"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Voynet,  M.&#160;Biteau, M.&#160;Fournier, Mme&#160;Laernoes, M.&#160;Ruffin et M.&#160;Tavernier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 19", "articleDesignationCourte": "ART. 19", "articleDesignation": "ARTICLE 19", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1470.asp#D_Article_19"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;3, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;assainissement,&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">inse&#x0301;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;des constructions, ouvrages et installations de retenues collinaires,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement &#x00E9;tend le champ du r&#x00E9;gime de prise de possession anticip&#x00E9;e &#x00E0; la construction de retenues collinaires. Les retenues collinaires Combani et Dzoumogn&#x00E9;, situ&#x00E9;es dans le nord du d&#x00E9;partement, ne permettent pas de combler les besoins de la population en saison s&#x00E8;che. La finalisation de l&#x2019;acquisition des terrains n&#x00E9;cessaires &#x00E0; la r&#x00E9;alisation d&#x2019;un troisi&#x00E8;me ouvrage strat&#x00E9;gique permettra de r&#x00E9;pondre plus efficacement &#x00E0; la pr&#x00E9;carit&#x00E9; hydrique qui touche le territoire mahorais. La s&#x00E9;curisation d&#x2019;une retenue collinaire de l&#x2019;Ourov&#x00E9;ni, d&#x00E9;j&#x00E0; annonc&#x00E9;e depuis une dizaine d'ann&#x00E9;es, permettra d&#x2019;augmenter le potentiel de stockage des &#x00E9;coulements d'eau en produisant 10.000 m&#x00E8;tres cubes d'eau suppl&#x00E9;mentaires par jour.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-05", "datePublication": "2025-06-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-06-11T10:39:18+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B1470P0D1N000079?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
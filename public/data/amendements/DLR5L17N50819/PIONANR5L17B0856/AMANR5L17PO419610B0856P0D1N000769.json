{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0856P0D1N000769", "chronotag": "1757947551797", "legislature": "17", "identification": {"numeroLong": "CE769", "numeroOrdreDepot": "769", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0856P0D1", "texteLegislatifRef": "PIONANR5L17B0856", "triAmendement": "joaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaasii", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793816", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA607553", "PA342415", "PA841315", "PA794854", "PA840765", "PA643157", "PA840673", "PA643192", "PA840829", "PA841295", "PA841633"]}, "suffixe": "et les membres du groupe Socialistes et apparent\u00e9s", "libelle": "Mme&#160;Thomin,  M.&#160;Potier, Mme&#160;Battistel, M.&#160;Benbrahim, M.&#160;Echaniz, M.&#160;Lhardit, M.&#160;Naillet, Mme&#160;Rossi, Mme&#160;Jourdan, M.&#160;Barusseau, M.&#160;Courbon, M.&#160;Dufau et les membres du groupe Socialistes et apparent&#233;s"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 7", "articleDesignationCourte": "ART. 7", "articleDesignation": "ARTICLE 7", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0856.asp#D_Article_7"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "6", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;6, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x2013;&nbsp;&#x00E0; la seconde phrase, apr&#x00E8;s le mot&nbsp;: &#x00AB;&nbsp;environnemental&nbsp;&#x00BB;, sont ins&#x00E9;r&#x00E9;s les mots&nbsp;: &#x00AB;&nbsp;r&#x00E9;alis&#x00E9;e conjointement par l&#x2019;Agence nationale de s&#x00E9;curit&#x00E9; sanitaire de l&#x2019;alimentation, de l&#x2019;environnement et du travail mentionn&#x00E9; &#x00E0; l&#x2019;article L1313&#x2011;1 du code de la sant&#x00E9; publique et l&#x2019;Institut national de recherche pour l&#x2019;agriculture, l&#x2019;alimentation et l&#x2019;environnement&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Socialistes et apparent&#x00E9;s vise &#x00E0;&nbsp;confier l&#x2019;analyse du risque phytosanitaire et environnemental &#x00E0; l&#x2019;ANSES et &#x00E0; l&#x2019;INRAE.&nbsp;</p><p style=\"text-align: justify;\">Leur expertise permettrait d&#x2019;assurer une &#x00E9;valuation ind&#x00E9;pendante et rigoureuse des risques associ&#x00E9;s &#x00E0; l&#x2019;introduction de macro-organismes non indig&#x00E8;nes, garantissant ainsi une meilleure protection des &#x00E9;cosyst&#x00E8;mes et des productions agricoles. L&#x2019;int&#x00E9;gration de ces organismes sp&#x00E9;cialis&#x00E9;s permettrait &#x00E9;galement de renforcer la transparence scientifique et d&#x2019;apporter un avis factuel et fiable sur les implications environnementales et &#x00E9;cologiques.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-05-16T12:56:59+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0856P0D1N000769?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
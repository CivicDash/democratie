e": [{"libelle": "Conseil d'\u00c9tat et autres juridictions administratives", "programmeRef": "671163", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "-7008130", "creditPaiementAnnule": "-2300000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conseil \u00e9conomique, social et environnemental", "programmeRef": "671165", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Cour des comptes et autres juridictions financi\u00e8res", "programmeRef": "671167", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "totalAE": {"supplementaire": "0", "annule": "-7008130", "solde": "7008130"}, "totalCP": {"supplementaire": "0", "annule": "-2300000", "solde": "2300000"}, "cpEgalAe": "false"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Par cet amendement, le groupe LFI vise &#x00E0; revenir sur l&#x2019;annulation de cr&#x00E9;dit sur la mission &#x00AB;&nbsp;Conseil et contr&#x00F4;le de l&#x2019;&#x00C9;tat&nbsp;&#x00BB;. Cette diminution de cr&#x00E9;dit dans cette mission constitue une nouvelle attaque &#x00E0; l&#x2019;encontre de notre justice et de l&#x2019;&#x00C9;tat de droit dans notre pays.</p><p style=\"text-align: justify;\">Nos tribunaux administratifs, largement sous dot&#x00E9;s, ne parviennent plus &#x00E0; r&#x00E9;pondre aux exigences d&#x2019;une justice rendue dans des d&#x00E9;lais raisonnables, qui est pourtant un principe constitutif du droit &#x00E0; un proc&#x00E8;s &#x00E9;quitable, garanti dans la Convention europ&#x00E9;enne des droits de l&#x2019;Homme et dans notre bloc de constitutionnalit&#x00E9;. &#x00C0; titre indicatif, le d&#x00E9;lai moyen constat&#x00E9; dans les tribunaux administratifs hors proc&#x00E9;dures d&#x2019;urgence est d&#x2019;environ un an et demi pour juger les affaires. On ne peut pas se contenter de ces chiffres&nbsp;: pourtant, le Gouvernement accentue l&#x2019;aust&#x00E9;rit&#x00E9; et fait payer &#x00E0; notre justice l&#x2019;&#x00E9;chec de sa politique &#x00E9;conomique&nbsp;!</p><p style=\"text-align: justify;\">Dans le d&#x00E9;tail, nous demandons la suppression des annulations de cr&#x00E9;dits de&nbsp;:</p><p style=\"text-align: justify;\">&#x2013;&nbsp;7&nbsp;008&nbsp;130&nbsp;&#x20AC; en AE et 2&nbsp;300&nbsp;000&nbsp;&#x20AC; en CP pr&#x00E9;vue pour le programme Conseil d&#x2019;&#x00C9;tat et autres juridictions administratives.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-11-12", "datePublication": "2025-11-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-11-13T14:57:26+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B2068P0D1N000033?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
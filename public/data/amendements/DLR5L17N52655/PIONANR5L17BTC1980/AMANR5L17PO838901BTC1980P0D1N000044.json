{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1980P0D1N000044", "chronotag": "1761069643539", "legislature": "17", "identification": {"numeroLong": "44", "numeroOrdreDepot": "44", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1980P0D1", "texteLegislatifRef": "PIONANR5L17BTC1980", "triAmendement": "paaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaao", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720846", "groupePolitiqueRef": "PO845413", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA795228", "PA793262", "PA794906", "PA794138", "PA795588", "PA795084", "PA842105", "PA841657", "PA720430", "PA793772", "PA795616", "PA793444", "PA794938", "PA795318", "PA841023", "PA796090", "PA793912", "PA842187", "PA842037", "PA791812", "PA793736", "PA721202", "PA795136", "PA793464", "PA842155", "PA795200", "PA795844", "PA841251", "PA794786", "PA794870", "PA796070", "PA795310", "PA794478", "PA841947", "PA794082"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Lachaud,  Mme&#160;Abomangoli, M.&#160;Alexandre, M.&#160;Amard, Mme&#160;Amiot, Mme&#160;Amrani, M.&#160;Arenas, M.&#160;Arnault, Mme&#160;Belouassa-Cherifi, M.&#160;Bernalicis, M.&#160;Bex, M.&#160;Bilongo, M.&#160;Bompard, M.&#160;Boumertit, M.&#160;Boyard, M.&#160;Cadalen, M.&#160;Caron, M.&#160;Carri&#232;re, Mme&#160;Cathala, M.&#160;Cernon, Mme&#160;Chikirou, M.&#160;Clouet, M.&#160;Coquerel, M.&#160;Coulomme, M.&#160;Delogu, M.&#160;Diouara, Mme&#160;Dufour, Mme&#160;Erodi, Mme&#160;Feld, M.&#160;Fernandes, Mme&#160;Ferrer, M.&#160;Gaillard, Mme&#160;Guett&#233;, M.&#160;Guiraud, Mme&#160;Hamdane et Mme&#160;Hignet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "visant \u00e0 reporter le renouvellement g\u00e9n\u00e9ral des membres du congr\u00e8s et des assembl\u00e9es de province de la NouvelleCal\u00e9donie afin de permettre la poursuite de la discussion sur l\u2019accord du 12 juillet 2025 et sa mise en \u0153uvre", "articleDesignationCourte": "TITRE", "articleDesignation": "TITRE", "type": "TITRE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1980.asp#D_visant_a_reporter_le_renouvellement_ge"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter le titre par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;malgr&#x00E9; le refus du FLNKS&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Il est faux et abusif de parler d&#x2019;un accord, puisque le Front de Lib&#x00E9;ration Nationale Kanak et Socialiste s&#x2019;est clairement prononc&#x00E9; contre le projet d&#x2019;accord. En tout &#x00E9;tat de cause, aucun &#x00AB; accord &#x00BB; n&#x2019;a &#x00E9;t&#x00E9; sign&#x00E9; le 12 juillet 2025. Le document sign&#x00E9; est un engagement &#x00E0; revenir devant les bases des d&#x00E9;l&#x00E9;gataires, et &#x00E0; pr&#x00E9;senter et d&#x00E9;fendre l&#x2019;&#x00E9;tat des discussions du texte, et en aucun cas un accord.</p><p style=\"text-align: justify;\">Puisque le FLNKS a rejet&#x00E9; le projet d&#x2019;accord le 9 ao&#x00FB;t 2025, et officiellement retir&#x00E9; leurs signatures du projet le 1er octobre, il est n&#x00E9;cessaire pour la clart&#x00E9; des d&#x00E9;bats de pr&#x00E9;ciser que ce report se fait malgr&#x00E9; le refus d&#x2019;un des acteurs historiques et incontournable qu&#x2019;est le FLNKS.</p><p style=\"text-align: justify;\">Les auteurs d&#x00E9;posent donc un amendement de clarification.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-21", "datePublication": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1980P0D1N000044?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
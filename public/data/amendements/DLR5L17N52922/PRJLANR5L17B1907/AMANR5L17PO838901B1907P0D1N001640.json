{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B1907P0D1N001640", "chronotag": "1762557557544", "legislature": "17", "identification": {"numeroLong": "1640", "numeroOrdreDepot": "1640", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B1907P0D1", "texteLegislatifRef": "PRJLANR5L17B1907", "triAmendement": "aiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaj", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793354", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe Rassemblement National", "libelle": "Mme&#160;M&#233;lin et les membres du groupe Rassemblement National"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1907.asp#D_Article_3"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La modernisation de notre syst&#x00E8;me de sant&#x00E9; est la clef d&#x2019;un mod&#x00E8;le social plus efficient. R&#x00E9;duire de 60 M&#x20AC; les cr&#x00E9;dits allou&#x00E9;s &#x00E0; cet investissement dans l&#x2019;avenir serait contre-productif.</p><p style=\"text-align: justify;\">Investir dans l&#x2019;innovation, l&#x2019;adaptation des infrastructures et l&#x2019;am&#x00E9;lioration de la qualit&#x00E9; des soins n&#x2019;est pas une d&#x00E9;pense de circonstance mais un choix strat&#x00E9;gique pour r&#x00E9;pondre aux d&#x00E9;fis d&#x00E9;mographiques, technologiques et sanitaires &#x00E0; venir. Cet amendement propose donc la suppression de cet article, afin de prot&#x00E9;ger ce levier indispensable pour garantir la p&#x00E9;rennit&#x00E9; et l&#x2019;efficacit&#x00E9; de notre mod&#x00E8;le social.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-31", "datePublication": "2025-11-04", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-11-04T23:39:08+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B1907P0D1N001640?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2026IDS29863", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "83691", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
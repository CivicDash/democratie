{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B1906P1D1N000404", "chronotag": "1761175303294", "legislature": "17", "identification": {"numeroLong": "I-CF404", "numeroOrdreDepot": "404", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B1906P1D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "osaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795778", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe Rassemblement National", "libelle": "M.&#160;Jean-Philippe&#160;Tanguy et les membres du groupe Rassemblement National"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ARTICLE 45", "articleDesignationCourte": "ART. 45", "articleDesignation": "ARTICLE 45", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906A.asp#D_ARTICLE_45"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;la fin de l&#x2019;alin&#x00E9;a, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&nbsp;&#x00AB;&nbsp;28&#x202F;781&#x202F;025&#x202F;011&nbsp;&#x20AC;&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">&nbsp;le montant&nbsp;:</p><p style=\"text-align: justify;\">&nbsp;&#x00AB;&nbsp;27&nbsp;281&#x202F;025&#x202F;011&nbsp;&#x20AC;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Amendement d&#x2019;appel.&nbsp;</p><p style=\"text-align: justify;\">Depuis 2021, la France verse &#x00E0; l&#x2019;Union europ&#x00E9;enne une contribution dite &#x00AB;&nbsp;ressource propre plastique&nbsp;&#x00BB;, calcul&#x00E9;e sur le volume d&#x2019;emballages plastiques non recycl&#x00E9;s.</p><p style=\"text-align: justify;\">Cette taxe repr&#x00E9;sente pr&#x00E8;s de 1,5 milliard d&#x2019;euros en 2024 pour notre pays &#x2014; une somme consid&#x00E9;rable pr&#x00E9;lev&#x00E9;e sur le&nbsp;dos des Fran&#x00E7;ais, alors m&#x00EA;me qu&#x2019;aucune taxe &#x00E9;quivalente n&#x2019;a &#x00E9;t&#x00E9; instaur&#x00E9;e au niveau fran&#x00E7;ais pour en compenser la charge.</p><p style=\"text-align: justify;\">Autrement dit, la France paie &#x00E0; l&#x2019;Union europ&#x00E9;enne pour son plastique non recycl&#x00E9;, sans que les producteurs ou consommateurs fran&#x00E7;ais n&#x2019;en voient le moindre retour concret.</p><p style=\"text-align: justify;\">Il est donc urgent de r&#x00E9;cup&#x00E9;rer cette contribution. Tel est le sens de cet amendement.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-17", "datePublication": "2025-10-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-23T01:21:28+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B1906P1D1N000404?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "32709", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
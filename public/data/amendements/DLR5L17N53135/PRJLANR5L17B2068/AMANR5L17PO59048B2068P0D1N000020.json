[{"libelle": "Aide \u00e9conomique et financi\u00e8re au d\u00e9veloppement", "programmeRef": "671041", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "-105724053", "creditPaiementAnnule": "-19098557", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Renforcement des fonds propres de l'Agence fran\u00e7aise de d\u00e9veloppement", "programmeRef": "671043", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Solidarit\u00e9 \u00e0 l'\u00e9gard des pays en d\u00e9veloppement", "programmeRef": "671045", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "-63760074", "creditPaiementAnnule": "-10053826", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Restitution des \u00ab biens mal acquis \u00bb", "programmeRef": "671047", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Fonds de solidarit\u00e9 pour le d\u00e9veloppement", "programmeRef": "671049", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "totalAE": {"supplementaire": "0", "annule": "-169484127", "solde": "169484127"}, "totalCP": {"supplementaire": "0", "annule": "-29152383", "solde": "29152383"}, "cpEgalAe": "false"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Par cet amendement, le groupe LFI demande la suppression des annulations de cr&#x00E9;dits de la mission.</p><p style=\"text-align: justify;\">Alors que la COP 30 s&#x2019;ouvre au Br&#x00E9;sil, le besoin des pays en d&#x00E9;veloppement n&#x2019;a jamais &#x00E9;t&#x00E9; aussi important. Plus de 300 milliards par an ont d&#x00E9;j&#x00E0; &#x00E9;t&#x00E9; promis par les pays d&#x00E9;velopp&#x00E9;s l&#x2019;ann&#x00E9;e derni&#x00E8;re. La France a un r&#x00F4;le central &#x00E0; jouer dans cet objectif&nbsp;: elle faisait partie des cinq principaux donateurs en 2023. Cet effort doit &#x00EA;tre poursuivi dans un contexte de bifurcation &#x00E9;cologique et de r&#x00E9;duction de l&#x2019;extr&#x00EA;me pauvret&#x00E9; dans le monde.&nbsp;</p><p style=\"text-align: justify;\">Apr&#x00E8;s des coupes massives en LFI l&#x2019;an dernier, ces nouvelles coupes en catimini sont insupportables.</p><p style=\"text-align: justify;\">Dans le d&#x00E9;tail, nous demandons la suppression des annulations de cr&#x00E9;dits de&nbsp;:</p><p style=\"text-align: justify;\">&#x2013;&nbsp;105&nbsp;724&nbsp;053&nbsp;&#x20AC; en AE et 19&nbsp;098&nbsp;557&nbsp;&#x20AC; en CP pr&#x00E9;vue pour le programme Aide &#x00E9;conomique et financi&#x00E8;re au d&#x00E9;veloppement.</p><p style=\"text-align: justify;\">&#x2013;&nbsp;63&nbsp;760&nbsp;074&nbsp;&#x20AC; en AE et 10&nbsp;053&nbsp;826&nbsp;&#x20AC; en CP pr&#x00E9;vue pour le programme Solidarit&#x00E9; &#x00E0; l&#x2019;&#x00E9;gard des pays en d&#x00E9;veloppement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-11-12", "datePublication": "2025-11-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-11-13T14:52:24+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B2068P0D1N000020?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "34857", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
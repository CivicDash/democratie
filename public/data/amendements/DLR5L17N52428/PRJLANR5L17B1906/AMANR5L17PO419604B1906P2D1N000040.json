ositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "France T\u00e9l\u00e9visions", "programmeRef": "585181", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "ARTE France", "programmeRef": "585183", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Radio France", "programmeRef": "585185", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "France M\u00e9dias Monde", "programmeRef": "585187", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Institut national de l'audiovisuel", "programmeRef": "585189", "autorisationEngagement": "-29999999", "creditPaiement": "-29999999", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "TV5 Monde", "programmeRef": "585191", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Programme de transformation", "programmeRef": "585193", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Augmentation inconditionnelle des cr\u00e9dits allou\u00e9s aux soci\u00e9t\u00e9s de l'audiovisuel public", "programmeRef": "585195", "autorisationEngagement": "29999999", "creditPaiement": "29999999", "action": "cr\u00e9
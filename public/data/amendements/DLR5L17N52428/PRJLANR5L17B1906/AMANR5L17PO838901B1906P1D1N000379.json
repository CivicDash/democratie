{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B1906P1D1N000379", "chronotag": "1762206433541", "legislature": "17", "identification": {"numeroLong": "I-379", "numeroOrdreDepot": "379", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B1906P1D1", "texteLegislatifRef": "PRJLANR5L17B1906", "triAmendement": "awaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabdk", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA840979", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642725", "PA608826", "PA794178", "PA266808", "PA718884", "PA718784", "PA718838", "PA718850", "PA719968", "PA330008"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Le&#160;Fur,  M.&#160;Li&#233;geon, Mme&#160;Duby-Muller, M.&#160;Taite, M.&#160;Rolland, Mme&#160;Bazin-Malgras, M.&#160;Pauget, M.&#160;Fabrice&#160;Brun, M.&#160;Cordier, Mme&#160;Sylvie&#160;Bonnet et M.&#160;Breton"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ARTICLE 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1906A.asp#D_ARTICLE_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "13", "alineaDesignation": "Alin\u00e9a 13"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>Supprimer l&#x2019;alin&#x00E9;a&nbsp;13.</p><p>&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\"><b>Le pr&#x00E9;sent amendement vise &#x00E0; supprimer la disposition supprimant la r&#x00E9;duction d&#x2019;imp&#x00F4;t pour frais de scolarit&#x00E9; dans l&#x2019;enseignement secondaire et sup&#x00E9;rieur.</b></p><p style=\"text-align: justify;\">Cette r&#x00E9;duction d&#x2019;imp&#x00F4;t, d&#x2019;un montant de 61&nbsp;euros pour un coll&#x00E9;gien, 153&nbsp;euros pour un lyc&#x00E9;en et 183&nbsp;euros pour un &#x00E9;tudiant, concerne plusieurs millions de foyers fiscaux. Elle constitue un instrument de redistribution horizontale, reconnaissant l&#x2019;effort consenti par les familles pour la scolarit&#x00E9; de leurs enfants.</p><p style=\"text-align: justify;\">Sa suppression affaiblirait notre politique familiale et enverrait un signal n&#x00E9;gatif aux m&#x00E9;nages, alors que le co&#x00FB;t de la vie &#x00E9;tudiante et de la scolarit&#x00E9; continue d&#x2019;augmenter.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-20", "datePublication": "2025-10-25", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-11-03T22:46:42+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B1906P1D1N000379?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2026IDS29861", "article99": "false", "loiReference": {"codeLoi": "0", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "32881", "typePosition": "Dernier"}, "discussionIdentique": {"idDiscussion": "81777", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
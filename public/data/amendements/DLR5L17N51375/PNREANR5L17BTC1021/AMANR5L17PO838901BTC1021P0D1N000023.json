{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1021P0D1N000023", "chronotag": "1757947619266", "legislature": "17", "identification": {"numeroLong": "23", "numeroOrdreDepot": "23", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1021P0D1", "texteLegislatifRef": "PNREANR5L17BTC1021", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaak", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719032", "groupePolitiqueRef": "PO845470", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642868", "PA794278", "PA793992", "PA795278", "PA332228", "PA687214", "PA841339", "PA841431", "PA267780", "PA794234", "PA794470", "PA722070", "PA719778", "PA719052", "PA719282", "PA842125", "PA794426", "PA794838", "PA720230", "PA794466", "PA720908", "PA793398", "PA721844", "PA793572", "PA719528", "PA795270", "PA720552", "PA721458", "PA720138", "PA722336", "PA795350", "PA795026"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bouyx,  M.&#160;Christophe, M.&#160;Albertini, M.&#160;Alfandari, Mme&#160;Bellamy, M.&#160;Benoit, M.&#160;Berrios, M.&#160;Brard, Mme&#160;Colin-Oesterl&#233;, Mme&#160;Firmin&#160;Le&#160;Bodo, M.&#160;Gernigon, Mme&#160;G&#233;rard, M.&#160;Henriet, M.&#160;Jolivet, M.&#160;Kervran, M.&#160;Lacombe, M.&#160;Lam, Mme&#160;Le&#160;H&#233;nanff, M.&#160;Lemaire, Mme&#160;Lise&#160;Magnier, M.&#160;Moulliere, Mme&#160;Moutchou, M.&#160;Patrier-Leitus, Mme&#160;Piron, M.&#160;Plassard, M.&#160;Portarrieu, Mme&#160;Poussier-Winsback, Mme&#160;Rauch, M.&#160;Roseren, Mme&#160;Saint-Paul, M.&#160;Thi&#233;baut, M.&#160;Valletoux et Mme&#160;Violland"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1021.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "30", "alineaDesignation": "Alin\u00e9a 30"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;30, supprimer le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et&nbsp;&#x00BB;.&nbsp;</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter&nbsp;le m&#x00EA;me alin&#x00E9;a&nbsp;30 par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et demande l&#x2019;envoi d&#x2019;une mission internationale ind&#x00E9;pendante, sous l&#x2019;&#x00E9;gide du Haut-Commissariat des Nations unies aux droits de l&#x2019;homme ou en coop&#x00E9;ration avec des m&#x00E9;canismes du Conseil de l&#x2019;Europe sp&#x00E9;cialis&#x00E9;s dans la pr&#x00E9;vention de la torture, afin d&#x2019;&#x00E9;valuer&nbsp;les conditions de d&#x00E9;tention de M. Boualem Sansal et d&#x2019;autres prisonniers d&#x2019;opinion en Alg&#x00E9;rie.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; renforcer la pression internationale en appelant &#x00E0; une action concr&#x00E8;te, visible et ind&#x00E9;pendante de la part d&#x2019;organismes internationaux reconnus. En sollicitant une mission sous l&#x2019;&#x00E9;gide du Haut-Commissariat des Nations unies aux droits de l&#x2019;homme ou en coop&#x00E9;ration avec des m&#x00E9;canismes du Conseil de l&#x2019;Europe, le texte place le sort de Boualem Sansal dans une dimension internationale et multilat&#x00E9;rale, d&#x00E9;passant le cadre bilat&#x00E9;ral France-Alg&#x00E9;rie. Il s&#x2019;agit d&#x2019;un levier diplomatique fort, qui expose publiquement la situation et contraint les autorit&#x00E9;s alg&#x00E9;riennes &#x00E0; rendre des comptes sur les conditions de d&#x00E9;tention et le respect des droits fondamentaux.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-02", "datePublication": "2025-05-06", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-05-06T17:11:31+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1021P0D1N000023?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO866672B1112P0D1N000241", "chronotag": "1757947727500", "legislature": "17", "identification": {"numeroLong": "CS241", "numeroOrdreDepot": "241", "prefixeOrganeExamen": "CSCYBER", "numeroRect": "0"}, "examenRef": "EXANR5L17PO866672B1112P0D1", "texteLegislatifRef": "PRJLANR5L17B1112", "triAmendement": "oaiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaas", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795808", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA588884", "PA610968", "PA793780", "PA841701", "PA795362", "PA795454", "PA840997", "PA841885", "PA841507", "PA794008", "PA721210", "PA793452", "PA841351", "PA793980", "PA795010", "PA841419", "PA842013", "PA840947", "PA794022", "PA794146", "PA841223", "PA795636", "PA842045", "PA793708", "PA793664", "PA794154", "PA794778", "PA795076", "PA722142", "PA610002", "PA796018", "PA736201", "PA841713", "PA793948", "PA2940"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Sebaihi,  Mme&#160;Autain, M.&#160;Amirshahi, Mme&#160;Arrighi, Mme&#160;Balage&#160;El&#160;Mariky, Mme&#160;Belluco, M.&#160;Ben&#160;Cheikh, M.&#160;Biteau, M.&#160;Arnaud&#160;Bonnet, M.&#160;Nicolas&#160;Bonnet, Mme&#160;Chatelain, M.&#160;Corbi&#232;re, M.&#160;Davi, M.&#160;Duplessy, M.&#160;Fournier, Mme&#160;Garin, M.&#160;Damien&#160;Girard, M.&#160;Gustave, Mme&#160;Catherine&#160;Hervieu, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lahais, M.&#160;Lucas-Lundy, Mme&#160;Ozenne, M.&#160;Peytavie, Mme&#160;Pochon, M.&#160;Raux, Mme&#160;Regol, Mme&#160;Sandrine&#160;Rousseau, M.&#160;Ruffin, Mme&#160;Sas, Mme&#160;Simonnet, Mme&#160;Taill&#233;-Polian, M.&#160;Tavernier, M.&#160;Thierry et Mme&#160;Voynet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 58 bis", "articleDesignationCourte": "ART. 58 BIS", "articleDesignation": "ARTICLE 58 BIS", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1112.asp#D_Article_58_bis"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "1", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter cet article par l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;II.&nbsp;&#x2013;&nbsp;Le Gouvernement remet au Parlement&nbsp;dans un d&#x00E9;lai de six mois &#x00E0; compter de l&#x2019;entr&#x00E9;e en vigueur du pr&#x00E9;sent un rapport, r&#x00E9;alis&#x00E9; sur la base de travaux men&#x00E9;s par le Conseil d&#x2019;analyse &#x00E9;conomique, qui &#x00E9;value les impacts et l&#x2019;efficacit&#x00E9; &#x00E9;conomiques et sociaux de la modification de l&#x2019;article&nbsp;L.&nbsp;121&#x2011;8 du code des assurances sur les garanties concernant les risques li&#x00E9;s aux cyberattaques.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;58&nbsp;<i>bis</i> a &#x00E9;t&#x00E9; introduit par amendement au S&#x00E9;nat du gro&#x00E0;upe centriste (<a href=\"https://www.senat.fr/amendements/2024-2025/394/Amdt_14.html\">amendement n&#x00B0; 14</a>), face &#x00E0; un avis d&#x00E9;favorable du Gouvernement. Au stade de la commission sp&#x00E9;ciale, nous souhaitons pouvoir &#x00E9;lever le d&#x00E9;bat et appeler &#x00E0; ce qu&#x2019;une r&#x00E9;elle &#x00E9;tude des effets de la modification du droit des assurances soit approfondie. L&#x2019;enjeu est bien d&#x2019;avoir un cadre juridique permettant une garantie assurantielle efficace des acteurs, notamment les petits (communes, PME/TPE, association) et nous sommes en l&#x2019;&#x00E9;tat d&#x00E9;pourvu de projection pr&#x00E9;sentant les avantages et inconv&#x00E9;nients de l&#x2019;&#x00E9;volution du droit vot&#x00E9;e au S&#x00E9;nat.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-09-05", "datePublication": "2025-09-10", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-09-10T15:56:32+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO866672B1112P0D1N000241?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
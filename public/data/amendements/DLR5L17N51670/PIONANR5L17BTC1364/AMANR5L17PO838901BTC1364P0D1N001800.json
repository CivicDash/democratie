contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;7,&nbsp;substituer au mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ou&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>&nbsp;<br>Ce texte&nbsp;introduit une notion qui ne permet pas au m&#x00E9;decin de d&#x00E9;terminer le stade &#x00E0; partir duquel le patient va pouvoir formuler une demande d&#x2019;aide &#x00E0; mourir, et rend difficile l&#x2019;appr&#x00E9;ciation de ce crit&#x00E8;re d&#x2019;&#x00E9;ligibilit&#x00E9; ainsi &#x00E9;largi. &#x00C0; cet effet, il est propos&#x00E9; de retenir la notion de phase avanc&#x00E9;e et terminale.<br>Cet amendement a &#x00E9;t&#x00E9; travaill&#x00E9; avec l&#x2019;Ordre national des m&#x00E9;decins.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-09", "datePublication": "2025-05-19", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-05-19T16:19:50+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1364P0D1N001800?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instan
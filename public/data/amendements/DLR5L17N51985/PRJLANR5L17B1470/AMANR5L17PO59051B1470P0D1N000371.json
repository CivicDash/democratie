{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B1470P0D1N000371", "chronotag": "1757949778539", "legislature": "17", "identification": {"numeroLong": "CL371", "numeroOrdreDepot": "371", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B1470P0D1", "texteLegislatifRef": "PRJLANR5L17B1470", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaapo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA266797", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Gosselin"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "RAPPORT ANNEX\u00c9", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE PREMIER", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1470.asp#D_RAPPORT_ANNEXE"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "36", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 36"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;36, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x2013;&nbsp;la cr&#x00E9;ation d&#x2019;une base de la Marine en eau profonde permettant d&#x2019;y affecter des b&#x00E2;timents hauturiers, notamment un patrouilleur outre-mer de nouvelle g&#x00E9;n&#x00E9;ration, en vue de renforcer le contr&#x00F4;le et la ma&#x00EE;trise des espaces maritimes&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Situ&#x00E9;e &#x00E0; l&#x2019;entr&#x00E9;e Nord du strat&#x00E9;gique canal du Mozambique et soumise &#x00E0; une pression migratoire et aux trafics d&#x2019;&#x00EA;tres humains, il est indispensable de renforcer les moyens de souverainet&#x00E9; maritime &#x00E0; Mayotte par la substitution de la base de la Marine&nbsp;actuelle, qui ne peut recevoir que des patrouilleurs c&#x00F4;tiers de petites dimensions, par une base en eau profonde permettant de hausser, in situ, les capacit&#x00E9;s de la Marine au centre d&#x2019;une zone strat&#x00E9;gique o&#x00F9; les trafics prolif&#x00E8;res et o&#x00F9; la souverainet&#x00E9; fran&#x00E7;aise sur le 5<sup>&#x00E8;me</sup>&nbsp;district des TAAF est discut&#x00E9;e.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-05", "datePublication": "2025-06-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-06-11T11:12:48+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSch
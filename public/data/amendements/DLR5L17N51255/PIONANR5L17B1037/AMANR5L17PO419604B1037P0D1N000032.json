{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B1037P0D1N000032", "chronotag": "1761124963295", "legislature": "17", "identification": {"numeroLong": "AC32", "numeroOrdreDepot": "32", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B1037P0D1", "texteLegislatifRef": "PIONANR5L17B1037", "triAmendement": "ciaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841553", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Tesson, rapporteur"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3 bis", "articleDesignationCourte": "ART. 3 BIS", "articleDesignation": "ARTICLE 3 BIS", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1037.asp#D_Article_3_bis"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Au premier alin&#x00E9;a, substituer &#x00E0; la r&#x00E9;f&#x00E9;rence&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L.&nbsp;111&#x2011;3&#x2011;1&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">la r&#x00E9;f&#x00E9;rence&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L.&nbsp;911&#x2011;4&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, au m&#x00EA;me alin&#x00E9;a, substituer &#x00E0; la r&#x00E9;f&#x00E9;rence&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L.&nbsp;111&#x2011;3&#x2011;2&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">la r&#x00E9;f&#x00E9;rence&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L.&nbsp;911&#x2011;4&#x2011;1&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">III.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;2, substituer &#x00E0; la r&#x00E9;f&#x00E9;rence:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L.&nbsp;111&#x2011;3&#x2011;2&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">la r&#x00E9;f&#x00E9;rence&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L.&nbsp;911&#x2011;4&#x2011;1&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;interdiction de la transmission des coordonn&#x00E9;es du personnel est, en l&#x2019;&#x00E9;tat actuel du texte, plac&#x00E9;e au sein livre premier de la premi&#x00E8;re partie du code de l&#x2019;&#x00E9;ducation, consacr&#x00E9; aux principes g&#x00E9;n&#x00E9;raux de l&#x2019;&#x00E9;ducation.</p><p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; d&#x00E9;placer cet article au sein du livre IX de la quatri&#x00E8;me partie, consacr&#x00E9; aux personnels de l&#x2019;&#x00E9;ducation.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-10-21", "datePublication": "2025-10-21", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-10-22T11:22:18+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B1037P0D1N000032?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}
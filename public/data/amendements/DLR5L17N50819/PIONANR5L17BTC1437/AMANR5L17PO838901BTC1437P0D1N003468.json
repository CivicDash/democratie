urFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6 ter", "articleDesignationCourte": "ART. 6 TER", "articleDesignation": "ARTICLE 6 TER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1437.asp#D_Article_6_ter"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p>Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; supprimer cet ajout, qui impose une contrainte excessive en limitant la libert&#x00E9; de communication des autorit&#x00E9;s de l&#x2019;&#x00C9;tat. Si la protection des agents est essentielle, cette disposition risque de cr&#x00E9;er des tensions inutiles et de restreindre le dialogue entre les acteurs locaux et les services de l'&#x00C9;tat. Il est important de maintenir une relation de confiance et de transparence sans imposer des obligations disproportionn&#x00E9;es.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-22", "datePublication": "2025-05-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-ins